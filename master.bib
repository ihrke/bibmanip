@article{Hoefer1999,
  author = {H{\"o}fer, Ilse and Della Casa, Vera and Feldon,
                  Joram},
  year = {1999},
  title = {The interaction between schizotypy and latent
                  inhibition: Modulation by experimental parameters},
  journal = {Personality \& Individual Differences},
  volume = {26},
  number = {6},
  pages = {1075-1088},
  abstract = {Two recent experiments by the present authors (in
                  press) yielded the typical reduced latent inhibition
                  (LI) in high vs low schizotypy Ss after a slow,
                  irregularly presented masking task (Stroop task),
                  and reduced LI in low vs high schizotypy Ss after a
                  fast, regularly presented masking task. The present
                  experiment, using 101 Ss (aged 22-44 yrs), examined
                  whether speed or regularity was responsible for the
                  different results. The present slow, regular
                  experiment replicated the results of the former
                  slow, irregular experiment, i.e. reduced LI in high
                  schizotypals and significant LI in low schizotypals,
                  indicating that speed and not regularity is the
                  critical experimental variable.  The modulation of
                  the schizotypy-LI relationship can be attributed to
                  differences in attentional resources or the time
                  available to process the to-be-target stimuli during
                  preexposure, in accordance with the hypothesis of
                  R. E. Lubow and J. C. Gewirtz (1995) that automatic
                  processing of these stimuli is critical for the
                  development of LI in adult humans. In addition,
                  results on Stroop and negative priming effects are
                  presented.},
  keywords = {modulation in experimental variables speed vs
                  regularity in masking task presentation, latent
                  inhibition, 22-44 yr olds with high vs low
                  schizotypal traits Visual Stimulation},
  tags = {psychology;np},
}
@article{Hoenig2002,
  author = {Hoenig, Klaus and Hochrein, A. and M{\"u}ller,
                  D. J. and Wagner, M.},
  year = {2002},
  title = {Different negative priming impairments in
                  schizophrenia and subgroups of obsessive-compulsive
                  disorder},
  journal = {Psychological Medicine},
  volume = {32},
  number = {3},
  pages = {459-468},
  abstract = {In order to investigate inhibitory functioning,
                  priming tasks are commonly used. This study isolated
                  specific distractor features for
                  examination. Negative priming (NP) for stimulus
                  identity and location was examined in 16 Ss with
                  schizophrenia (7 females, aged 20-58 yrs), 15 Ss
                  with obsessive-compulsive disorder (OCD) (6 females,
                  aged 20-50 yrs) and 16 controls (8 females, aged
                  21-56 yrs). All groups performed a task in which
                  they were instructed successively to select the
                  larger 1 of 2 cardinal numbers. The deeper
                  processing of both stimuli was expected to yield
                  large NP effects that allow the detection of subtle
                  group differences. Large NP effects were found for
                  stimulus identity in all 3 groups. Schizophrenic
                  patients differed from normal controls with respect
                  to the amount of incidental location
                  priming. Subgroup analyses of OCD patients showed NP
                  impairments for checkers when the response-stimulus
                  interval (RSI) was short, but large identity NP when
                  the RSI was long. OCD non-checkers showed normal NP
                  patterns with short RSI, but reduced identity NP
                  with longer RSI. Schizophrenic patients do not show
                  the ability to use spatial selective attention in
                  order to guide their actions. Information processing
                  was differentially impaired in OCD subgroups
                  dependent on RSI variation.},
  keywords = {negative priming schizophrenia obsessive-compulsive
                  disorder inhibitory functioning distractor features
                  stimulus identity \& location information processing
                  Print Electronic},
  tags = {psychology;np},
}
@article{Hoffmann2003,
  author = {Hoffmann, Joachim and Kiesel, Andrea and Sebald,
                  Albrecht},
  year = {2003},
  title = {Task switches under Go/NoGo conditions and the
                  decomposition of switch costs},
  journal = {European Journal of Cognitive Psychology},
  volume = {15},
  number = {1},
  pages = {101-128},
  abstract = {Four experiments, each employing 12 subjects (aged
                  19-47 yrs), explored whether stimulus-driven
                  components of switch costs can be properly
                  disentangled from intentionally driven components by
                  combining a task switch paradigm with a Go/NoGo
                  paradigm. It was argued that non-switches after Go
                  trials should selectively profit from
                  stimulus-driven repetition benefits, whereas
                  switches after NoGo trials should selectively suffer
                  from stimulus-driven negative priming. Finally,
                  intentional components of switch costs should be
                  assessable by reaction time differences between
                  switches after Go trials, which do not suffer from
                  negative priming, and non-switches after NoGo
                  trials, which do not profit from repetitions. Exp 1
                  indicates that with short preparation time
                  repetition benefits, negative priming costs, and
                  intentional components contribute approximately
                  additively to switch costs. Exp 2 confirms that the
                  delay of switches after NoGo trials is indeed due to
                  negative priming. Exps 3 and 4 show that repetition
                  benefits and intentional components of switch costs
                  are properly assessed only if the settings assure
                  that participants reconfigure the required task set
                  in NoGo as well as Go trials.},
  keywords = {task switching switch costs priming reaction time
                  repetition cognitive processes Go/NoGo paradigm
                  Print Electronic},
  tags = {psychology;np},
}
