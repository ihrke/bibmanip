@article{Wirth-Beaumont2004,
  author = {Wirth-Beaumont, Erin T.},
  year = {2004},
  title = {Adaptation to life events: The ubiquitous role of
                  perception},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {65},
  number = {2},
  pages = {1076},
  abstract = {Recently, much research has focused on coping
                  efforts, typically emphasizing an effortful,
                  deliberate process engineered to help one recover
                  from life stressors. However, some coping, it seems,
                  occurs below the level of conscious awareness. Three
                  studies investigated the effects on unconscious
                  primes on mood, health, coping efficacy, and well
                  being. The first study investigated the priming
                  efficacy of six versions of the Scrambled Sentence
                  Task (Chartrand, 1998). Results indicated that three
                  conditions emerged, positive (success + health),
                  negative (failure + illness), and control
                  (affectively neutral). The second study examined how
                  the priming manipulation affected perceptions of
                  mood, health, coping efficacy, and well being after
                  an in-lab stressor, the delivery of a self-relevant
                  speech. Counter to hypotheses, a reversal of effects
                  obtained, such that participants' perception of
                  their own coping and well being decreased after
                  being exposed to the positive primes. After the
                  negative priming manipulation, perceptions improved
                  on these measures. The third study tested
                  alternative explanations for the unexpected findings
                  of the previous investigation.  Results suggested
                  that the reversal of effects was due to an
                  alteration of participants' relative standard of
                  comparison, rather than self-perceptions.  That is,
                  after the positive priming manipulation, the
                  "average other" seemed significantly better off,
                  making the self feel less good.  Results of these
                  studies are discussed in terms of implications for
                  the current methodologies used in the coping
                  literature and the role of the cognitive unconscious
                  in perceptions of global wellness status.},
  keywords = {coping effort life events social adaptation Print},
  tags = {psychology;np},
}
