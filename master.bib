@article{Gordon1996Whats,
  author = {Gordon, R. D. and Irwin, D. E.},
  year = {1996},
  title = {What's in an object file? Evidence from priming
                  studies.},
  journal = {PP},
  volume = {58},
  number = {8},
  pages = {1260--1277},
  month = {nov},
  url = {http://view.ncbi.nlm.nih.gov/pubmed/8961836},
  abstract = {Six experiments were conducted to investigate the
                  nature of the contents of object files, temporary
                  representations that store information about
                  objects. Experiment 1 used a lexical priming
                  paradigm with a lexical decision task, in which the
                  prime and target could appear in either the same or
                  different locations. The results indicated a greater
                  priming effect when the prime and target appeared in
                  the same location than when they appeared in
                  different locations (object-or location-specific
                  priming). Experiment 2 replicated these findings for
                  objects that changed position during the
                  display. Experiment 3 demonstrated that these
                  findings reflected the inclusion of abstract
                  identity information, rather than physical form, in
                  object files. Three additional experiments tested
                  for the presence of three types of semantic
                  information (related concepts, semantic features,
                  and category membership) in object files. No
                  object-specific priming effects were found. Taken
                  together, these experiments suggest that an object
                  file includes identity information, but not semantic
                  information. Implications of the results for object
                  file theory are discussed.},
  issn = {0031-5117},
  tags = {binding;np;psychology},
  address = {Department of Psychology, University of Illinois,
                  Champaign, USA. rgordon@s.psych.uiuc.edu},
}
@article{Graham1998,
  author = {Graham, Steven and McLaren, I. P. L.},
  year = {1998},
  title = {Retardation in human discrimination learning as a
                  consequence of pre-exposure: Latent inhibition or
                  negative priming?},
  journal = {Quarterly Journal of Experimental Psychology},
  volume = {51},
  number = {2},
  pages = {155-172},
  abstract = {A series of experiments with human Ss, using
                  black-and-white chequerboard patterns, demonstrated
                  that non-reinforced pre-exposure could impair
                  performance in a subsequent learning task. Ss' task
                  was to turn over a series of cards by mouse-clicking
                  on a pack of cards Iying face-down, and then to
                  classify these cards into 1 of 2 categories. In a
                  subsequent task, Ss were asked to discriminate
                  between pairs of chequerboards, some of which had
                  previously appeared in the initial categorization
                  phase: either directly ("fronts") or incidentally
                  ("backs") involved in categorization. In Exp 1, for
                  those stimuli used as the backs of the cards, there
                  was a significant impairment in performance relative
                  to non-pre-exposed control stimuli. Although the
                  impairment appeared to be specific to the stimuli
                  pre-exposed, when the pre-exposed "backs" were
                  minimally distorted in the discrimination task of
                  Exp 2, performance was still significantly impaired
                  relative to non-pre-exposed control stimuli. The
                  results of Exp 2 do not support the interpretation
                  that retardation in learning following masked
                  pre-exposure in human experiments is comparable to
                  latent inhibition following simple preexposure in
                  other animals.},
  keywords = {role of pre-exposure \& latent inhibition \&
                  negative priming in retardation in discrimination
                  learning, 20-40 yr olds},
  tags = {psychology;np},
}
@article{Grande2003,
  author = {Grande, Laura J.},
  year = {2003},
  title = {Inhibition and attentional processes in Parkinson's
                  disease},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {63},
  number = {9},
  pages = {4369},
  abstract = {The purpose of this project was to investigate the
                  possibility that the neuropathology of Parkinson's
                  disease (PD) impairs the inhibitory mechanism of
                  attention while sparing the facilitatory mechanism.
                  It was further predicted that only
                  endogenously-evoked inhibition (that is, inhibition
                  driven by goal directed behavior) is disrupted in PD
                  with sparing of exogenously-evoked inhibition (that
                  is, inhibition driven by sensory information). Two
                  computerized experimental tasks were employed to
                  investigate these hypotheses in PD patients and
                  healthy control (HC) participants. Participants
                  completed an inhibition of return and negative
                  priming task on two occasions, and PD patients
                  completed each measure both on and off their
                  dopamine medication.  Results indicated that
                  patients with PD demonstrated intact
                  exogenously-evoked inhibition for a spatial
                  location, as evidenced in the inhibition of return
                  experiment, and impaired endogenously-evoked
                  inhibition for a spatial location, as evidenced in
                  the negative priming experiment.  These findings
                  support the idea that the characteristic motor
                  dysfunction in PD exists for voluntary or volitional
                  movements with relative sparing of automatic or
                  reflexive movements (Pollak, 1998). While current
                  models characterizing the cognitive impairment
                  associated with PD have focused primarily on the
                  motor disorder associated with PD, evidence exists
                  that suggests the same systems play a role in
                  higher-level cognitive processing (Alexander,
                  DeLong, \& Strick, 1986; Middleton \& Strick, 2000a,
                  2000b; Penney \& Young, 1986).  Based on this work,
                  the current study suggests that disruption in a
                  cortico-striato-pallido-thalamo circuit may be
                  important in producing the disruption of inhibitory
                  functions in PD. This study underscores the
                  importance of considering the connectivity of
                  neuroanatomical structures, and illustrates how the
                  impaired functioning of one structure may affect the
                  functioning of another structure downstream. It is
                  suggested that the ultimate effect of PD is
                  excessive inhibition of the thalamus and resultant
                  reduction in cortical excitation. This increased
                  thalamic inhibition results in the altered
                  functioning of specific thalamic nuclei as well as
                  frontal lobes that have been shown to be necessary
                  for endogenously-evoked inhibition.},
  keywords = {neuropathology Parkinson's disease inhibitory
                  functions attentional processes Print},
  tags = {psychology;np},
}
@article{Grant2000,
  author = {Grant, Julia D. and Dagenbach, Dale},
  year = {2000},
  title = {Further considerations regarding inhibitory
                  processes, working memory, and cognitive aging},
  journal = {American Journal of Psychology},
  volume = {113},
  number = {1},
  pages = {69-94},
  abstract = {The present study explored the relationship between
                  inhibitory processing (as indexed by identity
                  negative priming in a letter-naming task), working
                  memory, discourse processing, and cognitive aging
                  effects.  Consistent with several other recent
                  reports, this study found evidence of intact
                  inhibitory processing, as measured by negative
                  priming, in older adults (65-92 yrs old) as well as
                  younger adults (18-21 yrs old). This intact negative
                  priming occurred in conjunction with diminished
                  working memory span and impaired memory in the
                  discourse processing task in the same sample of
                  older adults, further arguing against the likelihood
                  that declines in these areas are caused by
                  impairments in the inhibitory processes measured by
                  negative priming.  The implications of these results
                  for theories of cognitive aging and possible reasons
                  for inconsistent findings regarding negative priming
                  effects among the elderly are discussed.},
  keywords = {inhibitory \& discourse processing \& working memory
                  \& cognitive aging, 18-21 \& 65-92 yr olds},
  tags = {psychology;np},
  comment = {JB; NP; CA; WM; CI; Geronto},
}
@article{Gratton1983,
  author = {Gratton, Gabriele and Coles, Michael G. H. and
                  Donchin, Emanuel},
  year = {1983},
  title = {A new method for off-line removal of ocular
                  artifact},
  journal = {Electroencephalography and Clinical Neurophysiology},
  volume = {55},
  number = {4},
  pages = {468--484},
  abstract = {A new off-line procedure for dealing with ocular
                  artifacts in ERP recording is described. The
                  procedure (EMPC) uses EOG and EEG records for
                  individual trials in an experimental session to
                  estimate a propagation factor which describes the
                  relationship between the EOG and EEG traces.  The
                  propagation factor is computed after stimulus-linkes
                  variability in both traces has been
                  removed. Different propagation factors are computed
                  for blinks and eye movements. Tests are presented
                  which demonstrate the validity and reliability of
                  the procedure. ERPs derived from trials corrected by
                  EMCP are more similar to a `true' ERP than are ERPs
                  derived from either uncorrected or randomly
                  corrected trials.  The procedure also reduces the
                  difference between ERPs which are based on trials
                  with different degrees of EOG variance. Furthermore,
                  variability at each time point, across trials, is
                  reduced following correction. The propagation factor
                  decreases from frontal to parietal electrodes, and
                  is larger for saccades than blinks. It is more
                  consistent within experimental sessions than between
                  sessions. The major advantage of the procedure is
                  that it permits retention of all trials in an ERP
                  experiment, irrespective of ocular artifact. Thus,
                  studies of populations characterized by a high
                  degree of artifact, and those requiring eye
                  movements as part of the experimental task, are made
                  possible. Furthermore, there is no need to require
                  subjects to restrict eye movement activity. In
                  comparison to procedures suggested by others, EMCP
                  also has the advantage that separate correction
                  factors are computed for blinks and movements and
                  that these factors are based on data from the
                  experimental session itself rather than from a
                  separate calibration session.},
  comment = {{\bf J{\"o}rg: } \\ {\bf Matthias: } \\ {\bf Hecke:
                  }},
}
@article{Green1999,
  author = {Green, Melissa J. and Williams, Leanne M.},
  year = {1999},
  title = {Schizotypy and creativity as effects of reduced
                  cognitive inhibition},
  journal = {Personality \& Individual Differences},
  volume = {27},
  number = {2},
  pages = {263-276},
  abstract = {Tested the hypothesis that enhanced creativity in
                  highly schizotypal individuals may be explicable in
                  terms of reduced cognitive inhibition.  A total of
                  72 participants (aged 17-47 yrs) completed the STA
                  measure of schizotypy (G. S. Claridge and P. Broks,
                  1984), the "instances" and "uses" tests from the
                  M. A. Wallach and N. Kogan (1965) divergent thinking
                  battery, and a negative priming task as a measure of
                  cognitive inhibition. It was predicted that higher
                  levels of schizotypy would be independently
                  associated with higher creativity as reflected in
                  greater divergent thinking, and with reduced
                  cognitive inhibition.  However, it was further
                  hypothesized that the relationship between
                  schizotypy and creativity would be partially
                  mediated by the level of cognitive inhibition. A
                  significant positive correlation was found between
                  STA scores and the number of original (unique)
                  responses generated for divergent thinking measures,
                  but the previously reported (e.g., A. Beech and
                  Claridge, 1987) association between reduced
                  cognitive inhibition and schizotypy was not
                  replicated. The contribution of schizotypy and
                  cognitive mechanisms to creative thinking is
                  discussed in terms of a broader theory which
                  considers the interaction of these traits with other
                  aspects of temperament.},
  keywords = {schitotypy \& creativity-divergent thinking \&
                  reduced cognitive inhibition assessed by negative
                  priming, 17-47 yr olds},
  tags = {psychology;np},
}
@article{Grill-Spector2006,
  author = {Kalanit Grill-Spector and Richard Henson and Alex
                  Martin},
  year = {2006},
  title = {Repetition and the brain: neural models of
                  stimulus-specific effects.},
  journal = {Trends Cogn Sci},
  volume = {10},
  number = {1},
  pages = {14--23},
  doi = {10.1016/j.tics.2005.11.006},
  abstract = {One of the most robust experience-related cortical
                  dynamics is reduced neural activity when stimuli are
                  repeated. This reduction has been linked to
                  performance improvements due to repetition and also
                  used to probe functional characteristics of neural
                  populations. However, the underlying neural
                  mechanisms are as yet unknown. Here, we consider
                  three models that have been proposed to account for
                  repetition-related reductions in neural activity,
                  and evaluate them in terms of their ability to
                  account for the main properties of this phenomenon
                  as measured with single-cell recordings and
                  neuroimaging techniques.  We also discuss future
                  directions for distinguishing between these models,
                  which will be important for understanding the neural
                  consequences of repetition and for interpreting
                  repetition-related effects in neuroimaging data.},
