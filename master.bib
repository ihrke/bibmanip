@article{Kolanczyk2002,
  author = {Kolanczyk, Alina and Pawlowska-Fusiara, Monika},
  year = {2002},
  title = {Automatic correction or controlled processing of
                  affective priming},
  journal = {Polish Psychological Bulletin},
  volume = {33},
  number = {2},
  pages = {37-46},
  abstract = {Studied the effects of positive and negative
                  semantic priming and of "automatic correction"
                  (J. Glaser and M. R. Banaji, 1999) conditioned by
                  attention in the affective priming
                  paradigm. Semantic priming involved loudly read
                  extremely and moderately positive or negative Polish
                  target words. In Exp 1 (80 college students) the
                  prime duration was 100 msec and in Exp 2 (80 law and
                  college students) 75 msec.  It was assumed that
                  longer reaction time was the result of deeper
                  information processing, which would mean better
                  remembering of words.  The results show a weak (100
                  msec) and a stronger (75 msec) assimilation effect;
                  affectively consistent words were read faster than
                  inconsistent words. However, the assimilation effect
                  was significant when primes were positive but not
                  when primes were negative. Expected difference
                  between positive and negative priming effects were
                  observed. Negative target words were remembered
                  better. It is suggested that negative words are
                  probably attentionally controlled earlier and
                  stronger, which is why the assimilation effect
                  weakened in the case of negative words.},
  keywords = {semantic priming affective priming positive words
                  negative words reaction time attention word recall
                  Emotional Content Reaction Time Recall (Learning)
                  Print},
  tags = {psychology;np},
}
