@article{Derryberry1991,
  author = {Derryberry, Douglas},
  year = {1991},
  title = {The immediate effects of positive and negative
                  feedback signals},
  journal = {Journal of Personality \& Social Psychology},
  volume = {61},
  number = {2},
  pages = {267-278},
  abstract = {Reaction time priming techniques were used in 4
                  experiments to assess the effects of feedback
                  signals on arousal, response, and attentional
                  processes. The letter A, C, or F served as a priming
                  signal presented 100 ms before a target
                  letter. Emotional value was manipulated by using A,
                  C, and F to reflect good, average, and poor
                  performance on the previous trial. The positive (A),
                  neutral (C), and negative (F) primes did not differ
                  in their effects on arousal. At the response level,
                  positive and neutral primes exerted similar effects,
                  and negative primes led to response
                  inhibition. Regarding attention, positive and
                  negative primes attracted greater attention than
                  neutral primes, with the effect stronger for
                  positive than for negative primes. These effects
                  disappeared when the emotional value of the stimuli
                  was removed, and the effects were not a consequence
                  of expectancies or performance factors extending
                  across trials.},
  keywords = {positive vs negative priming feedback, arousal \&
                  response \& attentional processes, college students},
  tags = {psychology;np},
}
@article{Deschepper1996a,
  author = {Deschepper, Brett Gerald},
  year = {1996},
  title = {Negative priming of novel visual shapes},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {56},
  number = {9},
  pages = {5202},
  abstract = {A series of experiments explored implicit memory for
                  novel nonsense shapes using a negative priming
                  paradigm. The results showed that memory for shapes
                  can be formed in a single trial and without
                  attention, and that they last with little or no
                  decrement across two hundred intervening trials and
                  for delays of up to a month. No explicit memory of
                  the shapes is available, either immediately after
                  presentation or after a delay. Repetition of the
                  unattended shapes makes them easier to reject
                  without increasing the negative priming when they
                  are later attended. Attended viewing of the shapes
                  clears the negative priming. Changing tasks between
                  prime and probe trials shifts the negative priming
                  to facilitation or no priming. There appears to be a
                  dissociation between the memory of the shape and an
                  associated "action tag" which specifies whether it
                  was attended or ignored.  Representations of the
                  shapes survive longer and are more likely to
                  transfer between different experimental conditions
                  and tasks than the associated "action tags". The
                  processing level at which the representations are
                  formed appear to depend upon the requirements of the
                  particular task being performed. The results
                  demonstrate that lasting memories can be formed
                  without the benefit of attention, suggesting that
                  the role of attention may be simply to ensure
                  voluntary retrievability.  The surprising
                  combination of plasticity and permanence in implicit
                  memory for novel stimuli seen without attention
                  provides strong constraints on possible models of
                  memory.},
  keywords = {negative priming, implicit memory of novel visual
                  shapes, human Ss},
  tags = {psychology;np},
  pdf = {./pdf/DeSchepper_1996_Visual.pdf},
}
