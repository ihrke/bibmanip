  tags = {psychology;np},
}
@article{Merrill1996,
  author = {Merrill, Edward C. and Taube, Meredith},
  year = {1996},
  title = {Negative priming and mental retardation: The
                  processing of distractor information},
  journal = {American Journal on Mental Retardation},
  volume = {101},
  number = {1},
  pages = {63-71},
  abstract = {Negative priming was assessed to study processing of
                  distractor (DT) information by adults with and
                  without mental retardation (MR). Ss were 18 adults
                  with MR (mean age 17.8 yrs; mean IQ = 62.2) and 18
                  adults without MR (mean age 19.1 yrs). Ss viewed
                  positive, negative, and neutral prime and probe
                  displays of 1 blue and 1 red letter presented at
                  stimulus onset asynchronies (SOAs) of 100, 250, and
                  500 msec.  Response times to identify the target and
                  DT were also recorded.  At 100 msec SOA, all Ss
                  showed facilitation in identifying targets in the
                  probe display that were the same as the target or DT
                  of the prime, indicating the automatic activation of
                  both letters. At 500 msec SOA, only the Ss without
                  MR exhibited inhibition of the DTs and Ss with MR
                  did not suppress responding to the DT to facilitate
                  their performance. Results support the role of
                  attention mechanisms in DT information processing.},
  keywords = {positive vs negative priming \& stimulus onset
                  asynchrony, processing distractor information \&
                  identification of letter target, adults with vs
                  without mental retardation Selective Attention},
  tags = {psychology;np},
}
@article{Merrill2001,
  author = {Merrill, Edward C. and McCown, Steven M. and Kelley,
                  Shirley},
  year = {2001},
  title = {Similarities in negative priming between persons
                  with and those without mental retardation of equal
                  chronological age},
  journal = {American Journal on Mental Retardation},
  volume = {106},
  number = {2},
  pages = {113-122},
  abstract = {Adolescents with and those without mental
                  retardation participated in a negative priming
                  procedure in two experiments. They identified
                  letters to stimulus displays presented in
                  pairs. Negative priming was observed as the slowing
                  of response times when the distractor in the first
                  display (prime) became the target in the second
                  display (probe). In the standard procedure, all
                  displays include one target and one distractor. In
                  our modification, prime displays occasionally
                  included a distractor without a target. Although
                  adolescents with mental retardation typically do not
                  exhibit negative priming when responding on the
                  basis of stimulus identity, they did so with single
                  letter primes in these experiments.},
