@article{Ellenbogen2006,
  author = {Jeffrey M Ellenbogen and Justin C Hulbert and Robert
                  Stickgold and David F Dinges and Sharon L
                  Thompson-Schill},
  year = {2006},
  title = {Interfering with theories of sleep and memory:
                  sleep, declarative memory, and associative
                  interference.},
  journal = {Curr Biol},
  volume = {16},
  number = {13},
  pages = {1290--1294},
  doi = {10.1016/j.cub.2006.05.024},
  abstract = {Mounting behavioral evidence in humans supports the
                  claim that sleep leads to improvements in recently
                  acquired, nondeclarative memories.  Examples include
                  motor-sequence learning; visual-discrimination
                  learning; and perceptual learning of a synthetic
                  language. In contrast, there are limited human data
                  supporting a benefit of sleep for declarative
                  (hippocampus-mediated) memory in humans (for review,
                  see). This is particularly surprising given that
                  animal models (e.g.,) and neuroimaging studies
                  (e.g.,) predict that sleep facilitates
                  hippocampus-based memory consolidation. We
                  hypothesized that we could unmask the benefits of
                  sleep by challenging the declarative memory system
                  with competing information (interference). This is
                  the first study to demonstrate that sleep protects
                  declarative memories from subsequent associative
                  interference, and it has important implications for
                  understanding the neurobiology of memory
                  consolidation.},
  keywords = {Adolescent; Adult; Association Learning; Female;
                  Humans; Male; Memory; Mental Recall; Models,
                  Neurological; Sleep},
  institution = {, Harvard Medical School, 330 Brookline Avenue,
                  Feldberg 861, Boston, Massachusetts 02215,
                  USA. jeffrey_ellenbogen@hms.harvard.edu},
}
@article{Endo2001,
  author = {Endo, Nobutaka and Saiki, Jun and Saito, Hirofumi},
  year = {2001},
  title = {Determinants of occurrence of negative priming for
                  novel shapes with matching paradigm},
  journal = {Japanese Journal of Psychology},
  volume = {72},
  number = {3},
  pages = {204-212},
  abstract = {Examined the effect of the relation between
                  interferences from a distractor and an inhibitory
                  mechanism on the occurrence of negative priming, by
                  using a matching paradigm. Ss were 48 undergraduates
                  with normal visual accuracy in Japan. The ease of
                  perceptual target selection was determined by
                  magnitudes of interference. Stereoscopic depth was
                  used to manipulate the relative position of a
                  target, a distractor and a standard figure. The
                  results show: (1) that when the ease of perceptual
                  target selection was manipulated by separation of
                  the target from the distractor in depth, positive
                  priming occurred; however, the ease of target
                  selection turned out to be unnecessary in the
                  occurrence of priming effects; and (2) that when the
                  distractor and the standard figure were displayed at
                  the same depth plane, where the distractor was
                  supposed to interfere with the encoding of a
                  standard figure, negative priming occurred
                  consistently regardless of the ease of perceptual
                  target selection. These results suggest that an
                  inhibitory mechanism contributes to the suppression
                  of inappropriate processes by a distractor, and an
                  inhibitory mechanism actively keeps distractors from
                  interfering with standard figures.},
  keywords = {interference distractors inhibitory mechanism
                  negative priming matching visual accuracy perceptual
                  target selection Print},
  tags = {psychology;np},
}
@article{Engle1995,
  author = {Engle, Randall W. and Conway, Andrew R. A. and
                  Tuholski, Stephen W. and Shisler, Rebecca J.},
  year = {1995},
  title = {A resource account of inhibition},
  journal = {Psychological Science},
  volume = {6},
  number = {2},
  pages = {122-125},
  abstract = {Explored whether inhibition on a letter-naming task
                  is a result of controlled attention with 45 Ss and
                  46 controls, all undergraduates.  When the naming
                  task was performed under a mental work load,
                  negative priming was diminished as work load
                  increased. This finding suggests that inhibition is
                  a product of controlled resources and that group
                  differences in inhibition may result from
                  differences in controlled attentional resources, not
                  from inefficient inhibitory mechanisms.},
  keywords = {controlled attentional resources, inhibition on
                  letter naming task, college students},
  tags = {psychology;np},
}
@article{Enright1990,
  author = {Enright, Simon J. and Beech, Anthony R.},
  year = {1990},
  title = {Obsessional states: Anxiety disorders or
                  schizotypes? An information processing and
                  personality assessment},
  journal = {Psychological Medicine},
  volume = {20},
  number = {3},
  pages = {621-627},
  abstract = {Examined negative priming effects and prevalence of
                  schizotypal personality traits in 16 patients with
                  obsessive-compulsive disorder (OCD) and 15 controls
                  with anxiety disorders (ADs). Findings support the
                  hypothesis that OCD is a distinct disorder
                  qualitatively different from AD categories.  OCD Ss
                  exhibited nonsignificant priming effects while AD
                  controls showed a highly significant negative
                  priming effect. OCD may be misclassified as an
                  anxiety disorder and may be categorically more
                  closely aligned to the schizophrenic constellation
                  of disorders.},
  keywords = {negative priming effects \& prevalence of
                  schizotypal personality traits, patients with
                  obsessive compulsive vs anxiety disorders},
  tags = {psychology;np},
}
@article{Enright1993,
  author = {Enright, Simon J. and Beech, Anthony R.},
  year = {1993},
  title = {Further evidence of reduced cognitive inhibition in
                  obsessive-compulsive disorder},
  journal = {Personality \& Individual Differences},
  volume = {14},
  number = {3},
  pages = {387-395},
  abstract = {The concept of cognitive inhibition is reviewed
                  together with experimental evidence in both clinical
                  and non-clinical samples. Two negative priming
                  experiments are described in which 36
                  obsessive-compulsive disordered (OCD) Ss are
                  consistently distinguished from all other categories
                  of anxiety disorder. It is suggested that this
                  differentiation becomes more apparent as the
                  cognitive complexity of the experimental task
                  increases. Results are interpreted as further
                  evidence for reduced cognitive inhibition in OCD
                  sufferers. Comparison is drawn between the current
                  results and those of previous negative priming
                  studies (e.g., D. A. Allport et al, 1985) with
                  nonclinical, high schizotype, and schizophrenic
                  Ss. Implications of these findings for understanding
                  of OCD are discussed.},
  keywords = {complexity of negative priming task, cognitive
                  inhibition, patients with obsessive compulsive vs
                  other anxiety disorders Priming},
  tags = {psychology;np},
}
@article{Enright1993a,
  author = {Enright, Simon J. and Beech, Anthony R.},
  year = {1993},
  title = {Reduced cognitive inhibition in obsessive-compulsive
                  disorder},
  journal = {British Journal of Clinical Psychology},
  volume = {32},
  number = {1},
  pages = {67-74},
  abstract = {On a semantic negative priming task designed to
                  investigate putative inhibitory mechanisms of
                  selective attention, 36 outpatients with
                  obsessive-compulsive disorder (OCD) were
                  distinguished from 60 outpatients with other
                  subcategories of anxiety disorder (OAD). OCD Ss
                  failed to show any priming effects in the repetition
                  priming condition and exhibited facilitation in the
                  semantic priming condition (i.e., shorter reaction
                  times [RTs] to previously ignored stimuli). OAD Ss
                  demonstrated negative priming (i.e., longer RTs to
                  previously ignored stimuli) in both experimental
                  conditions. Results are interpreted in terms of
                  reduced cognitive inhibition in OCD.},
  keywords = {semantic negative priming \& cognitive inhibition in
                  selective attention, outpatients with obsessive
                  compulsive vs other anxiety disorders Semantics},
  tags = {psychology;np},
}
@article{Enright1995,
  author = {Enright, Simon J. and Beech, Anthony R. and
                  Claridge, Gordon S.},
  year = {1995},
  title = {A further investigation of cognitive inhibition in
                  obsessive-compulsive disorder and other anxiety
                  disorders},
  journal = {Personality \& Individual Differences},
  volume = {19},
  number = {4},
  pages = {535-542},
  abstract = {Examined cognitive inhibition, in terms of negative
                  priming effects, in 32 obsessive-compulsive disorder
                  (OCD) patients, by replicating the study of
                  S. J. Enright and A. R. Beech (1990, 1993). OCD Ss
                  (mean age 36.7 yrs) were divided into high and low
                  checkers, on the basis of clinical interviews, and
                  their responses on the Maudsley Obsessive Compulsive
                  Inventory (R. J. Hodgson et al, 1977). Ss were
                  compared with patients of other anxiety disorders
                  (OAD, mean age 35.2 yrs).  They were required to
                  discriminate between ink colors and words written in
                  ink, presented under 4 experimental conditions, and
                  at 3 different speeds. Results show that reduced
                  negative priming occurred at fastest presentation
                  speed in OCD Ss, as compared to OAD Ss; these
                  differences between the groups became
                  indistinguishable as presentation speed
                  lowered. Negative priming in high checkers is
                  distinguished from low or noncheckers for all
                  presentation speeds.},
  keywords = {negative priming effects in cognitive inhibition,
                  patients with obsessive compulsive disorder vs
                  generalized anxiety disorder or monophobia or social
                  phobia Social Phobia},
  tags = {psychology;np},
}
