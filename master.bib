@article{Schooler1997,
  author = {Schooler, Carmi and Neumann, Ewald and Caplan,
                  Leslie J. and Roberts, Bruce R.},
  year = {1997},
  title = {Continued inhibitory capacity throughout adulthood:
                  Conceptual negative priming in younger and older
                  adults},
  journal = {Psychology \& Aging},
  volume = {12},
  number = {4},
  pages = {667-674},
  abstract = {Two negative priming experiments in older and
                  younger adults are reported.  Participants in
                  Experiment 1, involving both positive and negative
                  priming conditions, showed both types of
                  priming. There were no significant differences
                  between age groups. If anything, older participants
                  showed more negative priming. In Experiment 2,
                  involving only negative priming conditions, similar
                  results were obtained. Our findings rule out
                  possible effects of experimental conditions that
                  episodic retrieval theorists have suggested might
                  account for negative priming in older
                  adults. Although our results may be consistent with
                  an explanation of negative priming in older adults
                  by an expansively specified theory of episodic
                  retrieval, they are at least as consistent with the
                  view that inhibitory processes are intact in older
                  adults. In light of these findings, conflicting
                  empirical results and alternative views of negative
                  priming in older adults are examined.},
  keywords = {negative priming, inhibitory component of selective
                  attention, 20-35 vs 65-77 yr olds},
  tags = {psychology;np},
}
@article{Schreiber1999,
  author = {Thomas Schreiber and Andreas Schmitz},
  year = {1999},
  title = {Improved surrogate data for nonlinearity tests},
  journal = {arXiv},
  volume = {chao-dyn},
  month = {Sep},
  url = {http://arxiv.org/abs/chao-dyn/9909041v1},
  abstract = {Current tests for nonlinearity compare a time series
                  to the null hypothesis of a Gaussian linear
                  stochastic process. For this restricted null
                  assumption, random surrogates can be constructed
                  which are constrained by the linear properties of
                  the data. We propose a more general null hypothesis
                  allowing for nonlinear rescalings of a Gaussian
                  linear process. We show that such rescalings cannot
                  be accounted for by a simple amplitude adjustment of
                  the surrogates which leads to spurious detection of
                  nonlinearity. An iterative algorithm is proposed to
                  make appropriate surrogates which have the same
                  autocorrelations as the data and the same
                  probability distribution.},
  keywords = {nlin.CD, chao-dyn},
  tags = {{lpaper}},
  eprint = {chao-dyn/9909041v1},
  annote = {Published in: Phys. Rev. Lett. 77 (1996) 635 4
