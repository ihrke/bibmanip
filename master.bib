@article{Metzler2000,
  author = {Metzler, Claudia and Parkin, Alan J.},
  year = {2000},
  title = {Reversed negative priming following frontal lobe
                  lesions},
  journal = {Neuropsychologia},
  volume = {38},
  number = {4},
  pages = {363-379},
  abstract = {Investigated whether lesions to the prefrontal
                  cortex would lead to impaired inhibitory control in
                  selective attention. Patients with uni- and
                  bilateral frontal lobe damage (aged 24.17-65.83 yrs)
                  and age-matched control participants (aged 18-68)
                  were compared in an identity negative priming task
                  involving letter naming. Whereas all control groups
                  revealed robust negative priming and distractor
                  interference, the majority of patients showed
                  positive instead of negative priming and not all of
                  them were more susceptible to interference. These
                  results suggest that frontal lobe lesions disrupt
                  distractor inhibition and cannot be explained in
                  terms of a retrieval deficit. However, the level of
                  interference may not only be determined by
                  inhibition but also by excitation of the target
                  stimulus.},
  keywords = {prefrontal cortical lesions, inhibitory control in
                  selective attention, 24.17-65.83 yr old patients
                  with uni- \& bilateral frontal lobe damage Brain
                  Damage Priming},
  tags = {psychology;np},
}
@article{Metzler2001,
  author = {Metzler, Claudia},
  year = {2001},
  title = {Location negative priming may be abolished in older
                  age},
  journal = {Zeitschrift f{\"u}r Gerontopsychologie \&
                  -psychiatrie},
  volume = {14},
  number = {4},
  pages = {187-202},
  abstract = {Investigated the effects of aging on location
                  negative priming (LNP) in 2 experiments. Exp 1
                  explored whether LNP was a robust phenomenon across
                  a wide age range in 99 Ss (aged 18-71 yrs). Exp 2
                  investigated whether the abolition of LNP in older
                  adults observed in Exp 1 could be replicated and
                  whether individual variables that have been shown to
                  influence the level of negative priming could
                  account for differences between age groups in 20 Ss
                  (aged 18-28 yrs) and 20 Ss (aged 62-80 yrs). A
                  significant abolition of LNP in older relative to
                  younger Ss was found. LNP was a robust effect over
                  early and middle adulthood (19-59 yrs) but declined
                  from the age of 60 yrs onwards. Moreover, this
                  effect could not be accounted for by psychometric
                  variables that have been associated with impaired
                  negative priming (dementia, depression, schizotypy,
                  cognitive failure). It is concluded that the data
                  provide evidence that LNP can be severely diminished
                  in older adults. It is suggested that discrepancies
                  to previous studies might have arisen due to
                  differences in the educational level of participants
                  in different studies.},
  keywords = {aging aged differences location negative priming
                  Print},
  tags = {psychology;np},
  pdf = {./pdf/Metzler_2001.pdf},
  comment = {JB; NP; Rigososum; gelesen; Geronto},
}
@article{Meuter1999,
  author = {Meuter, Renata F. I. and Allport, Alan},
  year = {1999},
  title = {Bilingual language switching in naming: Asymmetrical
                  costs of language selection},
  journal = {Journal of Memory \& Language},
  volume = {40},
  number = {1},
  pages = {25-40},
  abstract = {In an experimental study of language switching and
                  selection, bilinguals named numerals in either their
                  first or second language unpredictably.  Response
                  latencies (RTs) on switch trials (where the response
                  language changed from the previous trial) were
                  slower than on nonswitch trials.  As predicted, the
                  language-switching cost was consistently larger when
                  switching to the dominant L-sub-1 from the weaker
                  L-sub-2 than vice versa such that, on switch trials,
                  L-sub-1 responses were slower than in L-sub-2. This
                  "paradoxical" asymmetry in the cost of switching
                  languages is explained in terms of differences in
                  relative strength of the bilingual's two languages
                  and the involuntary persistence of the previous
                  language set across an intended switch of language.
                  Naming in the weaker language, L-sub-2, requires
                  active inhibition or suppression of the stronger
                  competitor language, L-sub-1; the inhibition
                  persists into the following (switch) trial in the
                  form of "negative priming" of the L-sub-1 lexicon as
                  a whole.},
  keywords = {language switching \& selection \& response
                  latencies during naming task, 23-44 yr old bilingual
                  speakers},
  tags = {psychology;np},
}
