@article{Frings2005a,
  author = {Christian Frings and Dirk Wentura},
  year = {2005},
  title = {Negative priming with masked distractor-only prime
                  trials: awareness moderates negative priming.},
  journal = {EP},
  volume = {52},
  number = {2},
  pages = {131--139},
  abstract = {The literature yields inconsistent evidence for
                  negative priming (NP) following masked
                  distractor-only prime trials. We contrast two
                  different hypotheses on the inconsistent findings:
                  one - which is most compatible with the temporal
                  discrimination theory - that relates the sign of
                  priming effects to the absence vs. presence of prime
                  awareness and one -which is most compatible with the
                  inhibition and episodic retrieval accounts - that
                  relates the sign of priming effects to the prime
                  event being categorized as a to-be-attended
                  vs. to-be-ignored event.  In two experiments, it
                  turned out that participants' awareness of the
                  masked stimuli caused the different results (with
                  participants being not aware of the primes showing
                  NP), whereas the factor prime color = probe target
                  color vs. prime color = probe distractor color
                  (i.e., the prime contains the to-be-attended vs. the
                  to-be-ignored signal) did not moderate NP. These
                  findings are discussed with regard to theories of
                  negative priming and the debate on conscious
                  vs. unconscious perception.},
  keywords = {Adult, Awareness, Female, Humans, Inhibition
                  (Psychology), Male, Perceptual Masking, Reaction
                  Time, Time Perception, Unconscious (Psychology),
                  15850160},
  tags = {psychology;np},
  comment = {{\bf Hecke:} Non Awareness = Actively ignoring
                  distractor-only prime trials: if unaware or ignored
                  $\rightarrow$ np egal welche Farbe, unaware
                  $\rightarrow$NP, marginally aware $\rightarrow$ PP.
                  Eins der wenigen Paper, die nochmal auf die temporal
                  discrimination theory eingehen.},
}
