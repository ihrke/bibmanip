@article{Otten1998,
  author = {Otten, Leuntje Jacoba},
  year = {1998},
  title = {Processing unattended visual information: Insights
                  from an analysis of repetition priming using
                  event-related brain potentials},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {58},
  number = {10},
  pages = {5665},
  abstract = {Two unresolved issues in the attention and memory
                  literature concern the extent and nature of the
                  processing of unattended visual information.  In an
                  earlier study, it was found that when individuals
                  make lexical decisions about one of two
                  simultaneously presented, visual letter strings, a
                  modulation of event-related potentials (ERPs) may be
                  observed when the unattended word is repeated on the
                  subsequent trial as the unattended word. This
                  modulation differs in polarity and scalp
                  distribution from that typically seen when attended
                  words are repeated. In seven experiments, it is
                  established that the ERP modulation for repeated
                  unattended words (a) is reliable and attributable to
                  the repeated unattended words rather than to
                  accompanying attended words, (b) can occur for words
                  repeated in a different location and in a different
                  font, (c) is associated with faster lexical decision
                  responses, (d) is not related to the mechanism that
                  underlies the slowing in reaction times to recently
                  ignored targets (negative priming), (e) can occur
                  for digits as well as words, and (f) is related to
                  ease of selection.  The occurrence of this ERP
                  modulation suggests that the processing of
                  unattended information can extend beyond elementary
                  physical features (to involve orthographic identity
                  in the case of words). This processing leaves a
                  representation in memory which can influence
                  subsequent processing for at least 3 s. The
                  qualitative differences between the ERP modulations
                  for repeated unattended and repeated attended
                  information suggest that repeated unattended
                  information is processed in a qualitatively
                  different manner than repeated attended information.
                  Repeated unattended information appears to benefit
                  from an inhibitory mechanism sometimes applied to
                  unattended information to enable selective
                  processing. The inhibited representation may carry
                  forward in time, making subsequent selections easier
                  when the same item is again irrelevant.},
  keywords = {Processing unattended visual information: Insights
                  from an analysis of repetition priming using
                  event-related brain potentials (attention) Human
                  Information Storage Lexical Access Words (Phonetic
                  Units)},
  tags = {psychology;np},
}
@article{Ozari1997,
  author = {Ozari, Tomer Joseph},
  year = {1997},
  title = {The inhibition of multiple visual distractors in
                  visual selective attention: Parallel and serial
                  processing of inhibition},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {58},
  number = {6},
  pages = {3333},
  abstract = {A priming paradigm was used to examine the
                  systematic reduction of the negative priming effect
                  as the number of distractors in the prime display is
                  increased, as reported by Neumann and DeSchepper
                  (1992).  In Experiments 1 and 2, subjects were
                  presented visual displays in pairs. Inhibition was
                  observed when a distractor in the prime display
                  became a target in the next (probe) display and was
                  measured by increased response times to identifying
                  the target in the second display. In Experiment 1,
                  the location of the target was consistent. In
                  Experiment 2, the location of the target was
                  varied. In each experiment, the number of
                  distractors (one or three) and stimulus onset
                  asynchrony (SOA) were manipulated. Varying SOA was a
                  way of controlling subjects' processing of the prime
                  display before the onset of the probe display.  In
                  Experiment 1, due to the consistent and predictable
                  location of the target, there was no effect of
                  priming when comparing the negative priming to the
                  neutral trials. These results were explained in
                  terms of an early selection strategy when selection
                  is easy. In Experiment 2, the uncertain location of
                  the target from trial to trial forced the subjects
                  to process the distractors. In the one distractor
                  negative priming condition there was inhibition at
                  the long SOA and facilitation at the short
                  SOA. Facilitation at the short SOA provided evidence
                  for a lack of inhibition. In the three distractor
                  negative priming condition there was facilitation at
                  both the short and long SOAs.  This indicates that
                  the distractors were activated and remained active,
                  thus facilitating a response when one of the
                  distractors became a target in the subsequent
                  trial. The findings were explained in terms of a
                  limited resource attentional model in which there is
                  a lack of resources for inhibition to take place
                  when perceptual load is high. The resource account
                  of inhibition might explain why populations such as
                  children, older adults, and individuals with mental
                  retardation do not exhibit the negative priming
                  effect in an identity task with one distractor.},
  keywords = {target location \& number of visual distractors \&
                  stimulus onset asynchrony, visual selective
                  attention \& inhibition of negative priming effect,
                  humans Visual Stimulation},
  tags = {psychology;np},
}
@article{Ozonoff1997,
  author = {Ozonoff, Sally and Strayer, David L.},
  year = {1997},
  title = {Inhibitory function in nonretarded children with
                  autism},
  journal = {Journal of Autism \& Developmental Disorders},
  volume = {27},
  number = {1},
  pages = {59-77},
  abstract = {Examined inhibitory function in 13 nonretarded
                  children with autism and 13 normally developing
                  controls matched on age and IQ. Tasks measuring
                  motor and cognitive components of inhibition were
                  administered to both groups. On the Stop-Signal
                  paradigm, children with autism were able to inhibit
                  motor responses to neutral and prepotent stimuli as
                  well as control Ss. On the Negative Priming task,
                  the groups were equally capable of inhibiting
                  processing of irrelevant distractor stimuli in a
                  visual display. Results suggest that at least 2
                  components of inhibition are spared in individuals
                  with autism, standing in contrast to flexibility and
                  other executive deficits that have been found in
                  previous studies. These findings may help
                  distinguish children with autism from those with
                  other neurodevelopmental conditions that involve
                  executive dysfunction.},
  keywords = {motor \& cognitive inhibitory function, nonretarded
                  children with autism, implications for
                  distinguishing autism with other neurodevelopmental
                  conditions Priming},
  tags = {psychology;np},
}
@article{Ozonoff1998,
