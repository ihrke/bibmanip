@article{Newman2002Random,
  author = {Newman, M. E. J. and Watts, D. J. and Strogatz,
                  S. H.},
  year = {2002},
  title = {Random graph models of social networks},
  journal = {Proceedings of the National Academy of Sciences of
                  the United States of America},
  volume = {99},
  number = {Suppl 1},
  pages = {2566--2572},
  month = {feb},
  day = {19},
  doi = {10.1073/pnas.012582999},
  url = {http://dx.doi.org/10.1073/pnas.012582999},
  abstract = {We describe some new exactly solvable models of the
                  structure of social networks, based on random graphs
                  with arbitrary degree distributions. We give models
                  both for simple unipartite networks, such as
                  acquaintance networks, and bipartite networks, such
                  as affiliation networks. We compare the predictions
                  of our models to data for a number of real-world
                  social networks and find that in some cases, the
                  models are in remarkable agreement with the data,
                  whereas in others the agreement is poorer, perhaps
                  indicating the presence of additional social
                  structure in the network that is not captured by the
                  random graph.},
  issn = {0027-8424},
  tags = {network-structure},
  pdf = {pdf/Newman_2002_Random_19658193.pdf},
  address = {Santa Fe Institute, 1399 Hyde Park Road, Santa Fe,
                  NM 87501, USA. mark@santafe.edu},
}
@article{Newton2002,
  author = {Newton, Craig John},
  year = {2002},
  title = {Beyond negative priming: Exploring the relationship
                  between continued inhibition and schizotypy},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {62},
  number = {10},
  pages = {4797},
  abstract = {Three experiments are reported using the 'Wegner
                  paradigm' to examine inhibitory differences in
                  normal versus schizotypal participants.  It is
                  argued that the Wegner paradigm may overcome some of
                  the limitations of the negative priming task, and
                  allow the study of continuation-of-inhibition
                  deficits in a more ecologically valid
                  context. Participants were asked to suppress
                  bodily-related thoughts in each of the experiments,
                  and differential responses were examined during
                  subsequent attending/expression
                  sessions. Experiments 1 and 2 provided evidence that
                  those scoring high on schizotypy possess
                  continuation-of-inhibition deficits that manifest
                  during a subsequent expression session. Experiment
                  3, undertaken to test for a potential
                  fatigue/practice effect, provided no indication of
                  differential inhibition, and the role of
                  practice/fatigue could not be ruled out as a cause
                  of the primary effects in experiments 1 and
                  2. Findings are discussed in terms of potential
                  advantages and problems of the Wegner paradigm, and
                  future directions for research are suggested that
                  examine multiple forms of inhibition across multiple
                  paradigms.},
  keywords = {negative priming continued inhibition schizotypy
                  Print},
  tags = {psychology;np},
}
@article{Nicodemus1998,
  author = {Nicodemus, Paul Douglas},
  year = {1998},
  title = {The role of the frontal lobes in the inhibitory
                  mechanisms of suppression},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {58},
  number = {10},
  pages = {5679},
  abstract = {The primary hypothesis examined in the present
                  experiment was that the executive functions of the
                  frontal lobe play a dominant role in negative
                  priming. Sixty-one young adults performed various
                  inhibitory reading tasks in which the participants
                  identified red-target letters while inhibiting
                  green-distractor letters. Participants also
                  performed various inhibitory tasks that are commonly
                  used in estimating performance levels of executive
                  functions. The results of the inhibitory reading
                  tasks showed that these participants demonstrated an
                  interference effect and a facilitation effect;
                  however, an overall negative priming effect was not
                  found. Although a negative priming effect was not
                  achieved, 39% of the participants displayed negative
                  priming effects, 34% demonstrated positive priming
                  effects, and the remaining participants (26%) showed
                  no priming effects. A mixed factorial analysis
                  showed that the negative primers completed both the
                  reading tasks and the inhibitory tasks that involved
                  executive functions in less time than either the
                  positive primers or the no primers. A multiple
                  regression analysis was computed in which the
                  negative priming effect served as the dependent
                  variable and the various inhibitory measurements for
                  executive functions served as the independent
                  variables. The results of this analysis indicated
                  that the executive functions of the frontal lobe do
                  not serve as good predictors for the negative
                  priming effect. Therefore, no support was found for
                  the hypothesis that the executive functions play a
                  significant role in the inhibitory process of
                  negative priming. Interpretation of these findings
                  was that the type of priming demonstrated by the
                  participant during an inhibitory task was the result
                  of their individual differences in the speed of
                  processing.},
  keywords = {The role of the frontal lobes in the inhibitory
                  mechanisms of suppression (executive functions,
                  negative priming)},
  tags = {psychology;np},
}
@article{Nigg2002,
  author = {Nigg, Joel T. and Butler, Karin M. and
                  Huang-Pollock, Cynthia L.  and Henderson, John M.},
  year = {2002},
  title = {Inhibitory processes in adults with persistent
                  childhood onset ADHD},
  journal = {Journal of Consulting \& Clinical Psychology},
  volume = {70},
  number = {1},
  pages = {153-157},
  abstract = {The theory that attention-deficit/hyperactivity
                  disorder (ADHD) stems from a deficit in an executive
                  behavioral inhibition process has been little
                  studied in adults, where the validity of ADHD is in
                  debate.  This study examined, in high-functioning
                  young adults with persistent ADHD and a control
                  group, 2 leading measures of inhibitory control: the
                  antisaccade task and the negative priming task. ADHD
                  adults showed weakened ability to effortfully stop a
                  reflexive or anticipated oculomotor response but had
                  normal ability to automatically suppress irrelevant
                  information. Results suggest that an inhibitory
                  deficit in ADHD is confined to effortful inhibition
                  of motor response, that antisaccade and negative
                  priming tasks index distinct inhibition systems, and
                  that persistence of ADHD symptoms into adulthood is
                  associated with persistence of executive motor
                  inhibition deficits.},
  keywords = {attention deficit hyperactivity disorder behavioral
                  inhibition motor response executive motor inhibition
                  deficits Print},
  tags = {psychology;np},
}
@article{Nolet2002,
  author = {Nolet, Michael Wayne},
  year = {2002},
  title = {Cognitive inhibition in schizophrenia: An
                  investigation of negative priming in chronic
                  schizophrenic patients and the first-degree
                  relatives of schizophrenic patients},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {63},
  number = {1},
  pages = {542},
  abstract = {Frith (1979) proposed that schizophrenia may result
                  from an impairment in cognitive inhibition. Previous
                  studies have demonstrated that those high in
                  schizotypy demonstrate impairments in cognitive
                  inhibition in negative priming tasks. Previous
                  research has also indicated that this reduced
                  negative priming can also be influenced by gender
                  and neuroleptic medication. Three different tasks, a
                  primed Stroop task, a spatial location task, and
                  pairs of discordant and concordant word primes were
                  used to gain a better understanding of the nature of
                  this deficit. Three different groups of eighteen
                  subjects each were utilized (a group of medicated
                  chronic schizophrenic patients, a normal control
                  group recruited from the community, and a normal
                  control group recruited from the first-degree
                  relatives of schizophrenic patients). Both control
                  groups were clinically screened to ensure that they
                  were free from symptoms of the schizophrenia
                  spectrum disorders and Axis I diagnoses. Consistent
                  with previous research, the community normal control
                  subjects did demonstrate negative priming on the
                  Stroop task while neither the patients nor the
                  normal control group consisting of first-degree
                  relatives showed significant priming on the task.
                  However, the three groups did not demonstrate
                  significantly different negative priming on this
                  task. This finding appears to lend support to the
                  fact that neuroleptic medication normalizes the
                  negative priming effect. On the location task, all
                  three groups demonstrated significant negative
                  priming that did not differ between the groups. The
                  interaction between gender, group and condition was
                  significant in terms of the errors made on the
                  location task, due apparently, to the fact that
                  female patients tended to make more errors in the
                  negative priming condition than male
                  patients. Normal community controls performed
                  differently than patients or their family members in
                  that the normal controls appear to show slight
                  facilitation in the Discordant condition relative to
                  the unrelated condition. Suggestions for future
                  research include the use of a sample individuals
                  clinically diagnosed with schizophrenia spectrum
                  personality disorders drawn from both the community
                  as well as from the first-degree relatives of
                  schizophrenic patients, as well as the use of a
                  symptomatic patient group.},
  keywords = {cognitive inhibition chronic schizophrenia negative
                  priming Print},
  tags = {psychology;np},
}
@article{Norman2005,
  author = {Kenneth A Norman and Ehren L Newman and Adler J
                  Perotte},
  year = {2005},
  title = {Methods for reducing interference in the
                  Complementary Learning Systems model: oscillating
                  inhibition and autonomous memory rehearsal.},
  journal = {Neural Netw},
  volume = {18},
  number = {9},
  pages = {1212--1228},
  doi = {10.1016/j.neunet.2005.08.010},
  abstract = {The stability-plasticity problem (i.e. how the brain
                  incorporates new information into its model of the
                  world, while at the same time preserving existing
                  knowledge) has been at the forefront of
                  computational memory research for several
                  decades. In this paper, we critically evaluate how
                  well the Complementary Learning Systems theory of
                  hippocampo-cortical interactions addresses the
                  stability-plasticity problem. We identify two major
                  challenges for the model: Finding a learning
                  algorithm for cortex and hippocampus that enacts
                  selective strengthening of weak memories, and
                  selective punishment of competing memories; and
                  preventing catastrophic forgetting in the case of
                  non-stationary environments (i.e. when items are
                  temporarily removed from the training set). We then
                  discuss potential solutions to these problems:
                  First, we describe a recently developed learning
                  algorithm that leverages neural oscillations to find
                  weak parts of memories (so they can be strengthened)
                  and strong competitors (so they can be punished),
                  and we show how this algorithm outperforms other
                  learning algorithms (CPCA Hebbian learning and
                  Leabra at memorizing overlapping patterns.  Second,
                  we describe how autonomous re-activation of memories
                  (separately in cortex and hippocampus) during REM
                  sleep, coupled with the oscillating learning
                  algorithm, can reduce the rate of forgetting of
                  input patterns that are no longer present in the
                  environment. We then present a simple demonstration
                  of how this process can prevent catastrophic
                  interference in an AB-AC learning paradigm.},
  keywords = {Algorithms; Animals; Hippocampus; Humans; Learning;
                  Memory; Models, Neurological; Models, Psychological;
                  Neocortex; Nerve Net; Neural Networks (Computer);
                  Neuronal Plasticity; Recognition (Psychology);
                  Sleep, REM; Theta Rhythm},
  institution = {Department of Psychology Princeton University, Green
                  Hall, Princeton, NJ 08544,
                  USA. knorman@princeton.edu},
}
