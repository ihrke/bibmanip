@article{Richards1999,
  author = {Richards, Anne},
  year = {1999},
  title = {The effects of cueing target location and response
                  mode on interference and negative priming using a
                  visual selection paradigm},
  journal = {Quarterly Journal of Experimental Psychology},
  volume = {52},
  number = {2},
  pages = {449-463},
  abstract = {Two experiments are reported that examine the
                  effects of cueing the location of a target in the
                  prime display on interference and subsequent
                  negative priming. The prime and probe displays
                  comprised 2 words, a target and a distractor. In the
                  prime display, the 2 words were either the same
                  (response compatible) or different (response
                  incompatible).  The target in the probe display was
                  unrelated to the prime distractor (control), the
                  same word as the distractor (ignored repetition), or
                  semantically related to the distractor (ignored
                  semantic repetition).  In Exp 1, cueing the location
                  of the prime target significantly reduced the
                  interference effect but not the subsequent identity
                  negative priming (NP) effect. In contrast, not
                  cueing the prime target resulted in the elimination
                  of the identity NP. There was no evidence of
                  semantic NP in this experiment. In Exp 2, where a
                  categorization response was required, significant
                  interference was obtained in the prime display that
                  was not influenced by cueing the location of the
                  target.  Although there was significant semantic NP,
                  identity NP failed to reach significance. The 2
                  experiments were analysed together, and findings are
                  discussed in relation to current models of negative
                  priming.},
  keywords = {effect of cueing target location \& response mode on
                  interference \& negative priming in visual selection
                  task Classification (Cognitive Process) Reaction
                  Time Visual Stimulation},
  tags = {psychology;np},
}
