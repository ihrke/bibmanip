@article{Salo1996,
  author = {Salo, Ruth and Robertson, Lynn C. and Nordahl,
                  Thomas E.},
  year = {1996},
  title = {Normal sustained effects of selective attention are
                  absent in schizophrenic patients withdrawn from
                  medication},
  journal = {Psychiatry Research},
  volume = {62},
  number = {2},
  pages = {121-130},
  abstract = {Compared the attentional performance of 12
                  medication-withdrawn schizophrenic outpatients with
                  that of 16 matched normal controls on a Stroop
                  negative priming task. This task allowed examination
                  of within- and between-trial attentional
                  effects. Compared with controls, patients showed
                  normal within-trial attentional effects as measured
                  by standard Stroop interference and
                  facilitation. Across trials, however, patients
                  exhibited reduced negative priming and in some cases
                  a complete reversal of sustained inhibitory
                  processes. Results suggest that a normal inhibitory
                  tag occurred during initial selection in the patient
                  group, but it did not influence a subsequent act of
                  selection, as was the case for the controls. Either
                  inhibition decayed at an abnormally fast rate in the
                  patient group, or a separate facilatory tag
                  dominated. In either case, priming effects linked to
                  attentional selection were abnormal in the patient
                  group.},
  keywords = {within vs between trial attentional effects on
                  Stroop negative priming task, medication withdrawn
                  outpatients with schizophrenia},
  tags = {psychology;np},
}
@article{Salo1997,
  author = {Salo, Ruth and Robertson, Lynn C. and Nordahl,
                  Thomas E. and Kraft, Louis W.},
  year = {1997},
  title = {The effects of antipsychotic medication on
                  sequential inhibitory processes},
  journal = {Journal of Abnormal Psychology},
  volume = {106},
  number = {4},
  pages = {639-643},
  abstract = {The authors used a Stroop negative priming paradigm
                  to examine the effects of antipsychotic medication
                  on selective attentional processes.  The performance
                  of 14 patients with schizophrenia who were withdrawn
                  from neuroleptic medication was compared with that
                  of 10 medicated patients and 16 matched
                  controls. Results demonstrated an increase in
                  negative priming to normal levels with neuroleptic
                  therapy. In contrast, within-trial interference and
                  facilitation effects appeared to be less sensitive
                  to medication therapy. The sustainment of inhibitory
                  processes over time may differentiate the inhibitory
                  mechanisms of the medication-withdrawn patients from
                  both the medicated patients and the matched
                  controls. The study of sequential inhibitory
                  processes and their response to neuroleptic
                  treatment could be important methods for
                  understanding the temporal parameters associated
                  with inhibition in schizophrenia.},
  keywords = {antipsychotic medication \& presentation of Stroop
                  stimuli, sequential inhibitory processes,
                  outpatients with schizophrenia Cognitive Processes
                  Outpatients},
  tags = {psychology;np},
}
@article{Salo2002,
  author = {Salo, Ruth and Henik, Avishai and Nordahl, Thomas
                  E. and Robertson, Lynn C.},
  year = {2002},
  title = {Immediate versus sustained processing in
                  schizophrenia},
  journal = {Journal of the International Neuropsychological
                  Society},
  volume = {8},
  number = {6},
  pages = {794-803},
  abstract = {A Stroop negative priming (NP) task was used to
                  assess immediate selective attention and priming in
                  schizophrenia and schizoaffective disorder.  Subject
                  groups were comprised of 72 state hospital
                  inpatients (11 males and 2 females, mean age 41.8
                  yrs), 11 outpatients (9 males and 3 females, mean
                  age 39.8 yrs), and 16 controls (10 males and 6
                  women, mean age 36.4 yrs). Compared with the control
                  group and the outpatients, inpatients failed to
                  exhibit NP, despite exhibiting equivalent Stroop RT
                  interference. Error rates did not differ
                  significantly between the 3 groups. Although
                  medication types and dosages were similar between
                  the 2 patient groups, length of illness was
                  significantly longer in the inpatients (19.8 years)
                  than in the outpatients. Positive symptom ratings
                  were also significantly higher in the inpatients.
                  The finding of reduced NP in the state hospital
                  patients appears to be related to severity of
                  symptomatology and chronicity of illness.},
  keywords = {Stroop negative priming task selective attention
                  priming schizophrenia schizoaffective disorder
                  inpatients outpatients symptom severity illness
                  chronicity Chronic Illness Hospitalized Patients
                  Outpatients Psychiatric Patients Severity
                  (Disorders) Symptoms Print Electronic},
  tags = {psychology;np},
}
@article{Salo2002a,
  author = {Salo, Ruth and Henik, Avishai and Nordahl, Thomas
                  E. and Robertson, Lynn C.},
  year = {2002},
  title = {Time course of inhibition and facilitation in
                  patients with schizophrenia},
  journal = {Cognitive Neuropsychiatry},
  volume = {7},
  number = {4},
  pages = {283-300},
  abstract = {Negative priming (NP) paradigms have been used to
                  understand the nature and time course of inhibitory
                  deficits in patients. A majority of these studies
                  have reported abnormal NP effects, with patients
                  showing faster reaction times on NP sequences
                  compared to the slowing displayed by controls. A
                  Stroop NP task was used to measure immediate
                  (within-trial) attentional processing as well as the
                  sustainment (between-trial) of these processes in 12
                  medicated schizophrenia inpatients (Ss; mean age
                  40.4 yrs) and 13 controls. 500 msec and 2000 msec
                  response stimulus intervals (RSIs) were
                  presented. Within-trial Stroop effects (interference
                  and facilitation) and between-trial priming effects
                  (negative and positive priming) were
                  measured. Clinical symptomatology and duration of
                  illness were also examined. At short RSIs of 500
                  msec, the Ss failed to exhibit NP. In contrast, the
                  control subjects exhibited normal NP at the short
                  RSI but this priming faded at the long 2000 msec RSI
                  and was no longer significant. The facilitatory
                  priming effects remained stable across time in both
                  groups. The results suggest that schizophrenia
                  patients are better able to inhibit irrelevant
                  stimuli that are immediate, but the sustainment of
                  inhibition across very short time intervals (500
                  msec) may fade.},
  keywords = {schizophrenia patients negative and positive priming
                  effects time course of inhibition or facilitation
                  interference Stroop Effect Print Electronic},
  tags = {psychology;np},
}
