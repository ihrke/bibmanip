  url = {http://dx.doi.org/10.1037/a0020404},
  abstract = {There is evidence that the efficiency of selective
                  attention depends on the availability of cognitive
                  control mechanisms as distractor processing has been
                  found to increase with high load on working memory
                  or dual task coordination (Lavie, Hirst, de Fockert,
                  \& Viding, 2004). We tested the prediction that
                  cognitive control load would also affect the
                  negative priming effect produced when a distractor
                  from 1 trial appears as a target on the next
                  trial. We measured priming on trials that involved
                  either high or low cognitive control load, and found
                  that under high control load, negative priming was
                  eliminated, and could even be reversed to positive
                  priming, suggesting that the negative priming effect
                  depends on the availability of cognitive control
                  resources.},
  issn = {1939-1277},
  tags = {psychology;np},
  pdf = {pdf/De_Fockert_2010_No_19684860.pdf},
}

