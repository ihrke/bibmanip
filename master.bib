@article{Conway1997,
  author = {Conway, Andrew Robert Allan},
  year = {1997},
  title = {An investigation of selective attention: A test of
                  the episodic retrieval theory of negative priming},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {57},
  number = {7},
  pages = {4754},
  abstract = {Neill and his colleagues (Neill \& Valdes, 1992,
                  Neill, Valdes, Terry, \& Gorfein, 1992) have
                  recently proposed an episodic retrieval theory of
                  negative priming. There are two main findings that
                  support an episodic retrieval theory. The first is
                  that negative priming is affected by the RSI
                  (response-to-stimulus-interval) before the prime
                  display. The second is that negative priming appears
                  to decay when RSI is manipulated within-groups but
                  not when RSI is manipulated between-groups. Four
                  experiments are reported that question the
                  reliability of these findings, and subsequently, the
                  validity of episodic retrieval theory. It is argued
                  that in most experimental contexts, the negative
                  priming effect is due to the existence of an
                  inhibitory mechanism in selective attention.},
  keywords = {selective attention \& episodic retrieval theory of
                  negative priming},
  tags = {psychology;np},
}
@article{Conway1999,
  author = {Conway, Andrew R. A.},
  year = {1999},
  title = {The time-course of negative priming: Little evidence
                  for episodic trace retrieval},
  journal = {Memory \& Cognition},
  volume = {27},
  number = {4},
  pages = {575-583},
  abstract = {An episodic trace retrieval (ETR) explanation of
                  negative priming (NP) predicts that the NP effect
                  should be sensitive to the timing of delays between
                  trials (W. T. Neill and L. A. Valdes, see record
                  1992-30037-001; Neill et al, see record
                  1993-04404-001). Specifically, according to ETR, (1)
                  NP is affected by the response-stimulus interval
                  (RSI) before the prime display, and (2) NP decays
                  when RSI is manipulated within groups but not when
                  RSI is manipulated between groups. Two localization
                  tasks and two identification tasks are reported that
                  question the reliability of these
                  findings. Undergraduates from the Georgia Institute
                  of Technology served as Ss. The results suggest that
                  there is little in the time-course literature that
                  uniquely supports the ETR theory of NP. Instead, the
                  results seem more compatible with either a
                  dual-mechanism account (M. J. Kane; see record
                  1997-04590-004) or an integrative approach that
                  incorporates both memory and attention processes
                  (B. Milliken et al; see record 1994-35938-001).},
  keywords = {presentation of displays, episodic trace retrieval
                  \& time course of negative priming, college students
                  Stimulus Intervals Visual Displays},
  tags = {psychology;np},
}
@article{Conway1999a,
  author = {Conway, Andrew R. A. and Tuholski, Stephen W. and
                  Shisler, Rebecca J. and Engle, Randall W.},
  year = {1999},
  title = {The effect of memory load on negative priming: An
                  individual differences investigation},
  journal = {Memory \& Cognition},
  volume = {27},
  number = {6},
  pages = {1042-1050},
  abstract = {The effect of a verbal (Exp 1; 80 undergraduate Ss)
                  and a nonverbal (Exp 2; 75 undergraduate Ss) memory
                  load on negative priming was investigated by
                  employing a concurrent memory task with a letter
                  naming task. Across both experiments, negative
                  priming was reliable only under conditions of zero
                  memory load, suggesting that the processes that
                  contribute to negative priming are resource
                  demanding and dependent on a domain-free resource
                  pool. Individual differences in negative priming
                  were observed, such that high working memory
                  capacity Ss showed reliable negative priming whereas
                  low working memory capacity Ss did not. The results
                  suggest that the negative priming effect results
                  from allocation of controlled attention and that
                  individual differences in working memory capacity
                  correspond to the ability to efficiently handle
                  irrelevant information.},
  keywords = {negative priming effect, verbal vs nonverbal
                  cognitive load \& working memory capacity during
                  memory \& letter naming tasks, college students
                  Naming},
  tags = {psychology;np},
}
