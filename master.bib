@article{Zimmermann1999,
  author = {Zimmermann, Lyn Angier},
  year = {1999},
  title = {The effect of instruction on the processing of
                  irrelevant information: A schema approach to
                  explaining negative priming},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {59},
  number = {8},
  pages = {4505},
  abstract = {A schema approach to explaining processing of
                  distractors in selective attention tasks was
                  presented and was tested in three negative priming
                  experiments. It was proposed that the extent and
                  type of processing that a distractors receives
                  depends; in part, on information derived from the
                  instructions. The task was an intradisplay matching
                  task in which the prime distractors were presented
                  as probe targets for one-fourth of the trials;
                  response and response time were recorded.
                  Instruction was manipulated so that participants
                  either read instructions in which distractors were
                  defined and participants were told to ignore the
                  distractors (explicit instructions) or read
                  instructions that did not contain this information
                  (implicit instructions). Several other variables,
                  such as target position and stimulus salience, were
                  manipulated. The results generally were consistent
                  with the hypothesis that instruction (top-down
                  processing) and distractor salience (bottom-up
                  processing) would interact. When distractors were of
                  medium salience, explicit instructions produced
                  negative priming effects for two-thirds of the
                  participants, and implicit instructions produced
                  positive priming effects for two-thirds of the
                  participants. There was no difference in the number
                  of participants who showed a negative priming effect
                  and the number who showed a positive priming effect
                  when distractors were highly salient. Based on the
                  finding of an interaction of instruction and
                  distractor salience, it was proposed that the
                  mechanism responsible for negative priming may be
                  directed inhibition.},
  keywords = {explicit vs implicit instruction \& distractor
                  salience, extent \& type of processing of irrelevant
                  information, human Ss Stimulus Salience},
  tags = {psychology;np},
}
@article{Zmigrod2009Intermodal,
  author = {Zmigrod, Sharon and Spap\'{e}, Michiel and Hommel,
                  Bernhard},
  year = {2009},
  title = {Intermodal event files: integrating features across
                  vision, audition, taction, and action.},
  journal = {Psychological research},
  volume = {73},
  number = {5},
  pages = {674--684},
  month = {sep},
  day = {1},
  doi = {10.1007/s00426-008-0163-5},
  url = {http://dx.doi.org/10.1007/s00426-008-0163-5},
  abstract = {Understanding how the human brain integrates
                  features of perceived events calls for the
                  examination of binding processes within and across
                  different modalities and domains. Recent studies of
                  feature-repetition effects have demonstrated
                  interactions between shape, color, and location in
                  the visual modality and between pitch, loudness, and
                  location in the auditory modality: repeating one
                  feature is beneficial if other features are also
                  repeated, but detrimental if not. These
                  partial-repetition costs suggest that co-occurring
                  features are spontaneously bound into temporary
                  event files. Here, we investigated whether these
                  observations can be extended to features from
                  different sensory modalities, combining visual and
                  auditory features in Experiment 1 and auditory and
                  tactile features in Experiment 2. The same types of
                  interactions, as for unimodal feature combinations,
                  were obtained including interactions between
                  stimulus and response features. However, the size of
                  the interactions varied with the particular
                  combination of features, suggesting that the
                  salience of features and the temporal overlap
                  between feature-code activations plays a mediating
                  role.},
  issn = {1430-2772},
  tags = {binding;np;psychology},
  pdf = {pdf/Zmigrod_2009_Intermodal_19690367.pdf},
  pmcid = {PMC2708333},
}
