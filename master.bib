}
@article{Park1995,
  author = {Park, Jooyong},
  year = {1995},
  title = {Negative priming: Evidence for inhibition in
                  attentional selection?},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {55},
  number = {7},
  pages = {3043},
  abstract = {This thesis addresses the question of whether
                  inhibition plays any important role in attentional
                  selection. I focus on the negative priming (NP)
                  paradigm, which has been widely regarded as evidence
                  supporting inhibition. NP refers to the slowing of
                  response to the target in the nth display (the
                  "probe") when the target shares some properties with
                  the distractor in the (n-1)th display (the "prime").
                  According to Tipper and his colleagues (e.g.,
                  Allport, Tipper, \& Chmiel, 1985; Tipper, Brehaut,
                  \& Driver, 1990), the function of attention is to
                  select a target from among distractors, and this
                  selection is implemented by inhibiting
                  distractors. They argue that NP is a direct result
                  of this distractor inhibition. In Chapter II,
                  contrary to Tipper's claim, I show that selection
                  for a target is neither necessary nor sufficient for
                  NP in the spatial task developed by Tipper et
                  al. (1990). An alternative account of NP, the
                  "mismatching hypothesis", is proposed and then
                  supported. According to this hypothesis, NP is due
                  to the cost of changing the symbols which are bound
                  to a given location. In Chapter III, Tipper's more
                  recent evidence supporting the distractor inhibition
                  hypothesis (e.g., Milliken, Weaver, \& Tipper, 1994;
                  Tipper, Weaver, \& Houghton, 1994) is evaluated. In
                  their new NP task, Tipper and his colleagues found a
                  delay in a condition where the probe target was
                  exactly the same as the prime distractor.  Since
                  there was no mismatching, inhibition was proposed to
                  be the intervening mechanism of this delay. In
                  Chapter III, I proposed two alternative accounts of
                  the NP found in Tipper's new experiments.  According
                  to the cue-and-target "group mismatching"
                  hypothesis, the mismatching processes which occurred
                  between the prime and probe displays operated on the
                  entire cue-and-target group rather than on an
                  individual letter. According to the "attention
                  capture" hypothesis, the NP effect is due not to
                  slowing of response in the critical NP conditions
                  but rather to speedi},
  keywords = {Negative priming: Evidence for inhibition in
                  attentional selection?  (selective attention)},
  tags = {psychology;np},
}
@article{Park1997,
  author = {Park, Joo-yong},
  year = {1997},
  title = {Selection and negative priming effect in an
                  identification task},
  journal = {Korean Journal of Experimental \& Cognitive
                  Psychology},
  volume = {9},
  number = {1},
  pages = {1-15},
  abstract = {Studied the negative priming effect as evidence for
