@article{Fisher1998,
  author = {Fisher, Julia Anne},
  year = {1998},
  title = {Mental lexicons of children: Multiple routes to
                  lexical access with implications for reading},
  journal = {Dissertation Abstracts International Section A:
                  Humanities \& Social Sciences},
  volume = {59},
  number = {3},
  pages = {0803},
  abstract = {Eighty-seven students from the fourth, fifth, and
                  sixth grades performed a lexical decision task in
                  order to study what role developmental and
                  linguistic factors play in patterns of lexical
                  access. Stimuli in both auditory and visual
                  conditions were primed to investigate whether
                  students showed greater priming effects with
                  semantic, syntactic, phonological, or (in the visual
                  condition) orthographic priming.  Significant
                  effects of priming were observed for all prime
                  types.  In some cases negative priming was
                  observed. Scores were entered into a statistical
                  factor analytic procedure which suggested that, in
                  each condition, only two underlying factors
                  accounted for the variance between the
                  variables. The four factor scores for the students
                  were reduced to single values in a statistical
                  cluster analytic procedure and five distinct
                  subgroups of students were identified in terms of
                  overall performance on the study materials. Group
                  membership was examined relative to students'
                  performance on a number of standardized measures of
                  lexical knowledge, language, and reading. Results
                  were discussed relative to models of lexical access
                  and relative to models of reading which presume a
                  consistent pattern for lexical access processing for
                  all persons. The study is intended as a contribution
                  to the current body of research investigating
                  individual differences in psycholinguistic
                  processing.},
  keywords = {developmental \& linguistic factors \& stimuli
                  priming, patterns of lexical access, 4th vs 5th vs
                  6th graders, implications for reading},
  tags = {psychology;np},
}
