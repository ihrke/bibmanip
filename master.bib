@article{Girondi1997,
  author = {Girondi, Annette M.},
  year = {1997},
  title = {The self-regulation of affect: Application of the
                  negative priming paradigm},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {57},
  number = {8},
  pages = {5374},
  abstract = {The purpose of this study is to examine the ability
                  of individuals to inhibit "activated but
                  non-goal-relevant" (May, Kane, \& Hasher, 1995,
                  p. 35) emotional information. This study is
                  comprised of three experimental tasks designed to
                  explore this ability at three cognitive processing
                  levels: goal, categorical, and nonconscious. For the
                  goal and categorical levels, the negative priming
                  paradigm is used to examine the self-regulation of
                  affect for four emotions: Happy, Sad, Surprise, and
                  Anger. For the nonconscious level, an activation and
                  suppression paradigm is applied for these same
                  emotion categories.  The activation and suppression
                  paradigm yields limited support for the ability to
                  suppress emotions (emotion-expressing faces) at the
                  nonconscious level. No support was found for the
                  suppression or activation of emotions
                  (emotion-expressing words) at the categorical level.
                  Application of the negative priming paradigm at the
                  goal level provides limited support for the
                  suppression of goal level emotional stimuli
                  (emotion-expressing faces). The most impressive
                  evidence is the ability of subjects, at the goal
                  level, to express an emotion-related experience
                  based on a target stimuli while not expressing a
                  distractor emotion which they were required to
                  ignore. Also examined were the influence of
                  individual difference variables: Action versus State
                  Orientation, Depression, and Self-Monitoring on
                  suppression ability; overall, no significant
                  relationship exists between the individual
                  difference variables and suppression
                  ability. Suggestions for future research are
                  offered, including the use of a broader range of
                  emotional stimuli.},
  keywords = {self-regulation of emotions in three cognitive
                  processing levels, human Ss, application of negative
                  priming paradigm},
  tags = {psychology;np},
}
@article{Glenberg2003,
  author = {Glenberg, Arthur M. and Robertson, David A. and
                  Kaschak, Michael P. and Malter, Alan J.},
  year = {2003},
  title = {Embodied meaning and negative priming},
  journal = {Behavioral \& Brain Sciences},
  volume = {26},
  number = {5},
  pages = {644-648},
  abstract = {Responds to comments made by M. Kurthen et al (see
                  record 2004-12881-004), E. Neumann (see record
                  2004-12881-005), and E. Wright (see record
                  2004-12881-006) on the original article by
                  A. M. Glenberg (see record 1997-04873-001) that
                  discussed what memory is for. The current authors
                  assert that standard models of cognition are built
                  from abstract, amodal, arbitrary symbols, and the
                  meanings of those symbols are given solely by their
                  interrelations. The target article argues that these
                  models must be inadequate because meaning cannot
                  arise from relations among abstract symbols. For
                  cognitive representations to be meaningful they
                  must, at the least, be grounded; but abstract
                  symbols are difficult, if not impossible, to
                  ground. As an alternative, the target article
                  developed a framework in which representations are
                  grounded in perception and action, and hence are
                  embodied. Recent work (Glenberg \& Robertson 1999;
                  2000; Glenberg \& Kaschak 2002; Kaschak \& Glenberg
                  2000) extends this framework to language.},
  keywords = {embodied meaning negative priming memory models of
                  cognition symbols grounding perception action
                  language Cognition Language Meaning Models
                  Perception Priming Symbolism Print Electronic},
  tags = {psychology;np},
}
