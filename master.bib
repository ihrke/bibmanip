  year = {1996},
  title = {The relationship between psychoticism,
                  trait-creativity and the attentional mechanism of
                  cognitive inhibition},
  journal = {Personality \& Individual Differences},
  volume = {21},
  number = {1},
  pages = {143-153},
  abstract = {Examined whether there is a link among psychoticism
                  (P), cognitive traits associated with creativity
                  (i.e., Divergent Thinking [DT]), and the attentional
                  mechanism of cognitive inhibition. 37 adults were
                  tested using the Eysenck Personality Questionnaire
                  (EPQ), the Wallach-Kogan Divergent Thinking tests,
                  and a "negative priming" experimental task as a
                  measure of cognitive inhibition. The correlations of
                  P-scores with creativity scores were positive and
                  mostly significant, indicating that Ss who scored
                  highly on P also produced a larger number of
                  responses and gave more unique answers on the DT
                  items.  High-P scorers showed a significantly
                  reduced negative priming effect (i.e., reduced
                  ability to inhibit stimulation from the environment)
                  compared with low-P scorers. Ss who scored highly on
                  the DT test showed a nonsignificantly smaller
                  negative priming effect than did low-creativity
                  scorers.},
  keywords = {psychoticism \& creativity \& cognitive inhibition,
                  19-42 yr olds},
  tags = {psychology;np},
}
