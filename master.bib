@article{ODekirk1999,
  author = {O'Dekirk, James Mark},
  year = {1999},
  title = {Visual selective attention in persons with mental
                  retardation: Identity-based versus location-based
                  mechanisms},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {60},
  number = {3},
  pages = {1324},
  abstract = {This study directly compared the inhibition of
                  return, negative priming for identity, and negative
                  priming for location effects in adolescents with and
                  without mental retardation. It was suggested that
                  negative priming for location and inhibition of
                  return result from the operation of a more
                  involuntary process of inhibition than negative
                  priming for identity. The adolescents with mental
                  retardation exhibited negative priming when
                  selecting stimuli based on location, but did not
                  exhibit negative priming when selecting stimuli
                  based on identity. Adolescents without mental
                  retardation exhibited negative priming under both
                  identity-based and location-based
                  conditions. Adolescents with and without mental
                  retardation exhibited an inhibition of return effect
                  that did not differ for the two groups. The only
                  significant correlation between the three tasks
                  occurred for the inhibition of return effect at a
                  300 msec SOA and the identity-based negative priming
                  effect.  Although little evidence was found to
                  suggest a relation between the two location-based
                  tasks, several arguments were made to explain why
                  the expected results were not obtained. These
                  explanations focused on the task differences between
                  inhibition of return studies on infants and
                  inhibition of return studies on adults, whether the
                  inhibition of return task adequately captured the
                  participants' attention, and trends in the data that
                  occurred in the direction that was originally
                  predicted.},
  keywords = {identity-based vs location-based negative priming \&
                  inhibition of return, adolescents with vs without
                  mental retardation},
  tags = {psychology;np},
}
@article{ORiordan2000,
  author = {O'Riordan, Michelle},
  year = {2000},
  title = {Superior modulation of activation levels of stimulus
                  representations does not underlie superior
                  discrimination in autism},
  journal = {Cognition},
  volume = {77},
  number = {2},
  pages = {81-96},
  abstract = {The performance of children with and without autism
                  was compared in object-based positive and negative
                  priming tasks within a visual search procedure. Exp
                  1 investigated inhibitory processing in 11 Ss (aged
                  8-11 yrs) with autism and 12 control Ss (aged
                  7.8-9.8 yrs).  Ss were tested on 2 conjunctive
                  visual search tasks. Exp 2 investigated whether
                  object-based excitation contributes to visual search
                  task performance and whether a stronger object-based
                  excitation mechanism operates in autism relative to
                  normal development. 12 children with autism (aged
                  7.5-10.6 yrs) and 12 control Ss (aged 7.5-9.7 yrs)
                  completed 3 conjunctive visual search tasks. The
                  results show that object-based positive and negative
                  priming effects were found in both groups of
                  children. This result provides evidence for the
                  activation of object-based representations during
                  visual search task performance and supports the
                  notion that both excitatory and inhibitory guidance
                  mechanisms are involved in target location in visual
                  search. The children with autism were overall better
                  than the typically developing children at visual
                  search, thus replicating demonstrations of superior
                  discrimination in autism. There was no difference
                  between the magnitude of the positive nor the
                  negative priming effects of the groups.},
  keywords = {performance on object-based positive \& negative
                  priming tasks within visual search procedure,
                  inhibitory \& excitatory processing, 7.5-11 yr olds
                  with autism},
  tags = {psychology;np},
}
@article{Oka1987,
  author = {Oka, Naoki},
  year = {1987},
  title = {Priming effects and integration of sentences},
  journal = {Japanese Journal of Psychonomic Science},
  volume = {5},
  number = {2},
  pages = {71-76},
  abstract = {Studied factors influencing sentence integration in
                  2 experiments.  Human subjects: 20 normal Japanese
                  adolescents and adults. A sentence-recognition
