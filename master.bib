@article{Claridge1992,
  author = {Claridge, G. S. and Clark, K. H. and Beech, A. R.},
  year = {1992},
  title = {Lateralization of the "negative priming" effect:
                  Relationships with schizotypy and with gender},
  journal = {British Journal of Psychology},
  volume = {83},
  number = {1},
  pages = {13-23},
  abstract = {49 male and 52 female right-handed adults completed
                  a divided visual field version of a negative priming
                  (NPR) task of selective attention to examine whether
                  performance would be related to schizotypal
                  personality traits. In lateralized form the NPR task
                  produced a significant interaction between
                  schizotypy and visual field, and an influence of
                  gender.  Among males, high schizotypes showed an
                  absence of NPR after primes presented to the left
                  visual field. However, right field primes produced
                  slightly more NPR than in low schizotypes, who
                  showed strong NPR effects under both
                  conditions. Among females, there was less NPR
                  overall, no difference between visual fields, and no
                  association with schizotypy. Results are interpreted
                  in light of evidence from other hemisphere research
                  about lateralization of language in schizotypy and
                  schizophrenia.},
  tags = {psychology;np},
}
@article{Claridge1996,
  author = {Claridge, Gordon and Beech, Anthony},
  year = {1996},
  title = {Schizotypy and lateralised negative priming in
                  schizophrenics' and neurotics' relatives},
  journal = {Personality \& Individual Differences},
  volume = {20},
  number = {2},
  pages = {193-199},
  abstract = {Compared 44 schizophrenics' relatives with 24
                  neurotics' relatives on a range of schizotypy scales
                  and a hemisphere information processing (negative
                  priming) task. On most of the schizotypy scales
                  schizophrenics' relatives had lower scores than
                  neurotics' relatives, and also as compared with the
                  normative data for the questionnaires used. The
                  experimental task showed no significant differences
                  when Ss were compared according to relatives'
                  status. High schizotypes showed a lack of visual
