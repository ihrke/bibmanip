@article{Hasher1991,
  author = {Hasher, Lynn and Stoltzfus, Ellen R. and Zacks, Rose
                  T. and Rypma, Bart},
  year = {1991},
  title = {Age and inhibition},
  journal = {JEPLMC},
  volume = {17},
  number = {1},
  pages = {163-169},
  abstract = {Two experiments assess adult age differences in the
                  extent of inhibition or negative priming generated
                  in a selective-attention task. Younger adults
                  consistently demonstrated negative priming effects;
                  they were slower to name a letter on a current trial
                  that had served as a distractor on the previous
                  trial relative to one that had not occurred on the
                  previous trial. Whether or not inhibition dissipated
                  when the response-to-stimulus interval was
                  lengthened from 500 msec in Exp 1 to 1,200 msec in
                  Exp 2 depended on whether young Ss were aware of the
                  patterns across trial types. Older adults did not
                  show inhibition at either interval. The age effects
                  are interpreted within L. Hasher and R. T. Zacks's
                  (1988) framework, which proposes inhibition as a
                  central mechanism determining the contents of
                  working memory and consequently influencing a wide
                  array of cognitive functions.},
  keywords = {age differences, inhibition generated in selective
                  attention task, younger vs elderly adults},
  tags = {psychology;np},
}
@article{Hasher1996,
  author = {Hasher, Lynn and Zacks, Rose T. and Stoltzfus, Ellen
                  R. and Kane, Michael J. and et al.},
  year = {1996},
  title = {On the time course of negative priming: Another
                  look},
  journal = {Psychonomic Bulletin \& Review},
  volume = {3},
  number = {2},
  pages = {231-237},
  abstract = {In 2 experiments, the pattern of persistence of
                  negative priming effects across delay intervals of
                  500 and 2,500 msec was assessed using a within-Ss,
                  random sequencing of delays. W. T. Neill and
                  L. A. Valdes (see record 1992-30037-001) and
                  W. T. Neill et al (1992) have argued that a within-S
                  experimental design is required for decay of
                  negative priming to be seen, in contrast to results
                  reported elsewhere (e.g., S. P. Tipper et al, 1991)
                  showing stable negative priming effects across
                  delays. In neither experiment was substantial
                  evidence of decay detected, raising questions for
                  the notion that suppression necessarily declines
                  across brief temporal intervals and for the
                  assertion that episodic retrieval is the sole source
                  of negative priming.},
  keywords = {response-to-stimulus delay interval, decay of
