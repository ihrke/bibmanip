@article{Galen1997,
  author = {Galen, Luke W.},
  year = {1997},
  title = {Accessing alcohol expectancy networks through
                  semantic priming},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {57},
  number = {12},
  pages = {7724},
  abstract = {Cognitive theories of alcohol use have suggested
                  that information pertinent to an individuals'
                  experience with alcohol is acquired and stored in a
                  semantic network of expectancies regarding the
                  effects of alcohol. Semantic priming using a lexical
                  decision task has been used to access memory
                  processes within a semantic network. The present
                  study involved screening 40 individuals with heavy
                  current drinking and 40 with minimal alcohol use and
                  presenting both groups with alcohol related primes
                  (e.g., "beer"), followed by alcohol expectancy
                  targets (e.g., "sociable") or non-word derivatives
                  (e.g., "hapby") in a lexical decision task. Subjects
                  were divided into groups with prime presentation
                  times that were either short (150 ms) or long (650
                  ms) to differentiate automatic from strategic
                  processing. Analysis of variance did not detect a
                  group difference in priming for expectancies primed
                  by alcohol words; instead, there was a trend towards
                  negative priming, or inhibition across groups. Free
                  recall data indicated that alcohol primes were more
                  salient and were recalled more by both groups,
                  especially those the longer exposure groups. These
                  results suggest that alcohol expectancies stored in
                  memory may require more than simply lexical
                  information in order to exhibit positive priming.},
  keywords = {Accessing alcohol expectancy networks through
                  semantic priming (substance use) Alcohol Abuse},
  tags = {psychology;np},
}
