@article{Anderson1997a,
  author = {Anderson, Richard B. and Garavan, Hugh and Rivardo,
                  Mark G. and Chadwick, Randall},
  year = {1997},
  title = {Inhibitory consequences of memory selection},
  journal = {Acta Psychologica},
  volume = {96},
  number = {3},
  pages = {155-166},
  abstract = {When Ss select a prime from a visual display while
                  leaving a distractor prime unselected, response time
                  or response accuracy to a subsequent probe may be
                  impeded if the distractor prime and probe are
                  identical, or if they are related to one
                  another. This phenomenon, negative priming (NP), has
                  obvious implications for understanding perceptual
                  selection. However, it is not known whether NP
                  results from other kinds of selection processes. Two
                  experiments (n = 5, n = 15, respectively) were
                  designed to investigate whether NP occurs when
                  primes are selected from working memory rather than
                  from a visual display. In both experiments, the Ss
                  memorized two primes, selected one prime for further
                  processing, and classified the contents of a probe
                  display. Significant NP occurred in both
                  experiments. In the second experiment, however, NP
                  occurred only under easy-selection conditions; the
                  effect was reversed under difficult-selection
                  conditions. The findings indicate a role for NP in
                  memory processing, but contrast with the results
                  from perceptual selection studies showing greater NP
                  under difficult-selection than under easy-selection
                  conditions. The present findings suggest a complex
                  and perhaps strategy-dependent relationship between
                  memory selection difficulty and NP.},
  tags = {psychology;np},
}
