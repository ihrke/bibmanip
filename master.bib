@article{Li1996,
  author = {Li, Yalin},
  year = {1996},
  title = {A study on negative priming effect in selective
                  attention},
  journal = {Acta Psychologica Sinica},
  volume = {28},
  number = {3},
  pages = {245-251},
  abstract = {Studied the negative priming effect of distractors
                  in selective attention with Arabic or Chinese digit
                  naming tasks. Ss were 17 undergraduate students
                  (mean age: 21.4 yrs) (Exp 1) and 18 undergraduate
                  students (mean age: 21.6 yrs) (Exp 2) from the
                  department of psychology of South China Normal
                  University, Guangzhou, China. The negative priming
                  effect of distractors was tested in stressing
                  accuracy (Exp 1) and in stressing reaction speed
                  (Exp 2) manipulated by instructions.  The naming
                  tasks included controlled serials (with
                  disassociated targets and distractors) and
                  distractor serials (with targets changed from
                  distractors in a previous serial) of Arabic or
                  Chinese digits (1-9). The correct reaction time (RT)
                  and error rate in naming with multivariate analysis
                  of variance (MANOVA) and the differentiations
                  between naming controlled and distractor serials
                  were computed. The results are that a negative
                  priming effect of distractors was found in stressing
                  the accuracy and a positive priming effect of
                  distractors was found in stressing the speed. The
                  results suggest that the activation state of
                  distractors is a critical factor to the nature of
                  priming effect, supporting the spreading inhibition
                  of distractors, inhibition-based fan effect, and the
                  late-selection theory.},
  keywords = {negative priming effect selective attention positive
                  priming distractors inhibition-based fan effects
                  accuracy reaction speed theory of late selection
                  digit naming Naming Reaction Time Print},
  tags = {psychology;np},
}
@article{Li2001,
  author = {Li, S.C. and Lindenberger, U. and Sikstr{\"o}m, S.},
  year = {2001},
  title = {Aging cognition: from neuromodulation to
                  representation.},
  journal = {Trends Cogn Sci},
  volume = {5},
  number = {11},
  pages = {479--486},
  abstract = {Basic cognitive functions, such as the abilities to
                  activate, represent, maintain, focus and process
                  information, decline with age. A paradigm shift
                  towards cross-level conceptions is needed in order
                  to obtain an integrative understanding of cognitive
                  aging phenomena that cuts across neural,
                  information-processing, and behavioral levels. We
                  review empirical data at these different levels, and
                  computational theories proposed to enable their
                  integration. A theoretical link is highlighted,
                  relating deficient neuromodulation with noisy
                  information processing, which might result in less
                  distinctive cortical representations. These less
                  distinctive representations might be implicated in
                  working memory and attentional functions that
                  underlie the behavioral manifestations of cognitive
                  aging deficits.},
  keywords = {11684480},
}
@article{Li2003,
  author = {Shu-Chen Li},
  year = {2003},
  title = {Biocultural orchestration of developmental
                  plasticity across levels: the interplay of biology
                  and culture in shaping the mind and behavior across
                  the life span.},
  journal = {Psychol Bull},
  volume = {129},
  number = {2},
  pages = {171--194},
  abstract = {The author reviews reemerging coconstructive
                  conceptions of development and recent empirical
                  findings of developmental plasticity at different
                  levels spanning several fields of developmental and
                  life sciences.  A cross-level dynamic biocultural
                  coconstructive framework is endorsed to understand
                  cognitive and behavioral development across the life
                  span. This framework integrates main conceptions of
                  earlier views into a unifying frame, viewing the
                  dynamics of life span development as occurring
                  simultaneously within different time scales (i.e.,
                  moment-to-moment microgenesis, life span ontogeny,
                  and human phylogeny) and encompassing multiple
                  levels (i.e., neurobiological, cognitive,
                  behavioral, and sociocultural). Viewed through this
                  metatheoretical framework, new insights of potential
                  interfaces for reciprocal cultural and experiential
                  influences to be integrated with behavioral genetics
                  and cognitive neuroscience research can be more
                  easily prescribed.},
  keywords = {Aging, Cultural Characteristics, Ethnic Groups,
                  Evolution, Humans, Personality Development,
                  Phylogeny, Social Behavior, Social Environment,
                  12696838},
  comment = {{\bf J{\"o}rg: } \\ {\bf Matthias: } \\ {\bf Hecke:
                  }},
}
