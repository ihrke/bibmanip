@incollection{Khurana2000Neural,
  author = {Khurana, Beena},
  editor = {Metzinger, Thomas},
  year = {2000},
  title = {Face representation without conscious processing},
  booktitle = {Neural correlates of consciousness: Empirical and
                  conceptual questions},
  publisher = {The MIT Press},
  pages = {171-187},
  abstract = {To gauge the ability of the visual system to
                  represent token faces without focused attention,
                  observers compared target faces in the presence of
                  distractor faces. Five experiments were conducted
                  using a total of 72 undergraduate students. Evidence
                  was found for the representation and inhibition of
                  never-before-seen distractor faces, thus
                  establishing that the negative priming paradigm can
                  be used for the study of face perception without
                  conscious processing. It is proposed that the visual
                  system indeed represents unfamiliar faces without
                  focused attention, but blocks them only if they vie
                  with targets for the control of action.},
  keywords = {face representation without consciousness,
                  undergraduates},
  tags = {psychology;face-recognition},
  address = {Cambridge, MA},
}
@article{Khurana2000Not,
  author = {Khurana, Beena and Smith, W. C. and Baker, M. T.},
  year = {2000},
  title = {Not to be and then to be: Visual representation of
                  ignored unfamiliar faces},
  journal = {JEPHPP},
  volume = {26},
  number = {1},
  pages = {246-263},
  abstract = {Negative priming, the increase in response time
                  and/or errors to targets previously encountered as
                  distractors, is explained by inhibitory mechanisms
                  that block the access of distractor representations
                  to response systems. The processing of unfamiliar
                  human faces was investigated using negative
                  priming. Observers viewed a row of faces to decide
                  whether 2 target faces were the same or
                  different. Response latencies were longer when 1 or
                  both targets had appeared as distractors on the
                  immediately preceding trial--evidence that
                  never-before seen faces are represented and require
                  inhibition. Response latencies were shorter when
                  face targets had appeared as distractors, either
                  corrupted with high-frequency noise or contrast
                  inverted-evidence that representations are
                  facilitated. Finally, response latencies remained
                  unaltered when face targets had appeared as
                  upside-down distractors--evidence that not all
                  distractor representations afford response
                  priming. The visual system indeed represents ignored
                  unfamiliar faces, but blocks these representations
                  only if they vie with targets for the control of
                  action.},
  keywords = {visual negative priming paradigm, automaticity in
                  perceptual coding \& processing of attended to vs
                  ignored unfamiliar faces, college students
                  Familiarity Visual Stimulation},
  tags = {psychology;face-recognition},
}
@article{Kieley1997,
  author = {Kieley, James M. and Hartley, Alan A.},
  year = {1997},
  title = {Age-related equivalence of identity suppression in
                  the Stroop color-word task},
  journal = {Psychology \& Aging},
  volume = {12},
  number = {1},
  pages = {22-29},
  abstract = {Previous failures to find reliable identity
                  suppression (identity negative priming) in older
                  adults have led to conclusions that older adults
                  suffer from an impairment in the inhibitory
                  component of selective attention. Here, 2
                  experiments using the Stroop procedure found
                  identity suppression in older adults that was both
                  reliable and equivalent to that in younger
                  adults. Experiment 1 with repeated target colors
                  produced correlations consistent with an episodic
                  retrieval explanation of identity suppression,
                  Experiment 2 without repeated targets produced
                  correlations inconsistent with the episodic
                  retrieval interpretation.  These patterns were found
                  for both younger and older adults. No evidence was
                  found for reduced identity suppression that would be
                  consistent with a general inhibitory impairment in
                  older adults.},
  keywords = {identity suppression effect in Stroop color-word
                  task, RT for matching \& nonmatching trials, 19-32
                  vs 63-86 yr olds},
  tags = {psychology;np},
}
@article{Kim1998,
  author = {Kim, Nancy},
  year = {1998},
  title = {The inhibition and activation of extinguished
                  stimuli in hemispatial neglect},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {58},
  number = {8},
  pages = {4483},
  abstract = {Unilateral neglect is characterized by an inability
                  to attend to contralesional stimuli. There have been
                  many recent reports indicating that neglected
                  stimuli nonetheless undergo extensive perceptual and
                  semantic and processing in the absence of
                  attention. The present study examines whether
                  representations of such stimuli are subject to
                  inhibitory processes which may be involved in
                  selection: We report a study of negative priming for
                  stimuli presented in the neglected and non-neglected
                  visual fields in patients with unilateral neglect as
                  a result of posterior parietal injury. The results
                  indicate that negative priming can occur only for
                  previously ignored items which were presented in the
                  attended ipsilesional field during the prime trial
                  (Experiments 1 and 3). Experiment 2 demonstrates,
                  however, that these same stimuli can produce robust
                  positive priming in these same subjects. These
                  results differ markedly from those obtained from a
                  brain-injured control group consisting of patients
                  with similar lesions (posterior parietal damage) who
                  do not show behavioral signs of neglect. In this
                  group, both negative as well as positive priming
                  occur regardless of the field of presentation of
                  ignored distractor items. This study provides
                  evidence that attention is a necessary prerequisite
                  for inhibitory processes involved in selection.},
  keywords = {The inhibition and activation of extinguished
                  stimuli in hemispatial neglect (unilateral neglect,
                  perception, awareness, negative priming, spatial
                  attention) Parietal Lobe},
  tags = {psychology;np},
}
@article{Kimchi2004Perceptual,
  author = {Kimchi, Ruth and Razpurker-Apfeld, Irene},
  year = {2004},
  title = {Perceptual grouping and attention: not all groupings
                  are equal.},
  journal = {Psychonomic bulletin \& review},
  volume = {11},
  number = {4},
  pages = {687--696},
  month = {aug},
  url = {http://view.ncbi.nlm.nih.gov/pubmed/15581119},
  abstract = {We examined grouping under inattention using Driver,
                  Davis, Russell, Turatto, \& Freeman's (2001)
                  method. On each trial, two successive displays were
                  briefly presented, each comprising a central target
                  square surrounded by elements. The task was to judge
                  whether the two targets were the same or
                  different. The organization of the background
                  elements stayed the same or changed, independently
                  of the targets. In different conditions, background
                  elements grouped into columns/rows by color
                  similarity, a shape (a triangle/arrow, a
                  square/cross, or a vertical/horizontal line) by
                  color similarity, and a shape with no other elements
                  in the background. We measured the influence of the
                  background on the target same-different
                  judgments. The results imply that background
                  elements grouped into columns/rows by color
                  similarity and into a shape when no segregation from
                  other elements was involved and the shape was
                  relatively "good." In contrast, no background
                  grouping was observed when resolving figure-ground
                  relations for segregated units was required, as in
                  grouping into a shape by color similarity. These
                  results suggest that grouping is a multiplicity of
                  processes that vary in their attentional
                  demands. Regardless of attentional demands, the
                  products of grouping are not available to awareness
                  without attention.},
  issn = {1069-9384},
  tags = {attention, binding, psychology},
  pdf = {pdf/Kimchi_2004_Perceptual_19700949.pdf},
  address = {University of Haifa, Haifa,
                  Israel. rkimchi@research.haifa.ac.il},
}
@article{Kindt1998,
  author = {Kindt, Merel and Brosschot, Jos F.},
  year = {1998},
  title = {Cognitive avoidance in phobia},
  journal = {Journal of Psychopathology \& Behavioral Assessment},
  volume = {20},
  number = {1},
  pages = {43-55},
  abstract = {Investigated the stage in which the bias changed
                  into avoidance and whether cognitive avoidance of
                  threat is restricted to information that refers to
                  the anxiety response as opposed to the threatening
                  stimulus. Therefore, 37 spider phobics (mean age 31
                  yrs) and 34 controls (mean age 38 yrs) were
                  administered a negative priming task and a free
                  recall task, using threat words and neutral words,
                  both divided into stimulus-related and
                  response-related words. There was no indication of
                  cognitive avoidance of the response-related threat
                  words as assessed by the negative priming
                  task. Recall findings indicated an incomplete memory
                  bias in phobics, with a better recall of
                  stimulus-related threat words instead of a worse
                  recall of response-related threat words. This
                  suggests a passive cognitive avoidance mechanism,
                  which may still impede the processing of
                  response-related information, crucial for the
                  success of exposure therapy.},
  keywords = {cognitive avoidance of stimulus vs response related
                  threat words, adults with spider phobia Cognitive
                  Processes},
  tags = {psychology;np},
}
@article{Kindt1998a,
  author = {Kindt, Merel and Brosschot, Jos F.},
  year = {1998},
  title = {Cognitive inhibition in phobia},
  journal = {British Journal of Clinical Psychology},
  volume = {37},
  number = {1},
  pages = {103-106},
  abstract = {Examined whether cognitive bias for threat in
                  anxious individuals may be attributed to a defective
                  inhibition. It was investigated whether phobics
