@incollection{Allport1999a,
  author = {Allport, Alan and Wylie, Glenn},
  editor = {Humphreys, Glyn W. and Duncan, John and et al.},
  year = {1999},
  title = {Task-switching: Positive and negative priming of
                  task-set},
  booktitle = {Attention, space, and action: Studies in cognitive
                  neuroscience},
  publisher = {LondonOxford University Press},
  pages = {273-296},
  abstract = {Provides a critical review of experiments on
                  task-switching, with reference to the theoretical
                  interpretation of behavioural 'switch-costs',
                  focusing on intrinsically competing pairs of tasks,
                  such as object naming in either of two languages;
                  competing arithmetic operations; and Stroop
                  colour-naming vs word-reading. It is argued that the
                  behavioural costs of switching between competing
                  tasks of this kind result from components of the
                  task-set for the preceding task(s), which can remain
                  primed involuntarily ('task-set inertia'). The
                  second half of the chapter describes some new
                  experiments, designed to test the hypothesis of
                  higher-order priming at the level of task
                  control. Three experiments, using existing
                  task-switching paradigms, confirm the prediction
                  that it is control characteristics of the preceding
                  task (the task FROM which a switch is made) that
                  primarily determine switch-costs between competing
                  tasks, rather than characteristics of the upcoming
                  task.  The results also challenge the assumption
                  that task-set is fully reconfigured on 'non-switch'
                  trials, in current task-switching paradigms.  Two
                  further experiments explore a new experimental
                  procedure, designed to exploit the phenomenon of
                  higher-order priming of task control-states, to
                  investigate properties of task-set.},
  tags = {psychology;np},
}
@article{Allred1998a,
  author = {Allred, Stephanie Lynn},
  year = {1998},
  title = {Effects of intervening trials on the decay of
                  identity and related priming},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {58},
  number = {9},
  pages = {5170},
  abstract = {After making initial lexical decision and
                  comprehension responses in a sentence-priming
                  context, participants were exposed to repetitions of
                  items that were identical, related, or unrelated to
                  previous target words after zero, one, or four
                  intervening trials. With consistent contexts, the
                  manipulation of target words affects lexical
                  decision response times, relative to two baselines:
                  first presentation and unrelated words. Any change
                  from an identical target word slowed lexical
                  decisions. This effect was consistent across time
                  (lags).  Changing the target word also affected
                  comprehension. Identical context produced the
                  highest amount of priming. However, comprehension
                  priming in every condition (identical, related, and
                  unrelated) was always positive, given that the same
                  comprehension question was asked at each
                  presentation (Experiment 1a). Experiment 1b was run
                  to determine if the lack of lexical decision priming
                  found for related words in Experiment 1a was based
                  on a violation of the expectation that an identical
                  word would follow. Thus in Experiment 1b, the
                  identical condition was eliminated. The results
                  replicated those in Experiment 1a. An Interactive
                  Activation and Competition (IAC) model was
                  constructed to simulate the lexical decision
                  data. One possible explanation for the lack of
                  related priming was a high decay rate. Another is
                  the decrease in attention given to a prime word
                  presented in a sentence context as opposed to
                  presented alone. Experiment 2 examined the variables
                  that affect comprehension. Participants saw
                  identical or related target words, after lags of
                  zero, one, or four trials, but did not make a
                  lexical decision about them. The comprehension
                  question either remained the same or was changed to
                  a related question. The variable that had the most
                  impact was whether the question was repeated
                  identically or was replaced with another
                  question. Any change from the identical question
                  slowed comprehension; therefore, producing negative
                  priming. Lexical decision and comprehension are very
                  specific.  Any change from the event on which a
                  decision is made (target word or comprehension
                  question) eliminated and facilitation effects of
                  repeating the event identically.},
  tags = {psychology;np},
}
@article{Amieva2002a,
  author = {Amieva, H and Lafont, Sylviane and Auriacombe,
                  Sophie and Le Carret, Nicholas and Dartigues, J and
                  Orgogozo, Jean-Marc and Fabrigoule, Colette},
  year = {2002},
  title = {Inhibitory breakdown and dementia of the Alzheimer
                  type: A general phenomenon?},
  journal = {Journal of Clinical \& Experimental Neuropsychology},
  volume = {24},
  number = {4},
  pages = {503-516},
  abstract = {Several recent studies have provided substantial
                  support for the proposal that a decrease in
                  inhibitory processing may play an important role in
                  cognitive changes occurring in the early stages of
                  Dementia of the Alzheimer Type (DAT). The question
                  addressed by the present study was whether these
                  deficits are the result of the failure of a general
                  inhibitory mechanism, or whether DAT is associated
                  with selective decreases in a subset of inhibitory
                  processes. For this, a computerized battery of tasks
                  assessing several inhibitory mechanisms was
                  administered to 28 mild DAT patients (mean age 75.8
                  yrs) and 28 matched elderly adults (mean age 75.2
                  yrs). The results showed that DAT patients failed to
                  produce Negative Priming effects and were severely
                  impaired in the Stroop task. However, no evidence
                  was found for an impairment on the Go-No go task and
                  only limited impairment on the Stop Signal task,
                  suggesting that in the early stages of the disease,
                  not all inhibitory mechanisms are uniformly
                  impaired.},
  tags = {psychology;np},
}
