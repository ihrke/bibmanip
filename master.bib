@article{Drogosz2003,
  author = {Drogosz, Marek},
  year = {2003},
  title = {Negatywne poprzedzanie jako narzedzie badania
                  zaburzen poznawczych Negative priming as a tool for
                  the research of cognitive impairments},
  journal = {Studia Psychologiczne},
  volume = {41},
  number = {1},
  pages = {37-59},
  abstract = {This article describes basic properties of an
                  experimental method called negative priming and its
                  applications to the research on cognitive inhibition
                  processes in aging, depression and
                  schizophrenia. The presented analysis of the
                  literature suggests that cognitive inhibition
                  deficits (at different levels of processing: both
                  stimuli and response selection) reduce or erase
                  negative priming effects. Neuropsychological
                  research with Parkinson disease patients, brain
                  imaging and magnetic resonance (fMRl) provides more
                  detailed data on functioning of brain structures
                  responsible for these phenomena. Potential advances
                  of negative priming applications in the social
                  cognition research, especially on a structure and
                  functioning of stereotypes, are also discussed.},
  keywords = {cognitive impairments negative priming negative
                  priming applications cognition inhibition deficits
                  aging depression schizophrenia Print},
  tags = {psychology;np},
}
@article{Drugge2000,
  author = {Drugge, Jeffrey Edwin},
  year = {2000},
  title = {Psychopathy and inhibitory control},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {60},
  number = {9},
  pages = {4937},
  abstract = {The role of inhibitory control in psychopathy was
                  investigated in three experiments, contrasting
                  student control participants with prisoners divided
                  on the basis of Psychopathy Checklist (PCL-R) scores
                  into High and Low psychopathy (High-P and Low-P)
                  groups. Experiment 1 assessed motor inhibition in a
                  stop signal paradigm (Logan, 1994), and found
                  equivalent inhibitory control in the three
                  groups. Experiment 2 assessed semantic inhibition in
                  a negative priming Stroop paradigm.  High-P
                  prisoners showed larger magnitudes of negative than
                  the other participant groups, and negative priming
                  magnitudes were more closely related to Factor 2 of
                  the PCL-R than to Factor 1. Experiment 3 assessed
                  semantic inhibition in an identity negative priming
                  paradigm using emotionally positive, negative, and
                  neutral word stimuli. The results replicated
                  Experiment 2, but in contrast to previous research
                  (Williamson, Harpur, \& Hare, 1991), no significant
                  group differences were found in the processing of
                  emotionally laden stimuli. Overall, the findings
                  support an 'overselection' view of psychopathy, in
                  which an overactive selective attention mechanism
                  makes psychopaths less susceptible to the moderating
                  influences of seemingly irrelevant stimuli.},
  keywords = {psychopathy, motor \& semantic inhibitory control,
                  prisoners Print},
  tags = {psychology;np},
}
@article{Due2001,
  author = {Due, Deborah Lynn},
  year = {2001},
  title = {A study of the physiologic and cognitive
                  contributions of mesocorticolimbic dopamine circuits
                  to stimulus salience detection in smokers},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {61},
  number = {7},
  pages = {3442},
  abstract = {Psychotropic drugs such as nicotine, and natural
                  rewards such as food, have been associated with a
                  release of dopamine in the nucleus accumbens via
                  projections from the ventral tegmental area
                  (VTA). For many years this phenomenon was believed
                  to be the neural mechanism responsible for
                  subjective feelings of pleasure derived from
                  rewarding substances.  Data collected from animal
                  studies relatively recently (1990's) have caused
                  investigators to question the idea that dopamine in
                  the accumbens engenders pleasure. Electrophysiologic
                  and single unit recording studies of VTA and
                  accumbel responses to stimuli predicting reward have
                  clearly established that dopamine release can occur
                  well before ingestion and in some cases actually
                  peaks prior to ingestion of the reward. Furthermore,
                  studies of novelty and stress have determined that
                  dopamine is also released in these contexts. This
                  pattern of dopamine activation suggests that
                  dopamine release may occur in response to a broad
                  range of stimulus types, all of them having in
                  common the attribute of salience. In order to test
                  this hypothesis in human subjects, the idea was
                  explored from two levels: a physiological level and
                  a cognitive level. A functional MRI (fMRI) study was
                  performed in deprived smokers to identify which
                  brain substrates were preferentially activated by
                  visual smoking cues. The idea was studied from a
                  purely cognitive aspect by comparing the performance
                  of deprived and non-deprived smokers on reaction
                  time tasks. In the fMRI study, smoking cue stimuli
                  modulated a distributed neural circuit which
                  included neocortical areas known to be involved in
                  visuospatial attention (fusiform gyrus, parietal and
                  prefrontal cortex) as well as mesolimbic substrates
                  (VTA and nucleus accumbens). In the cognitive
                  studies, negative priming was decreased on the
                  craving as compared to the non-craving day.  We
                  conclude that a neural circuit encompassing
                  mesolimbic substrates and visual attention areas, is
                  activated in smokers due to the salience of visual
                  smoking cues. The increased activation of this
                  circuit as a result of craving, interferes with
                  cognitive processes dependent on the circuit for
                  salience detection in other contexts, such as the
                  processes operating during a negative priming task.},
  keywords = {mesocorticolimbic dopamine circuits stimulus
