@article{Milliken1994,
  author = {Milliken, Bruce and Tipper, Steven P. and Weaver,
                  Bruce},
  year = {1994},
  title = {Negative priming in a spatial localization task:
                  Feature mismatching and distractor inhibition},
  journal = {JEPHPP},
  volume = {20},
  number = {3},
  pages = {624-646},
  abstract = {Negative priming has recently been demonstrated in
                  tasks requiring spatial localization (S. P. Tipper
                  et al; see record 1991-00251-001), supporting the
                  notion that distractors are actively inhibited
                  during selection. However, it has since been argued
                  that this effect is caused by the appearance of
                  mismatching identities at a single location (J. Park
                  and N. Kanwisher; see record 1994-35939-001). The
                  present studies show that negative priming in a
                  spatial localization task can occur when the ignored
                  distractor and subsequent target are identical.
                  However, feature mismatches can also lead to
                  negative priming. The argument is made that
                  distractor inhibition and implicit retrieval of
                  previously presented items together provide a better
                  account of efficient sequential selective behavior
                  than does either process alone.},
  keywords = {stimulus feature mismatch vs distractor inhibition,
                  selective attention during localization, 19-42 yr
                  olds, Canada},
  tags = {psychology;np},
  pdf = {./pdf/Milliken_1994.pdf},
}
@article{Milliken1996,
  author = {Milliken, Bruce and Joordens, Steve},
  year = {1996},
  title = {Negative priming without overt prime selection},
  journal = {Canadian Journal of Experimental Psychology},
  volume = {50},
  number = {4},
  pages = {333-346},
  abstract = {Investigated whether negative priming occurs in the
                  absence of overt prime selection in 3 Exps using 16
                  college students with normal or corrected-to-normal
                  vision as Ss. In Exp 1, Ss responded to a target
                  item in the probe display only, instead of the usual
                  procedure that requires Ss to also respond to 1 of
                  the items in the prime display.  In Exp 2, Ss were
                  asked to choose the less bright of 2 probes
                  displayed in the same color. The same procedure was
                  used for Exp 3 except the distractor was removed
                  from the probe display. The authors conclude that
                  overt selection against a prime distractor in favor
                  of a probe target is not necessary to observe
                  negative priming. This result demonstrates a
                  fundamental misunderstanding concerning the
                  procedure required to measure negative priming and
                  presents an experimental procedure that is of
                  considerable utility in evaluating theoretical
                  accounts of negative priming.},
  keywords = {experimental procedure without overt prime target
                  selection, observation of negative priming, college
                  students with corrected or normal vision},
  tags = {psychology;np},
}
@article{Milliken1997,
  author = {Milliken, Bruce and Rock, Adrienne},
  year = {1997},
  title = {Negative priming, attention, and discriminating the
                  present from the past},
  journal = {Consciousness \& Cognition: An International
                  Journal},
  volume = {6},
  number = {2},
  pages = {308-327},
  abstract = {In the case of negative priming, an activation-based
                  framework for interpreting priming suggests that
                  attention inhibits the representation of prime
                  distractors and that this inhibition is measured in
                  performance to subsequent probes. Data summarized in
                  this article point out that negative priming does
                  not depend on selection of 1 of 2 primes and that
                  attentional influences during retrieval play an
                  important role in determining negative priming. Two
                  experiments are described. Exp 1, with 24
                  undergraduates (mean age 19 yrs), and Exp 2, with 32
                  undergraduates (mean age 20 yrs), demonstrated a
                  correlation between priming effects and knowledge of
                  the relation between primes and probes. The authors
                  suggest that negative priming is not determined
                  directly by a process of ignoring, but instead
                  occurs because a repeated probe is less temporally
                  distinct when ignored as a prime than when
                  attended.},
  keywords = {attentional influences on negative priming effects
                  during retrieval, undergraduates, Canada Visual
                  Stimulation},
  tags = {psychology;np},
}
@article{Milliken1998,
  author = {Milliken, Bruce and Joordens, Steve and Merikle,
                  Philip M. and Seiffert, Adriane E.},
  year = {1998},
  title = {Selective attention: A reevaluation of the
                  implications of negative priming},
  journal = {Psychological Review},
  volume = {105},
  number = {2},
  pages = {203-229},
  abstract = {The notion that inhibitory processes play a critical
                  role in selective attention has gained wide
                  support. Much of this support derives from studies
                  of negative priming. The authors note that the
                  attribution of negative priming to an inhibitory
                  mechanism of attention draws its support from a
                  common assumption underlying priming procedures,
                  together with the procedure that has been used to
                  measure negative priming. The results from a series
                  of experiments demonstrate that selection between 2
                  competing prime items is not required to observe
                  negative priming. This result is demonstrated across
                  several experiments in which participants named 1 of
                  2 items in a second display following presentation
                  of a single-item prime. The implications of these
                  results for existing theories of negative priming
                  are discussed, and a theoretical framework for
                  interpreting negative priming and several related
                  phenomena is forwarded.},
  keywords = {selective attention between competing prime items,
                  negative priming, college students, Canada},
  tags = {psychology;np},
  pdf = {pdf/Milliken_1998.pdf},
  comment = {{\bf J{\"o}rg: } NP;\\ {\bf Matthias: } \\ {\bf
                  Hecke: }},
}
