@article{Beech1989b,
  author = {Beech, Anthony and Baylis, Gordon C. and Smithson,
                  Paul and Claridge, Gordon},
  year = {1989},
  title = {Individual differences in schizotypy as reflected in
                  measures of cognitive inhibition},
  journal = {British Journal of Clinical Psychology},
  volume = {28},
  number = {2},
  pages = {117-129},
  abstract = {Used the negative priming paradigm to investigate
                  cognitive differences in 82 normal schizotypal Ss
                  (aged 18-44 yrs). Lists of color words from the
                  Stroop Color and Word Test were presented at
                  different display times in a number of priming and
                  nonpriming conditions, in one of which the ignored
                  color name predicted the color of the next target
                  item. The increased reaction time (RT) latencies to
                  the target normally found in this condition were
                  reduced, or even reversed, in high schizotypal Ss
                  selected on the basis of a schizotypy scale. This
                  effect was confined to a 100-msec presentation time,
                  suggesting that schizotypy is associated with
                  weakened inhibition operating in the early
                  (automatic) stages of information processing. The
                  familiar Stroop (interference) effect was related
                  overall to negative priming, but was not responsible
                  for schizotypy differences.},
  keywords = {individual differences in schizotypy \& negative
                  priming \& cognitive inhibition, 18-44 yr olds,
                  England Individual Differences},
  tags = {psychology;np},
}
