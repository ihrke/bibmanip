@article{Abbott2007,
  author = {Abbott, L F and Rohrkemper, R},
  year = {2007},
  title = {{A simple growth model constructs critical avalanche
                  networks}},
  journal = {Prog Brain Res},
  volume = {165},
  pages = {13--19},
  month = {jan},
  doi = {10.1016/S0079-6123(06)65002-4},
  url = {http://www.sciencedirect.com/science?\_ob=ArticleURL\&\_udi=B7CV6-4PV8M5N-4\&\_user=5731894\&\_rdoc=1\&\_fmt=\&\_orig=search\&\_sort=d\&view=c\&\_acct=C000043105\&\_version=1\&\_urlVersion=0\&\_userid=5731894\&md5=ee9dea8f96cf46f49f8d7495531480bc},
  abstract = {Neurons recorded from electrode arrays show a
                  remarkable scaling property in their bursts of
                  spontaneous activity, referred to as "avalanches"
                  (Beggs and Plenz, 2003, 2004). Such scaling suggests
                  a critical property in the coupling of these
                  circuits. We show that similar scaling laws can
                  arise in a simple model for the growth of neuronal
                  processes. In the model (Van Ooyen and Van Pelt,
                  1994, 1996), the spatial range of the processes
                  extending from each neuron is represented by a
                  circle that grows or shrinks as a function of the
                  average intracellular calcium concentration. Neurons
                  interact when the circles corresponding to their
                  processes intersect, with a strength proportional to
                  the area of overlap.},
  keywords = {Action Potentials, Animals, Models: Neurological,
                  Nerve Net, Neurons,Neural Networks (Computer)},
  tags = {{soc}},
}
