@article{Lambert1993,
  author = {Lambert, Anthony J.},
  year = {1993},
  title = {Attentional interaction in the split-brain: Evidence
                  from negative priming},
  journal = {Neuropsychologia},
  volume = {31},
  number = {4},
  pages = {313-324},
  abstract = {Used a negative priming paradigm in which Ss
                  attended to digits presented to the right visual
                  field (RVF) and ignored digits presented to the left
                  visual field (LVF). Ss were 2 split-brain men (36
                  and 38 yrs old) and 7 normal male control Ss (aged
                  25-41 yrs). The task was to categorize RVF digits as
                  odd or even. Each of the split-brain patients showed
                  significant amounts of negative priming from ignored
                  LVF distractors, as did the normal control
                  Ss. Findings indicate that even in the split brain
                  there are rich attentional interactions with respect
                  to complex attributes encoded within each
                  hemisphere.  These attributes may rely on
                  interaction between cortical and subcortical
                  processing, and may play an important role in
                  relation to the unified and apparently well
                  integrated behavior of commissurotomy patients in
                  everyday settings.},
  keywords = {interactional attention to stimulus in right visual
                  field with left visual field distractors in negative
                  priming paradigm, male split brain 36 \& 38 yr olds
                  Left Brain Right Brain},
  tags = {psychology;np},
}
@article{Lambert1996,
  author = {Lambert, Anthony J.},
  year = {1996},
  title = {Inter-hemispheric interaction in the split brain:
                  Inhibitory and facilitatory priming},
  journal = {New Zealand Journal of Psychology},
  volume = {25},
  number = {2},
  pages = {12-19},
  abstract = {Two experiments examined attentional interactions in
                  2 split-brain male patients (aged 40 and 42 yrs). Ss
                  were required to categorize a stimulus briefly
                  presented to the right visual field, while ignoring
                  a stimulus presented to the left visual field
                  (LVF). Exp 1 used word stimuli, while Exp 2 used
                  digits as stimuli. Both inhibitory (negative
                  priming) and facilitatory priming effects were
                  found. Exp 2 showed that in split-brain patients
                  these effects can be explained in terms of
                  sub-cortical transfer of a simple code representing
                  the category (living or non-living, odd or even) of
                  the unattended LVF stimulus.  It is proposed that
                  even in the absence of commissural connections there
                  are rich inter-hemispheric facilitatory and
                  inhibitory attentional interactions, proceeding via
                  subcortical pathways.},
  keywords = {interhemispheric attentional interactions in absence
                  of cerebral commissures, 40 \& 42 yr old male split
                  brain patients},
  tags = {psychology;np},
}
@article{Lammertyn2005,
  author = {Lammertyn, J. and Fias, W.},
  year = {2005},
  title = {{Negative priming with numbers: No evidence for a
                  semantic locus}},
  journal = {Quarterly Journal of Experimental Psychology},
  volume = {58},
  number = {7},
  pages = {1153--1172},
  abstract = {Selective attention has been studied extensively
                  using the negative priming (NP) paradigm. An
                  important issue regards the representational level
                  at which NP occurs. We investigated this issue by
                  using numbers as stimuli. Because numbers have a
                  well-defined semantic organization, which can be
                  clearly measured by means of the distance effect,
                  they are very suitable for testing the assumption
                  that NP is situated at a central semantic
                  level. Four experiments are presented in which the
                  numerical distance between prime distractor and
                  probe target was manipulated. The task was magnitude
                  comparison. Target and distractor were defined on
                  the basis of colour. In Experiment 1, all numbers
                  were presented in Arabic format; NP was observed
                  only with identical prime distractor and probe
                  target, and no distance-related NP was
                  observed. This could not be explained by a decay of
                  inhibition since in Experiment 2 similar results
                  were obtained with a shortened response-to-stimulus
                  interval. Experiment 3 showed that these
                  observations also hold for numbers presented
                  verbally. Nevertheless, a cross-notational
                  experiment with Arabic prime and verbal probe
                  (Experiment 4) revealed no NP whatsoever and
                  excluded the possibility that the absence of
                  distance-related negative priming was the result of
                  a fine-tuned inhibitory mechanism operating at the
                  semantic level. The results are considered in the
                  light of current theories of negative priming.},
  tags = {psychology;np},
  comment = {{\bf J{\"o}rg: } \\ {\bf Matthias: } \\ {\bf Hecke:
                  }},
}
