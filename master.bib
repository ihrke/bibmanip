@article{Moon2003,
  author = {Moon, Myungho},
  year = {2003},
  title = {An information processing approach in measuring
                  emotional regulation},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {63},
  number = {7},
  pages = {3504},
  abstract = {The purpose of the current study was to develop a
                  measure of emotional regulation based on the
                  negative priming paradigm and to provide some
                  evidence of the validity of the new measure. The
                  current study was focused on preconscious emotional
                  regulation processes and develops a negative priming
                  task in the context of emotion. To provide validity
                  evidence for the negative priming measure, the
                  current study also included several questionnaires
                  measuring emotion related constructs.  It also
                  included criterion variable measures including
                  recall, recognition, task satisfaction, the
                  Scrambled Sentence Task, editing, and depression.
                  The negative priming task yielded two kinds of
                  emotional regulation measures based on reaction time
                  and probe error. The pattern of relationships
                  between the negative priming measures and emotion
                  related measures suggested that the negative priming
                  task was actually measuring emotional regulation
                  processes. Especially the error measure at the
                  preconscious level showed significant predictions to
                  criterion variables including recall, task
                  satisfaction, depression, and editing task, whereas
                  the negative priming measure at the conscious level
                  did not show such significant
                  predictions. Affectivity and personality measures
                  significantly predicted some of the criterion
                  variables including depression and the Scrambled
                  Sentence Task. In addition, category error, which
                  was included as an experimental variable, showed
                  significant predictions to criterion variables. The
                  differential predictions of the negative priming
                  measures and the questionnaire measures suggested
                  that the negative priming task might be addressing a
                  broader range of emotional regulation processes. The
                  results of the current study provided some empirical
                  evidence for the utility of negative priming
                  paradigm in the study of emotional regulation. In
                  addition, the current study was the first that
                  provided empirical evidence for the predictability
                  of emotional processes to task performance. Other
                  theoretical and methodological implications, and
                  limitations were discussed, and suggestions for
                  future research were offered.},
  keywords = {negative priming emotion regulation Print},
  tags = {psychology;np},
}
@article{Moore1994,
  author = {Moore, Cathleen M.},
  year = {1994},
  title = {Negative priming depends on probe-trial conflict:
                  Where has all the inhibition gone?},
  journal = {PP},
  volume = {56},
  number = {2},
  pages = {133-147},
  abstract = {Investigated the effects of probe-trial conflict on
                  negative priming in 5 experiments involving 80
                  undergraduates. The task in each experiment was a
                  letter-identification task. Two factors were
                  manipulated that were intended to affect how easily
                  probes could be identified as conflict or
                  nonconflict trials: the context in which conflict
                  and nonconflict probe trials occurred, and the
                  similarity between conflict and nonconflict
                  trials. Context was manipulated within each of the
                  1st 4 experiments; similarity was manipulated
                  between experiments.  Results suggest that negative
                  priming will fail to occur under nonconflict
                  conditions only if it is quite apparent that no
                  conflicting information is present. Negative priming
                  may be associated with a specific part of the
                  selection process that is involved in protecting the
                  person from making a response based on incorrect
                  information, and this process contributes only
                  sometimes to reaction time (RT).},
  keywords = {context \& similarity of conflict \& nonconflict
                  probe trials, negative priming in letter
                  identification tasks, college students Letters
                  (Alphabet)},
  tags = {psychology;np},
  pdf = {pdf/Moore_1994.pdf},
  comment = {{\bf J{\"o}rg: } \\ {\bf Matthias: } \\ {\bf Hecke:
                  }Das ist der Literaturtip von Mister Frings
                  bezueglich der IN-Trials...},
}
@article{Moore1995,
  author = {Moore, Cathleen M.},
  year = {1995},
  title = {Negative priming: Evidence for postcategorical
                  inhibitory selection?},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {55},
  number = {12},
  pages = {5588},
  abstract = {Responses to recently ignored stimuli are often
                  slower than responses to new stimuli. This
                  slowing--referred to as negative priming--has been
                  cited as evidence for two important features of
                  selective attention.  Specifically, it is thought to
                  indicate that irrelevant stimuli are processed to
                  relatively deep levels of representation
                  (postcategorical) and that these stimuli are
                  subsequently inhibited. The research reported in
                  this dissertation was concerned with investigating
                  negative priming in detail, in part to assess the
                  validity of these interpretations, and in part to
                  learn more about the source of the slowing. In
                  chapter 1, five different models of negative priming
                  are presented. Three are classified as
                  inhibitory-selection models because they maintain
                  that negative priming is caused by an inhibitory
                  mechanism of selection.  The other two are
                  classified as post-selection models because they
                  maintain that the slowing is caused by a process
                  that occurs after selection, and that negative
                  priming is unrelated to selective attention.  In
                  four empirical chapters (chapters 2-5), the two
                  classes of models are contrasted. Three general
                  empirical points were gained from the research
                  reported in these chapters. First, it was
                  demonstrated that negative priming depends on
                  characteristics of the trial on which the slowing is
                  observed (i.e., after the inhibition has taken
                  place, according to inhibitory-selection
                  views). Second, it was demonstrated that negative
                  priming may occur only when, on the trial in which
                  the inhibition is supposed to take place, there is
                  substantial danger that a response may be made on
                  the basis of an incorrect stimulus.  Third it was
                  demonstrated that the level of representation with
                  which negative priming is associated is not
                  constant. Instead, it seems to vary depending on
                  both the task and the stimulus conditions under
                  which the slowing is observed. Finally, chapter 6
                  provides a summary of the results and an evaluation
                  of the five models. In addition},
  keywords = {negative priming, selective attention},
  tags = {psychology;np},
}
@article{Moore1996a,
  author = {Moore, Cathleen M.},
  year = {1996},
  title = {Does negative priming imply preselective
                  identification of irrelevant stimuli?},
  journal = {Psychonomic Bulletin \& Review},
  volume = {3},
  number = {1},
  pages = {91-94},
  abstract = {Examined whether negative priming reflected
                  preselective identification of irrelevant stimuli as
                  maintained by the late-selection interpretation of
                  the slowing of responses, in 2 experiments. The
                  opportunity for preselective identification of
                  irrelevant stimuli was eliminated in both the
                  experiments, by presenting the irrelevant stimuli
                  late within the trial. For 16 Ss (aged 18-24 yrs) in
                  Exp 1, the timing of distractor onset was
                  manipulated only on the prime trial. In Exp 2, the
                  timing of the distractor was manipulated in both
                  prime and probe trials for 24 Ss. Results do not
                  provide evidence to suggest that negative priming
                  was caused by late processing of irrelevant
                  stimuli. Negative priming failed to occur under the
                  present circumstances.},
  keywords = {preselective identification of irrelevant stimuli,
                  negative priming, 18-24 yr olds},
  tags = {psychology;np},
  comment = {{\bf J{\"o}rg: } \\ {\bf Matthias: } \\ {\bf Hecke:
                  nette Modellklassifizierung: 1. selektion vor
                  distractor-processing, in einer Nachbewertung wird
                  dann der Distraktor als irrelevant eingeschaetzt
                  2. selektion nach distractor-processing wie bei
                  Tipper etc. Hauptaussage: These results suggest that
                  irrelevant stimuli may indeed be processed before
                  target selection begins. Nettes Experiment: der
                  Distractor wird die ersten oder erst die zweiten 150
                  ms der 300 ms Targetpraesentationszeit gezeigt. Wenn
                  der Distractor spaet gezeigt wird, gibts kein
                  NP. bei early schon.}},
}
@article{Moritz1997,
  author = {Moritz, Steffen and Mass, Reinhard},
  year = {1997},
  title = {Reduced cognitive inhibition in schizotypy},
  journal = {British Journal of Clinical Psychology},
  volume = {36},
  number = {3},
  pages = {365-376},
  abstract = {Investigated the relationship between negative
                  priming and the positive symptoms of schizotypy. It
                  was hypothesized that high schizotypes would display
                  less negative priming than low schizotypes. A
                  further aim of the study was to disentangle the
                  modalities involved in negative priming performance,
                  since a reduction in negative priming, as revealed
                  by schizophrenics and schizotypes, might reflect a
                  failure to actively inhibit irrelevant information
                  (cognitive inhibition) as well as dyslexia or a
                  general slowness in information processing. 45
                  healthy adults (aged 18-57 yrs) were drawn from the
                  general population. After neuropsychological
                  assessment participants completed a battery of
                  schizotypal questionnaires. Correlational analyses
                  and mean comparisons were used to investigate the
                  relationship between neuropsychological data and
                  schizotypy. Results suggest that reduced negative
                  priming is related to positive schizotypy. Reduced
                  negative priming in high schizotypes was unrelated
                  to dyslexia, and found unlikely to be affected by
                  deficits in early processing. The inverted priming
                  condition produced comparatively fewer errors in
                  high schizotypes. Results support the hypothesis
                  that reduced cognitive inhibition may underlie
                  positive schizotypal symptomatology.},
  keywords = {negative priming \& cognitive inhibition, positive
                  symptomatology, healthy 18-57 yr olds high vs low in
                  schizotypy},
  tags = {psychology;np},
}
@article{Moritz1998,
  author = {Moritz, Steffen H. and Mass, Reinhard and Junk, Udo},
  year = {1998},
  title = {Further evidence of reduced negative priming in
                  positive schizotypy},
  journal = {Personality \& Individual Differences},
  volume = {24},
  number = {4},
  pages = {521-530},
  abstract = {Studied whether negative priming is reduced in
                  positive schizotypy but not in negative schizotypy
                  among 100 healthy adults. Results show that high
                  scorers on positive schizotypal scales displayed
                  reduced negative priming as compared to those with
                  low scores. Except for the Schizotypal Personality
                  Questionnaire (SPQ) subscale Social Anxiety, which
                  has been considered a negative symptom scale in
                  previous SPQ-analyses, no negative schizotypal or
                  depression scale was associated with negative
                  priming performance. In line with Diagnostic and
                  Statistical Manual of Mental Disorders-IV (DSM-IV),
                  it is suggested that social anxiety may partly
                  reflect an epiphenomenon of positive
                  symptomatology. The results add some convergent and
                  discriminant validity to the hypothesis that reduced
                  negative priming is a neuropsychological correlate
                  of high positive schizotypy.},
  keywords = {negative priming, adults with positive vs negative
                  schizotypy},
  tags = {psychology;np},
}
@article{Moritz2000,
  author = {Moritz, Steffen and Andresen, Burghard and
                  Probsthein, Ellen and Martin, Tom and Domin, Frauke
                  and Kretschmer, Gabi and Krausz, Michael and
                  Lambert, Martin and Naber, Dieter},
  year = {2000},
  title = {Stimulus onset asynchronicity as a modulator of
                  negative priming in schizotypy},
  journal = {Personality \& Individual Differences},
  volume = {29},
  number = {4},
  pages = {649-657},
  abstract = {A reduction of the negative priming (NP)-effect has
                  been found for both schizophrenics and high
                  schizotypes, which is commonly interpreted as a sign
                  of cognitive disinhibition in these populations. The
                  present study investigated the effects of stimulus
                  presentation times and stimulus onset asynchronicity
                  (SOA) by means of NP with a fixed stimulus
                  presentation time and 2 alternating
                  response-to-stimulus intervals, and correlated both
                  NP-effects with another potential measure of
                  cognitive inhibition (semantic priming) to attach
                  some convergent validity to the hypothesis that
                  reduced NP reflects disinhibition and not other
                  cognitive mechanisms. 123 Ss (mean age 29.6 yrs)
                  free of physical and mental disorders
                  participated. Results show that a negative
                  correlation between various schizotypal traits and
                  NP was replicated only when using a short SOA. For
                  long SOA conditions, NP and positive schizotypy
                  correlated in a positive direction at trend
                  level. NP did not correlate with semantic priming,
                  indicating that both measures tap different
                  cognitive functions. Findings suggest that NP is not
                  generally diminished in high schizotypes, but that
                  the relationship depends on the length of the SOA.},
  keywords = {stimulus onset asynchrony as modulator of negative
                  priming in schizotypy, Ss (mean age 29.6 yrs) free
                  of physical \& mental disorders Schizophrenia},
  tags = {psychology;np},
}
@article{Moritz2000a,
  author = {Moritz, Steffen and Jacobsen, Dirk and Mersmann,
                  Kathrin and Kloss, Martin and Andresen, Burghard},
  year = {2000},
  title = {Negative priming in schizophrenia: No evidence for
                  reduced cognitive inhibition},
  journal = {Journal of Nervous \& Mental Disease},
  volume = {188},
  number = {9},
  pages = {624-627},
  abstract = {Used 2 different versions of the negative priming
                  (NP) paradigm (allowing the measurement of 5 NP
                  indexes) to examine whether NP is attenuated by
                  schizophrenia. 30 psychiatric inpatients with
                  schizophrenia and 30 healthy participants completed
                  computerized NP tasks and a task with a classical
                  noncomputerized Stroop design. All Ss also completed
                  a psychopathological assessment. Although
                  schizophrenic Ss performed significantly slower in
                  the classical Stroop tasks and other tasks, they did
                  not exhibit decreased NP in any tasks. However, for
                  the classical Stroop design schizophrenic Ss
                  exhibited significantly greater NP.},
  keywords = {negative priming, inpatients with schizophrenia
                  Stroop Effect},
  tags = {psychology;np},
}
@article{Moritz2001,
  author = {Moritz, Steffen and Ruff, Christian and Wilke, Uta
                  and Andresen, Burghard and Krausz, Michael and
                  Naber, Dieter},
  year = {2001},
  title = {Negative priming in schizophrenia: Effects of
                  masking and prime presentation time},
  journal = {Schizophrenia Research},
  volume = {48},
  number = {2},
  pages = {291-299},
  abstract = {Investigated whether reduced negative priming in
                  schizophrenics may be an experimental
                  artifact. Based on evidence from backward masking
                  studies in schizophrenia, it was hypothesized that
                  brief prime presentation times and pattern masking
                  used in other research may have impaired the visual
                  perception of the prime display in
                  schizophrenics. 20 schizophrenic patients (mean age
                  33.0 yrs) and 20 matched healthy controls (mean age
                  31.8 yrs) participated in the study. Subjects
                  completed four negative priming experiments varying
                  in prime presentation time (100 or 250 ms) and
                  masking (a mask or a blank screen followed prime
                  presentation). In line with prediction, reduced
                  negative priming in schizophrenics only occurred for
                  trials with 100 ms prime presentation time followed
                  by a mask. Neither psychopathology nor any
                  sociodemographic variable correlated substantially
                  with negative priming. Results strongly suggest that
                  reduced negative priming in schizophrenics may not
                  be due to reduced cognitive inhibition but mirrors
                  perceptual deficits.},
  keywords = {schizophrenia pattern masking prime presentation
                  time negative priming visual perception Print
                  Electronic},
  tags = {psychology;np},
}
@article{Moritz2004,
  author = {Moritz, Steffen and Andresen, Burghard},
  year = {2004},
  title = {Reduced negative priming in schizotypy: Failure to
                  replicate},
  journal = {British Journal of Clinical Psychology},
  volume = {43},
  number = {2},
  pages = {211-215},
  abstract = {The aim of the present study was to investigate the
                  association of negative priming and schizotypal
                  personality using a Stroop-version of the
                  paradigm. Negative priming was determined for 160
                  healthy participants. Schizotypal symptoms were
                  assessed with several schizotypal scales. Results
                  indicate that no significant relationship emerged
                  between any of the schizotypal scales and negative
                  priming. It is inferred that negative priming is
                  undisturbed in schizophrenia and schizotypy when
                  unmasked negative priming tasks are
                  employed. Reduced negative priming in previous
                  studies may represent an epi-phenomenon of short
                  prime presentation times and backward masking.},
  keywords = {negative priming schizotypy schizotypal personality
                  symptoms schizophrenia Print Electronic},
  tags = {psychology;np},
}
