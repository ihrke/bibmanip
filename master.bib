@article{Armstrong1999a,
  author = {Armstrong, Irene T.},
  year = {1999},
  title = {Repetition deficit effects in serial recall: Finding
                  a common account for repetition blindness and the
                  Ranschburg effect},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {59},
  number = {7},
  pages = {3721},
  abstract = {When subjects are given a list of words and asked to
                  report them in order, they will recall a list with a
                  repeated item less well than a list without a
                  repeated item. The repetition deficit has been
                  documented in the memory literature, where it is
                  known as the Ranschburg effect and, more recently,
                  in the perception literature, where it is known as
                  Repetition blindness. Different kinds of mechanism
                  have been suggested to explain the repetition
                  deficit in the two sets of literature.  The
                  perceptual literature claims that the deficit
                  reflects a perceptual failure that, in turn,
                  reflects capacity limits challenged by a rapid rate
                  of presentation. The memory literature claims that
                  the deficit reflects a post-perceptual failure
                  related to retrieval from memory.  The latter claim
                  has the potential to account for both
                  cases. Proponents of perceptual theory argue that
                  the repetition deficit shown at fast rates of
                  presentation is a new phenomenon and established
                  criteria to distinguish it from the Ranschburg
                  effect. Several of the distinctions have already
                  been challenged in the literature, and I test the
                  remaining distinctions and reject them. I argue that
                  there is no need to distinguish the two repetition
                  deficits. Hence, I conclude that a post-perceptual
                  mechanism accounts for the repetition deficit. My
                  results provide boundary conditions that a
                  successful post-perceptual mechanism must
                  fit. First, the deficit appears under whole-report
                  conditions but not under partial report. Second, the
                  deficit occurs only when there are repetitions
                  across trials as well as within trials. Third, the
                  deficit does not occur when performance on no-repeat
                  control trials is too low. No existing accounts of
                  the repetition deficit fits all of the boundary
                  conditions, including two otherwise promising
                  computational models of memory for serial recall,
                  OSCAR and TODAM. I argue that an existing account of
                  negative priming provides a promising direction for
                  future work.},
  tags = {psychology;np},
}
@article{Audet2000,
  author = {T. Audet and L. Mercier and S. Collard and
                  A. Rochette and R. Hubert},
  year = {2000},
  title = {Attention deficits: is there a right hemisphere
                  specialization for simple reaction time, sustained
                  attention, and phasic alertness?},
  journal = {Brain Cogn},
  volume = {43},
  number = {1-3},
  pages = {17--21},
  abstract = {Impairments of attentional focus often are claimed
                  to be associated with lesions of the right
                  hemisphere. Although some studies comparing right-
                  and left-brain-damaged patients have supported this
                  idea, others have not found differences between
                  these patients in various attention tasks. The
                  present study was carried out in order to further
                  investigate the putative role of the right
                  hemisphere in a simple reaction time task, a
                  sustained attention task, and a phasic alertness
                  task. Subjects were 46 patients with
                  right-hemisphere lesions and 37 patients with
                  left-hemisphere lesions. Results showed no
                  difference between right- and left-brain-damaged
                  patients in simple reaction time, in speed of
                  response over time (sustained attention), and in the
                  capacity to prepare to an uncoming stimulus (phasic
                  alertness).  Future studies will have to use a more
                  precise categorization of lesions than only
                  right-versus left-brain damage.},
  comment = {{\bf J{\"o}rg: } \\ {\bf Matthias: } \\ {\bf Hecke:
                  }},
}
