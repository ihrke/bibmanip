@article{Milliken1999,
  author = {Milliken, Bruce and Lupianez, Juan and Debner, Jim
                  and Abello, Brent},
  year = {1999},
  title = {Automatic and controlled processing in Stroop
                  negative priming: The role of attentional set},
  journal = {Journal of Experimental Psychology: Learning,
                  Memory, \& Cognition},
  volume = {25},
  number = {6},
  pages = {1384-1402},
  abstract = {The distinction between automatic and controlled
                  attentional influences on priming effects was
                  examined in a series of Stroop color-naming
                  experiments. As expected, priming effects depended
                  on the proportion of repeated trials-those in which
                  a color word prime matched a following ink-color
                  probe. However, responses were slower for repeated
                  trials than for unrepeated trials when the
                  proportion of repeated trials was no greater than
                  chance (25 with 4 colors). This effect was shown not
                  to depend on slow-to-develop expectancies but did
                  depend on the selective-attention requirements of
                  the probe task. This dependence on probe task
                  selection parallels an often reported result in the
                  negative-priming literature (e.g., D. G. Lowe, 1979;
                  S. P. Tipper \& M. Cranston, 1985). Implications of
                  these results for the distractor inhibition and
                  episodic retrieval accounts of negative priming are
                  discussed.},
  keywords = {automatic vs controlled attentional processing in
                  Stroop negative priming, college students},
  tags = {psychology;np},
}
@article{Milliken2000,
  author = {Milliken, B. and Tipper, S.P. and Houghton, G. and
                  Lupi{\'a}{\~n}ez, J.},
  year = {2000},
  title = {Attending, ignoring, and repetition: On the relation
                  between negative priming and inhibition of return},
  journal = {PP},
  volume = {62},
  number = {6},
  pages = {1280-1296},
  abstract = {Examined the relation between negative priming and
                  inhibition of return in a series of spatial
                  localization experiments. In Exp 1, 20 Ss (mean age
                  20 yrs) responded selectively to the location of a
                  target and ignored the location of a distractor in 2
                  consecutive displays.  In Exp 2, 16 Ss (mean age
                  20.1 yrs) were shown the same type of displays but
                  were asked to respond to a target stimulus only in
                  the 2nd of the 2 displays. A single prime stimulus
                  was followed by either a single probe (Exp 3A) or a
                  probe target and distractor (Exp 3B).  Eight Ss
                  participated in Exp 3A and 16 Ss participated in Exp
                  3B (mean age 19.3 yrs in both experiments). The
                  results of Exp 1 demonstrate that slowed responses
                  to repeated location stimuli can be obscured by
                  repetition priming effects involving stimulus
                  dimensions other than spatial location. The results
                  of Exps 2, 3A, and 3B demonstrate that these
                  repetition priming effects may occur only when Ss
                  are required to respond to the prime
                  display. Together, these results suggest that
                  differences between attended and ignored repetition
                  effects in selective attention studies of spatial
                  localization do not provide a basis for
                  distinguishing between spatial negative priming and
                  inhibition of return.},
  keywords = {spatial localization \& negative priming \&
                  inhibition, college students},
  tags = {psychology;np},
}
@article{Mintzer1999,
  author = {Mintzer, Miriam Z. and Griffiths, Roland R.},
  year = {1999},
  title = {Triazolam and zolpidem: Effects on human memory and
                  attentional processes},
  journal = {Psychopharmacology},
  volume = {144},
  number = {1},
  pages = {8-19},
  abstract = {Compared the acute effects of single oral doses of
                  the imidazopyridine hypnotic zolpidem (ZP) and the
                  benzodiazepine hypnotic triazolam (TZ) on specific
                  memory and attentional processes. Drug effects on
                  memory for target (focal) information and contextual
                  information (peripheral details surrounding a target
                  stimulus presentation) were evaluated using a source
                  monitoring paradigm, and drug effects on selective
                  attention mechanisms were evaluated using a negative
                  priming paradigm, in 18 healthy Ss (aged 20-49
                  yrs). TZ and ZP produced very similar dose-related
                  effects on memory for target information. Both TZ
                  and ZP impaired Ss' ability to remember whether a
                  word stimulus had been presented to them on the
                  computer screen or whether they had been asked to
                  generate the stimulus based on an antonym cue
                  (memory for the origin of a stimulus). Results
                  suggest that TZ, but not ZP, impaired memory for the
                  screen location of picture stimuli (spatial
                  contextual information). Although both TZ and ZP
                  increased overall reaction time in the negative
                  priming task, only TZ increased the magnitude of
                  negative priming relative to placebo. Findings have
                  implications for the cognitive and pharmacological
                  mechanisms underlying drug-induced deficits in
                  specific memory and attentional processes.},
  keywords = {benzodiazepine hypnotic triazolam vs
                  nonbenzodiazepine hypnotic zolpidem, memory \&
                  attention, healthy 20-49 yr olds Triazolam},
  tags = {psychology;np},
}
