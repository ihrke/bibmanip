@article{Williams1995,
  author = {Williams, Leanne M.},
  year = {1995},
  title = {Further evidence for a multidimensional personality
                  disposition to schizophrenia in terms of cognitive
                  inhibition},
  journal = {British Journal of Clinical Psychology},
  volume = {34},
  number = {2},
  pages = {193-213},
  abstract = {Examined the relation between cognitive inhibition
                  and schizotypal features using a negative priming
                  paradigm, based on A. R. Beech et al (see record
                  1992-15264-001), including repeat and semantic
                  conventional and negative priming conditions for
                  verbal word identification.  70 undergraduates
                  completed the Schizotypy Scale (STA), Magical
                  Ideation (MI) Scale, Revised Social Anhedonia (SoA)
                  Scale, and Physical Anhedonia (PhA) Scale. High STA,
                  MI, and SoA Ss showed a reduced or reversed repeat
                  and semantic negative priming effect compared to
                  moderately low STA Ss and Ss low on all scales,
                  respectively. High STA Ss and high MI and SoA Ss
                  showed an inhibitory effect. Weakened inhibitory
                  processes appear associated with schizotypal
                  features related to positive schizophrenic symptoms;
                  schizotypal traits paralleling negative
                  schizophrenic symptoms are linked to reduced
                  inhibition only when positive schizotypal features
                  are present.},
  keywords = {semantic negative priming \& cognitive inhibition \&
                  schizotypal features, normal 18-43 yr olds,
                  Australia},
  tags = {psychology;np},
}
@article{Williams1996,
  author = {Williams, Leanne M.},
  year = {1996},
  title = {Cognitive inhibition and schizophrenic symptom
                  subgroups},
  journal = {Schizophrenia Bulletin},
  volume = {22},
  number = {1},
  pages = {139-151},
  abstract = {Examined repetition and semantic priming when the
                  prime stimulus was attended or ignored with 44
                  schizophrenic patients. Ss completed the Schedule
                  for the Assessment of Positive Symptoms and the
                  Schedule for the Assessment of Negative
                  Symptoms. Disorganization, Reality Distortion, and
                  Episodic subgroups showed reversed negative priming
                  in unattended priming conditions and reversed
                  positive priming in the repetition condition, while
                  a Psychomotor Poverty group showed the usual
                  negative and positive priming effects. Weakening of
                  inhibitory processes may underlie both the reality
                  distortion and disorganization dimensions of
                  positive schizophrenic symptomatology. Negative
                  symptoms contributing to psychomotor poverty are not
                  linked to reduced inhibition.  The association of
                  positive symptoms with reversed positive priming
                  suggest that stimuli and task differences impact the
                  preattentive activation of information underlying
                  such priming.},
  keywords = {negative vs positive symptoms, repetition \&
                  semantic priming when prime stimulus was attended vs
                  ignored, schizophrenic 18-41 yr olds, Australia \&
                  England},
  tags = {psychology;np},
}
@article{Williams2001,
  author = {Williams, Lisa Dawn},
  year = {2001},
  title = {Component transfer: Positive and negative transfer
                  effects on categorization and recognition ability},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {61},
  number = {7},
  pages = {3875},
  abstract = {Prior experience of a word usually facilitates
                  performance on a later occasion. Roediger (e.g.,
                  1990) has provided a transfer appropriate processing
                  (TAP) account of this facilitation. By this account,
                  transfer depends on the type of task: some tasks
                  benefit from earlier perceptual processing, whereas
                  others require earlier conceptual processing.  An
                  earlier experience can also sometimes cause negative
                  transfer, as in negative priming experiments. In
                  this case, the effect is usually attributed to
                  temporary inhibition of a logogen-type
                  representation of the word (e.g., Tipper, 1985). In
                  the first series of experiments, long-term priming,
                  both positive and negative, are demonstrated within
                  a conceptual task. Neither account is prepared to
                  explain such effects.  Instead, I argue for a
                  component transfer account, in which the fundamental
                  principle determining transfer is the match of very
                  specific operations performed on earlier and later
                  occasions. The second set of experiments
                  investigates how recognition decisions are
                  influenced by speeded and slowed
                  performance. Recognition judgments have typically
                  been theorized to be the result of increased
                  specific familiarity of an item (i.e., fluency) or
                  through retrieval of encoding context. The present
                  studies demonstrate that nonfluent performance is
                  not necessarily detrimental for recognition.},
  keywords = {component transfer categorization recognition
                  ability transfer appropriate processing negative
                  priming logogen type word representation Print},
  tags = {psychology;np},
}
