@article{David2002,
  author = {David, Daniel and Brown, Richard J.},
  year = {2002},
  title = {Suggestibility and negative priming: Two replication
                  studies},
  journal = {International Journal of Clinical \& Experimental
                  Hypnosis},
  volume = {50},
  number = {3},
  pages = {215-228},
  abstract = {Research suggests that inhibiting the effect of
                  irrelevant stimuli on subsequent thought and action
                  (cognitive inhibition) may be an important component
                  of suggestibility. Two small correlation studies
                  were conducted to address the relationship between
                  different aspects of suggestibility and individual
                  differences in cognitive inhibition, operationalized
                  as the degree of negative priming generated by
                  to-be-ignored stimuli in a semantic categorization
                  task. A total of 121 undergraduate students
                  participated. The first study found significant
                  positive correlations between negative priming,
                  hypnotic suggestibility, and creative imagination; a
                  significant negative correlation was obtained
                  between negative priming and interrogative
                  suggestibility, demonstrating the discriminant
                  validity of the study results. The second study
                  replicated the correlation between negative priming
                  and hypnotic suggestibility, using a different
                  suggestibility measurement procedure that assessed
                  subjective experience and hypnotic involuntariness
                  as well as objective responses to suggestions. These
                  studies support the notion that the ability to
                  engage in cognitive inhibition may be an important
                  component of hypnotic responsivity and maybe of
                  other forms of suggestibility.},
  keywords = {cognitive inhibition hypnotic suggestibility
                  individual differences negative priming
                  Suggestibility Print Electronic},
  tags = {psychology;np},
}
