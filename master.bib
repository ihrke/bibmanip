@article{Arbuthnott1995a,
  author = {Arbuthnott, Katherine D.},
  year = {1995},
  title = {Inhibitory mechanisms in cognition: Phenomena and
                  models},
  journal = {Cahiers de Psychologie Cognitive/Current Psychology
                  of Cognition},
  volume = {14},
  number = {1},
  pages = {3-45},
  abstract = {Reviews behavioral evidence of inhibition in
                  cognitive processing and addresses the question of
                  what inhibitory mechanisms are responsible for these
                  effects. Inhibitory effects from 5 paradigms
                  (negative priming, lexical priming of ambiguous
                  words, negative error priming, directed forgetting,
                  and stop-signal tests) in the domains of selective
                  attention, language and number processing, and
                  memory and response management are reviewed. Two
                  mechanisms, lateral inhibition and self-inhibition,
                  are described as potential sources of the reviewed
                  effects, and possible means by which intention could
                  influence these inhibitory processes are
                  considered. ((c) 1997 APA/PsycINFO, all rights
                  reserved)},
  tags = {psychology;np},
