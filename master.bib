@article{Healy1998,
  author = {Healy, David and Farquhar, Grant},
  year = {1998},
  title = {Immediate effects of droperidol},
  journal = {Human Psychopharmacology: Clinical \& Experimental},
  volume = {13},
  number = {2},
  pages = {113-120},
  abstract = {Catalogued the subjective and behavioral responses
                  of 20 healthy volunteers (aged 23-48 yrs) taking
                  droperidol 5 mg as part of a cognitive challenge
                  program. Ss were part of a larger study in which the
                  effects of droperidol, lorazepam, and a placebo were
                  compared. In all cases Ss were blind to the
                  medication they took. The trial protocol involved 30
                  min spent at 3 negative priming tests delivered by
                  computer prior to drug administration, then 1 hr and
                  3 hrs after intake. Some form of akathisia was
                  universally experienced. Half of the Ss were
                  dysphoric, but there appeared to be a number of
                  different inputs to their dysphoria and a range of
                  other effects were noted, including sedation,
                  dissociative experiences, alterations in sensation
                  and subtle changes in physiognomy. The duration of
                  these effects varied from a few hours to over a
                  week. In the acute phase, insight as to the origin
                  of what was happening was mixed.  The results have
                  implications for the interpretation of cognitive
                  challenge tests, the nature of akathisia, clinical
                  therapeutics and drug development.},
