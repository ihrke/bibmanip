@article{Kahan1998,
  author = {Kahan, Todd Aaron},
  year = {1998},
  title = {Positive and negative priming from masked words:
                  Fuzzy memories of center-surround theory},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {59},
  number = {4},
  pages = {1876},
  abstract = {Masked priming effects were examined following a
                  session in which people made different kinds of
                  judgments about a masked word. In Experiment 1,
                  lexical decisions were made following a session of
                  detection and semantic judgments. The results
                  replicated Dagenbach, Carr, and Wilhelmson's (1989)
                  and Carr and Dagenbach's (1990) findings of positive
                  semantic and repetition priming following detection
                  judgments and positive repetition but negative
                  semantic priming following semantic judgments. In
                  Experiment 2, following repetition judgments,
                  negative priming for lexical decisions was now found
                  for repetition priming.  This result poses problems
                  for center-surround theory (Carr \& Dagenbach, 1990;
                  Dagenbach et al., 1989). In Experiments 3 and 4, a
                  pronunciation task was used, and the proportion of
                  related and repetition priming trials was
                  manipulated within the experimental list. When the
                  proportion of related priming trials was high
                  positive repetition but negative semantic priming
                  was found. When the proportion of repetition priming
                  trials was high positive semantic but negative
                  repetition priming was found. Results from
                  Experiments 1 and 3 replicate prior research (Carr
                  \& Dagenbach, 1990; Dagenbach et al., 1989) and
                  results from Experiments 2 and 4 support a memory
                  retrieval account over center-surround theory.},
  keywords = {Positive and negative priming from masked words:
                  Fuzzy memories of center-surround theory (semantic
                  priming, repetitive priming, visual masking)},
  tags = {psychology;np},
}
@article{Kahan2000,
  author = {Kahan, Todd A.},
  year = {2000},
  title = {Negative priming from masked words: Retrospective
                  prime clarification or center-surround inhibition?},
  journal = {Journal of Experimental Psychology: Learning,
                  Memory, \& Cognition},
  volume = {26},
  number = {6},
  pages = {1392-1410},
  abstract = {Masked repetition and semantic priming effects were
                  examined in 2 experiments. In Experiment 1, a
                  masked-prime lexical decision task followed a phase
                  of detection, semantic, or repetition judgments
                  about masked words. In Experiment 2 participants
                  made speeded pronunciations to target words after
                  they tried to identify masked primes, and the
                  proportion of semantically and identically related
                  prime-target pairs was varied. Center-surround
                  theory (T. H. Carr \& D. Dagenbach, 1990;
                  D. Dagenbach, T. H. Carr, \& A. Wilhelmsen, 1989)
                  predicts positive repetition priming but negative
                  semantic priming when people attempt, but fail, to
                  extract the meanings of masked words. A
                  retrospective prime-clarification account, in
                  contrast, predicts that semantic and repetition
                  priming effects will vary (being positive or
                  negative) as a function of expectations about the
                  prime-target relation. The data support a
                  retrospective prime-clarification account, which,
                  unlike center-surround theory, correctly predicted
                  negative repetition priming effects.},
  keywords = {retrospective prime-clarification vs center-surround
                  accounts of masked repetition \& semantic priming
                  effects during lexical decision tasks, college
                  students},
  tags = {psychology;np},
}
@article{Kahneman1992Reviewing,
  author = {Daniel Kahneman and Anne Treisman and Brian
                  J. Gibbs},
  year = {1992},
  title = {The reviewing of object files: Object-specific
                  integration of information},
  journal = {Cognitive Psychology},
  volume = {24},
  number = {2},
  pages = {175--219},
  month = {apr},
  doi = {DOI: 10.1016/0010-0285(92)90007-O},
  url = {http://dx.doi.org/10.1016/0010-0285(92)90007-O},
  abstract = {A series of experiments explored a form of
                  object-specific priming. In all experiments a
                  preview field containing two or more letters is
                  followed by a target letter that is to be named. The
                  displays are designed to produce a perceptual
                  interpretation of the target as a new state of an
                  object that previously contained one of the
                  primes. The link is produced in different
                  experiments by a shared location, by a shared
                  relative position in a moving pattern, or by
                  successive appearance in the same moving frame. An
                  object-specific advantage is consistently observed:
                  naming is facilitated by a preview of the target, if
                  (and in some cases only if) the two appearances are
                  linked to the same object. The amount and the object
                  specificity of the preview benefit are not affected
                  by extending the preview duration to 1 s, or by
                  extending the temporal gap between fields to 590
                  ms. The results are interpreted in terms of a
                  reviewing process, which is triggered by the
                  appearance of the target and retrieves just one of
                  the previewed items. In the absence of an object
                  link, the reviewing item is selected at random. We
                  develop the concept of an object file as a temporary
                  episodic representation, within which successive
                  states of an object are linked and integrated.},
  issn = {0010-0285},
  tags = {attention;binding;psychology},
  pdf = {pdf/Kahneman_1992_Reviewing_19693367.pdf},
}
@article{Kane1997,
  author = {Kane, Michael J. and May, Cynthia P. and Hasher,
                  Lynn and Rahhal, Tamara and E. R. Stoltzfus},
  year = {1997},
  title = {Dual mechanisms of negative priming},
  journal = {JEPHPP},
  volume = {23},
  number = {3},
  pages = {632-650},
  abstract = {Three experiments examined whether negative priming
                  is a dually determined effect produced by inhibitory
                  mechanisms and by a memorial process.  Younger
                  adults (Experiment 1) and older adults (Experiments
                  1-3) were tested in procedures that varied the
                  likelihood of inducing retrieval of the prior
                  trial. This was done by making test-trial target
                  decoding difficult (Experiments 1 \& 2) or by making
                  prior information useful on some nonnegative priming
                  trials (Experiment 3). Younger adults demonstrated
                  negative priming under retrieval and nonretrieval
                  conditions, with patterns of performance indicating
                  different sources of negative priming effects. Older
                  adults showed negative priming only under
                  retrieval-inducing conditions, consistent with the
                  view of deficient inhibitory mechanisms for older
                  adults.  The data suggest that contextual variables
                  critically determine whether negative priming is
                  largely due to inhibition or to episodic retrieval.},
  keywords = {episodic retrieval \& accessibility of critical
                  information from prime-display in naming tasks,
                  negative priming, 17-22 vs 60-76 yr olds},
  tags = {psychology;np},
  pdf = {pdf/Kane_1997.pdf},
  comment = {{\bf J{\"o}rg: NP; DM; Geronto} \\ {\bf Matthias: }
                  \\ {\bf Hecke: }},
}
