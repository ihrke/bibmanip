@article{Gamboz2000,
  author = {Gamboz, Nadia and Russo, Riccardo and Fox, Elaine},
  year = {2000},
  title = {Target selection difficulty, negative priming, and
                  aging},
  journal = {Psychology \& Aging},
  volume = {15},
  number = {3},
  pages = {542-550},
  abstract = {It has been recently suggested that the presence of
                  identity negative priming effects in old adults
                  could occur when there is substantial processing of
                  the distracting information in a selective attention
                  task (J. M. Kieley \& A. A. Hartley, 1997). In three
                  experiments, using a letter identification task, it
                  was found that making target selection more
                  difficult increased the magnitude of the negative
                  priming effect to a similar extent in both young and
                  old adults.  Moreover, the size of the negative
                  priming effect did not differ between young and
                  elderly participants. These results are discussed
                  with respect to the issue of age-related deficits in
                  the mechanisms underlying negative priming.},
