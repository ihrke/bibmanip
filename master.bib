@article{Stoddard1996,
  author = {Stoddard, Mitchell},
  year = {1996},
  title = {Selective attention and schizophrenia spectrum
                  disorders: A study of inhibitory deficits as
                  measured by negative priming},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {56},
  number = {9},
  pages = {5186},
  abstract = {The present study was designed to assess
                  hypothesized deficits in inhibition in selective
                  attention processes in relatively remitted recent
                  onset schizophrenics and in normals scoring high on
                  measures of schizotypal characteristics. In
                  Experiment 1, a negative priming paradigm was used
                  to assess differences in inhibitory processes in
                  schizophrenics (n = 25) and a psychiatrically
                  screened normal comparison sample (n = 14). In
                  Experiment 2, independent samples of controls
                  scoring high (n = 16) and low (n = 15) on measures
                  of schizotypal characteristics were contrasted on
                  the same priming measure. In the computerized
                  priming paradigm, subjects were presented with lists
                  of stimuli (colored words presented in contrasting
                  colors) and instructed to name as quickly as
                  possible, the color of text (target), and ignore the
                  competing irrelevant word (distractor). Stimuli were
                  presented at three presentation times (100, 250,
                  500ms) to assess the effect of stimulus availability
                  on priming processes. By manipulating the
                  relationship between the distractor and target
                  across trials, three priming conditions were
                  developed: negative, positive, and interference.
                  The magnitude of the RT slowing in the negative
                  priming condition and RT speeding in the positive
                  priming condition were used as indices of the extent
                  of inhibition applied to the previous stimuli. The
                  interference condition was designed to assess the
                  efficiency of selection in the face of
                  distraction. Schizophrenics and schizotypals did not
                  differ from controls with regard to efficiency of
                  selection. Schizophrenics and schizotypals also
                  failed to demonstrate deficient inhibitory processes
                  in the form of attenuated priming
                  effects. Furthermore, the magnitude of priming
                  effects was not found to be related to the presence
                  of positive or negative symptoms in the
                  schizophrenics.  Priming effects interacted with
                  stimulus presentation times, with negative priming
                  occurring exclusively with brief stimulus
                  presentation times, and posit},
  keywords = {inhibitory deficits in selective attention \&
                  priming processes, schizophrenics Personality
                  Processes},
  tags = {psychology;np},
}
@article{Stolz2001,
  author = {Stolz, Jennifer A. and Neely, James H.},
  year = {2001},
  title = {Taking a bright view of negative priming in the
                  light of dim stimuli: Further evidence for memory
                  confusion during episodic retrieval},
  journal = {Canadian Journal of Experimental Psychology},
  volume = {55},
  number = {3},
  pages = {219-230},
  abstract = {A same-different letter-matching task was used to
                  examine the effects of stimulus intensity on
                  negative priming, which is poorer performance when
                  target letters have been presented as distractor
                  letters on the immediately preceding trial. In Exp
                  1, with 68 college students, stimulus intensity was
                  manipulated between-participants, whereas in Exp 2,
                  with 32 college students, it varied randomly from
                  trial-to-trial within-participants. In Exp 1,
                  negative priming was equivalent for both stimulus
                  intensities. In Exp 2, negative priming effects were
                  larger for repeated intensity stimuli than for
                  nonrepeated intensity stimuli, when stimulus
                  intensity was dim. Furthermore, for repeated
                  intensity stimuli, negative priming effects were
                  enhanced when the overt response required to the
                  stimulus was repeated from prime to probe
                  trial. These results are consistent with the
                  hypothesis that negative priming may be due to
                  memory confusion, rather than to inhibition of the
                  distractor stimuli.},
  keywords = {stimulus intensity negative priming memory confusion
                  Print Electronic},
  tags = {psychology;np},
  pdf = {pdf/Stolz_Neely_2001.pdf},
  comment = {{\bf J{\"o}rg: } NP; lesen\\ {\bf Matthias: } \\
                  {\bf Hecke: }},
}
@article{Stone1998,
  author = {Stone, Maria and Ladd, Sandra L. and Vaidya, Chandan
                  J. and Gabrieli, John D. E.},
  year = {1998},
  title = {Word-identification priming for ignored and attended
                  words},
  journal = {Consciousness \& Cognition: An International
                  Journal},
  volume = {7},
  number = {2},
  pages = {238-258},
  abstract = {Three experiments, with 162 community college
                  students (aged 18-36 yrs), examined contributions of
                  study phase awareness of word identity to subsequent
                  word-identification priming (WIP) by manipulating
                  visual attention to words at study. In Exp 1, words
                  were presented briefly, and study phase negative
                  priming was used as evidence of word encoding of the
                  ignored words. Exps 2 and 3 used the Stroop color
                  naming task.  In Exp 1, WIP was reduced for ignored
                  relative to attended words, even though ignored
                  words were identified sufficiently to produce
                  negative priming in the study phase. WIP was also
                  reduced after color naming relative to emotional
                  valence rating (Exp 2) or word reading (Exp 3), even
                  though an effect of emotional valence upon color
                  naming (Exp 2) indicated that words were identified
                  at study. Thus, WIP was reduced even when word
                  identification occurred at study. WIP may depend on
                  awareness of word identity at the time of study.},
  keywords = {study phase awareness of word identity,
                  word-identification priming, 18-36 yr olds
                  Attention},
  tags = {psychology;np},
}
@article{Stone2007Categorical,
  author = {Stone, Anna and Valentine, Tim},
  year = {2007},
  title = {The categorical structure of knowledge for famous
                  people (and a novel application of Centre-Surround
                  theory)},
  journal = {Cognition},
  volume = {104},
  number = {3},
  pages = {535--564},
  month = {sep},
  doi = {10.1016/j.cognition.2006.07.014},
  url = {http://dx.doi.org/10.1016/j.cognition.2006.07.014},
  abstract = {Knowledge of familiar people is essential to guide
                  social interaction, yet there is uncertainty about
                  whether semantic knowledge for people is stored in a
                  categorical structure as for objects. Four priming
                  experiments using hard-to-perceive primes
                  investigated whether occupation forms a category
                  connecting famous persons in semantic memory. Primes
                  were famous faces exposed for 17ms with masking,
                  resulting in severely restricted awareness and thus
                  precluding expectancy-based priming effects. Targets
                  were consciously perceptible famous faces
                  (Experiments 1-3), famous names (Experiment 3), or
                  occupations (Experiment 4) representing either the
                  same or different occupation to the
                  prime. Significant priming demonstrated the
                  operation of automatic processes, including
                  spreading activation, among persons sharing a common
                  occupation; this supports the categorical view. The
                  direction of priming (faster/slower responses to
                  same-occupation than different-occupation targets)
                  was dependent on prime-target stimulus onset
                  asynchrony (Experiments 1-3) and type of target
                  (Experiment 4). This pattern of results is
                  attributed to the Centre-Surround mechanism proposed
                  by Carr and Dagenbach [Carr, T. H., \& Dagenbach,
                  D. (1990). Semantic priming and repetition priming
                  from masked words: evidence for a centre-surround
                  attentional mechanism in perceptual
                  recognition. Journal of Experimental Psychology:
                  Learning, Memory and Cognition, 16, 341-350]. These
                  results support (a) the categorical structure of
                  semantic knowledge for famous people and (b) the
                  application of the Centre-Surround mechanism to the
                  domain of person recognition.},
  issn = {0010-0277},
  tags = {face-recognition;psychology},
  pdf = {pdf/Stone_Valentine_2007.pdf},
}
@article{Stone2008,
  author = {Stone, Anna},
  year = {2008},
  title = {{Categorical priming of famous person recognition: a
                  hitherto overlooked methodological factor can
                  resolve a long-standing debate.}},
  journal = {Cognition},
  volume = {108},
  number = {3},
  pages = {874--80},
  month = {sep},
  doi = {10.1016/j.cognition.2008.06.001},
  abstract = {The Burton, Bruce and Johnston [Burton, A. M.,
                  Bruce, V., \& Johnston, R. A. (1990). Understanding
                  face recognition with an interactive activation
                  model. British Journal of Psychology, 81, 361-380]
                  model of person recognition proposes that
                  representations of known persons are connected by
                  shared semantic attributes. This predicts that
                  priming should be observed between persons from the
                  same category, e.g. famous persons with the same
                  occupation. Empirical investigations to date have
                  produced mixed results, and comparison of methods
                  suggests that priming based on shared occupation may
                  have been suppressed by the presence of prime-target
                  pairs representing a stronger relationship of close
                  association. In the present experiment, 72
                  participants performed a familiarity decision to
                  famous names preceded by close associates or members
                  of the same occupational category. As predicted,
                  categorical priming was observed in the group of
                  participants for whom the same occupation
                  prime-target pairs were presented before the
                  close-associate pairs, but not in the group for whom
                  the two types of relationship were
                  intermixed. Associate priming was significant in
                  both groups. These results are attributed to
                  differing levels of processing of the primes,
                  invoked by participants' observation of the most
                  salient prime-target relationship.},
  issn = {0010-0277},
  keywords = {Adolescent,Adult,Association
                  Learning,Face,Female,Humans,Male,Mental
                  Recall,Middle Aged,Occupations,Pattern
                  Recognition,Reaction Time,Semantics,Set
                  (Psychology),Visual},
  pdf = {pdf/Stone_2008.pdf},
  file = {:Users/ihrke/work/Papers/Mendeley/pdf/2008/Stone\_2008.pdf:pdf},
  pmid = {18649877},
}
@article{Stout2001,
  author = {Stout, Julie C. jcstout indiana edu Indiana U. Dept
                  of Psychology Bloomington I. N. U. S. and Wylie,
                  Scott A. and Simone, Patricia M. and Siemers, Eric
                  R.},
  year = {2001},
  title = {Influence of competing distractors on response in
                  Huntington's disease and Parkinson's disease},
  journal = {Cognitive Neuropsychology},
  volume = {18},
  number = {7},
  pages = {643-653},
  abstract = {Current theories of the basal ganglia suggest a
                  functional role in filtering stimuli that are
                  competing for response selection. We hypothesized
                  that damage to the basal ganglia, as occurs in
                  Huntington's disease (HD) and Parkinson's disease
                  (PD), may alter the effects of distractors on this
                  filtering process. 14 HD Ss (mean age 48.3 yrs old),
                  16 PD Ss (mean age 65.4 yrs old), and age-matched
                  healthy controls performed an ignored repetition
                  test of negative priming. Negative priming was
                  defined as a significant time cost in responding to
                  a target that shared features with the distractor
                  from the previous trial.  Results indicated that
                  whereas healthy controls and PD Ss showed normal
                  negative priming, HD Ss failed to show negative
                  priming. The results indicate that disruption to
                  cells in the neostriatum, but not necessarily to
                  cells in the substantia nigra, may affect selective
                  attention by altering the influence of distractor
                  stimuli competing for action.},
  keywords = {basal ganglia response selection Huntington's
                  disease Parkinson's disease negative priming Print
                  Electronic},
  tags = {psychology;np},
}
@article{Stout2002,
  author = {Stout, Julie C. jcstout indiana edu Indiana U. Dept
                  of Psychology Bloomington I. N. U. S. and Wylie,
                  Scott A. and Filoteo, J. Vincent},
  year = {2002},
  title = {Divergent findings regarding negative priming in
                  Parkinson's disease: A comment of Filoteo et al
                  (2000) and Wylie and Stout (2000)},
  journal = {Neuropsychology},
  volume = {16},
  number = {2},
  pages = {251-253},
  abstract = {This commentary discusses divergent findings in 2
                  articles published in this issue of
                  Neuropsychology. The studies used negative priming
                  (NP) to probe the associations between basal ganglia
                  function and cognition in Parkinson's disease (PD)
                  and tested different predictions about NP in
                  PD. Different NP tasks were used, and although the
                  subject samples appeared to have similar clinical
                  features, results were quite different. This
                  commentary, written jointly by the authors of the 2
                  studies (J. V. Filoteo, L. M. Rilling, \&
                  D. L. Strayer, 2002; S. A. Wylie \& J. C. Stout,
                  2002), describes a process by which their disparate
                  results may be used to facilitate the design of new
                  studies that may determine how specific features of
                  NP tasks lead to different findings in PD. The
                  results are a more systematic account of how task
                  features, such as specific response demands,
                  interact with the response selection processes that
                  are implemented by the basal ganglia.},
  keywords = {negative priming Parkinson's disease basal ganglia
                  cognition neurophysiology Print},
  tags = {psychology;np},
}
