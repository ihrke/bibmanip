@article{Fosse2003a,
  author = {Magdalena J Fosse and Roar Fosse and J. Allan Hobson
                  and Robert J Stickgold},
  year = {2003},
  title = {Dreaming and episodic memory: a functional
                  dissociation?},
  journal = {J Cogn Neurosci},
  volume = {15},
  number = {1},
  pages = {1--9},
  doi = {10.1162/089892903321107774},
  abstract = {The activity that takes place in memory systems
                  during sleep is likely to be related to the role of
                  sleep in memory consolidation and learning, as well
                  as to the generation of dream hallucinations. This
                  study addressed the often-stated hypothesis that
                  replay of whole episodic memories contributes to the
                  multimodal hallucinations of sleep. Over a period of
                  14 days, 29 subjects kept a log of daytime
                  activities, events, and concerns, wrote down any
                  recalled dreams, and scored the dreams for
                  incorporation of any waking experiences. While 65\%
                  of a total of 299 sleep mentation reports were
                  judged to reflect aspects of recent waking life
                  experiences, the episodic replay of waking events
                  was found in no more than 1-2\% of the dream
                  reports.  This finding has implications for
                  understanding the unique memory processing that
                  takes place during the night and is consistent with
                  evidence that sleep has no role in episodic memory
                  consolidation.},
  keywords = {Consciousn; Dissociative Disorders; Dreams; Female;
                  Humans; Life Change Events; Male; Memory,
                  Short-Term; Mental Processes; Models, Psychological;
                  Psychoanalytic Interpretation; Sleep Stages;
                  Wakefulness; ess},
  institution = {Laboratory of Neurophysiology, Harvard Medical
                  School, Massachusetts Mental Health Center, Boston
                  02115, USA. rfosse@hms.harvard.edu},
}
@article{Fox1994,
  author = {Fox, Elaine},
  year = {1994},
  title = {Attentional bias in anxiety: A defective inhibition
                  hypothesis},
  journal = {Cognition \& Emotion},
  volume = {8},
  number = {2},
  pages = {165-195},
  abstract = {Examined attentional bias in high trait-anxious
                  (HTA), low trait-anxious, and repressor Ss in 3
                  experiments. Exp 1 tested selective avoidance of
                  threat-related material, Exp 2 tested effects of
                  negative priming, and Exp 3 tested the magnitude of
                  negative priming. Exps 1, 2, and 3 involved 248, 81,
                  and 54 university students, respectively. Measures
                  of interference did not provide consistent
                  results. However, negative priming effects suggested
                  that HTA Ss had difficulty inhibiting threat-related
                  information, as well as nonthreat-related
                  distracting information under conditions of
                  attentional search. There was some evidence that Ss
                  with a repressive coping style were particularly
                  efficient in inhibiting threat-related
                  information. It is suggested that defective
                  inhibition of distracting information may be an
                  important mechanism in understanding the cognitive
                  basis of anxiety.},
  keywords = {negative priming \& magnitude \& selective avoidance
                  of threat related material, high vs low trait
                  anxious vs repressive college students, implications
                  for attentional bias Attention},
  tags = {psychology;np},
}
@article{Fox1994a,
  author = {Fox, Elaine},
  year = {1994},
  title = {Interference and negative priming from ignored
                  distractors: The role of selection difficulty},
  journal = {PP},
  volume = {56},
  number = {5},
  pages = {565-574},
  abstract = {Investigated whether various experimental
                  manipulations would influence measures of distractor
                  interference and negative priming in the same or in
                  opposite directions. In Exp 1, 20 Ss (aged 18-45
                  yrs) were presented with a letter-identification
                  task in which target-distractor separation on the
                  prime trial had been manipulated. In Exp 2, 12 Ss
                  (aged 19-25 yrs) were presented with a similar task
                  under conditions in which the location of the prime
                  target letter was either precued or uncued. Results
                  indicated that when to-be-ignored distractors were
                  presented close to the prime target, the magnitude
                  of interference and the subsequent negative priming
                  was greater than that when distractors were moved
                  farther away from the target. On the other hand,
                  precuing prime target location reduced the magnitude
                  of interference effects but did not decrease the
                  magnitude of subsequent negative priming relative to
                  uncued trials.},
  keywords = {target distractor separation \& cuing of prime
                  target letter, magnitude of interference from
                  ignored distractors \& subsequent negative priming
                  in target selection, 18-45 yr olds},
  tags = {psychology;np},
}
@article{Fox1995,
  author = {Fox, Elaine},
  year = {1995},
  title = {Negative priming from ignored distractors in visual
                  selection: A review},
  journal = {Psychonomic Bulletin \& Review},
  volume = {2},
  number = {2},
  pages = {145-173},
  abstract = {Reviews research on negative priming (NP) in
                  ignoring distractors during visual selection, in
                  order to consider the evidence for 3 hypotheses of
                  NP: Selective inhibition, feature mismatching and
                  episodic retrieval. The processing of ignored
                  information has been measured using the Stroop and
                  Flanker tasks and the NP paradigm. Research on
                  various aspects of NP, such as the level of
                  representation at which it operates, influencing
                  factors, persistence, survival over trials, and
                  individual differences, reveal that NP is a robust
                  effect, demonstrated across a range of stimuli, and
                  a variety of task demands.  None of the 3 can
                  provide a complete explanation for all the extant
                  data; however, the selective inhibition and episodic
                  retrieval hypotheses have a high degree of
                  psychobiological plausibility.},
  keywords = {selective inhibition vs feature mismatching vs
                  episodic retrieval hypotheses of negative priming in
                  visual selection, literature review Distraction
                  Episodic Memory},
  tags = {psychology;np},
  pdf = {pdf/Fox_1995.pdf},
}
@article{Fox1995a,
  author = {Fox, Elaine},
  year = {1995},
  title = {Pre-cuing target location reduces interference but
                  not negative priming from visual distractors},
  journal = {Quarterly Journal of Experimental Psychology},
  volume = {48},
  number = {1},
  pages = {26-40},
  abstract = {Investigated the impact of focusing attention on
                  negative priming and interference effects. In Exp 1,
                  24 university students were presented with a prime
                  display followed by a probe display. Exp 2
                  replicated Exp 1, but with 12 university students
                  and a within-Ss rather than between-Ss prime
                  display. Exp 3, with 14 undergraduates, removed the
                  possible artefact of forward masking arising from
                  the cuing stimuli.  In a letter-identification task,
                  reliable interference and negative priming effects
                  were observed from distractors. When the location of
                  the target in prime display was pre-cued,
                  interference effects were significantly reduced;
                  negative priming effects did not decrease.  Thus,
                  under optimal conditions, Ss can focus attention
                  efficiently on one spatial location and minimize
                  interfering effects of stimuli appearing in
                  irrelevant locations.},
  keywords = {precuing target location, interference \& negative
                  priming effects from visual distractors, college
                  students Visual Stimulation},
  tags = {psychology;np},
  pdf = {./pdf/Fox_1995.pdf},
}
@article{Fox1996,
  author = {Fox, Elaine},
  year = {1996},
  title = {Cross-language priming from ignored words: Evidence
                  for a common representational system in bilinguals},
  journal = {Journal of Memory \& Language},
  volume = {35},
  number = {3},
  pages = {353-370},
  abstract = {Two experiments examined cross-language priming from
                  ignored stimuli in 19 bilingual college students in
                  England. 11 Ss were native English speakers, while 8
                  Ss were native French speakers. In Exp 1, Ss
                  categorized a focally attended number while ignoring
                  flanking words during a prime trial and then made
                  lexical decisions to probe letter strings.  When
                  probe words were semantic associates of previously
                  ignored flanker words, cross-language negative
                  priming occurred only when the ignored flankers were
                  in the S's 1st language (FL), and the probe target
                  was in the 2nd language (SL). Using translation
                  equivalents, Exp 2 found that cross-language
                  negative priming occurred in FL-SL and SL-FL
                  conditions. However, there was still an asymmetry
                  with more negative priming occurring in the FL-SL
                  condition. Results suggest that bilingual speakers
                  access common conceptual representations across
                  languages and support a revised hierarchical model
                  of bilingual memory organization.},
  keywords = {unattended stimulus word, cross language semantic
                  priming of attended word, French English bilinguals,
                  England},
  tags = {psychology;np},
}
