@article{Healy2003,
  author = {Healy, Daniel and Burt, Jennifer S.},
  year = {2003},
  title = {Attending to the distractor and old/new
                  discriminations in negative priming},
  journal = {Quarterly Journal of Experimental Psychology},
  volume = {56},
  number = {3},
  pages = {421-443},
  abstract = {When participants ignore an irrelevant distractor
                  they typically show impaired responding to that item
                  if it becomes the relevant stimulus on a subsequent
                  trial. In Experiment 1 (N= 64), a masked white
                  colour name was presented briefly before a Stroop
                  display. Negative priming in colour naming occurred
                  when the colour of the lettering for the Stroop
                  stimulus matched the colour name displayed in the
                  first display, consistent with the proposal of
                  temporal discrimination theory that negative priming
                  arises because a recurrence of an unattended
                  stimulus cannot readily be classified as old or
                  new. Experiment 2 (N = 32) replicated negative
                  priming in the interleaved word display where
                  participants had to name the red word from a pair of
                  red and green words. In Experiment 3 (N= 32) and
                  Experiment 4 (N= 28) the participants were required
                  to attend to but not respond to the words in the
                  prime display and name one of two interleaved words
                  in the probe display.  Negative priming was observed
                  in this arrangement, consistent with the episodic
                  retrieval theory of negative priming. The temporal
                  discrimination model may need to be extended to
                  situations in which the attended stimuli have
                  different responses attached to them.},
  keywords = {negative priming temporal discrimination irrelevant
                  distractor unattended stimulus episodic retrieval
                  stimulus classification retrieval theory attended
                  stimuli Stroop stimulus prime display Episodic
                  Memory Temporal Lobe Visual Discrimination Print
                  Electronic},
  tags = {psychology;np},
  comment = {{\bf Hecke: }{\bf J{\"o}rg: } \\ {\bf Matthias: } \\
                  {\bf Hecke: } Zitat von Frings: Awareness in NP
                  paradigms. Haben auch maskierte primes gezeigt,
                  dieselben Ergebnisse wie Milliken98.},
}
