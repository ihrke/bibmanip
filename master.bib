}
@article{Duscherer2002,
  author = {Katia Duscherer and Daniel Holender},
  year = {2002},
  title = {No negative semantic priming from unconscious
                  flanker words in sight.},
  journal = {J Exp Psychol Hum Percept Perform},
  volume = {28},
  number = {4},
  pages = {839--853},
  abstract = {In replicating 1 of the within-language conditions
                  of E. Fox's (1996) Experiment 1, the authors
                  confirmed that unattended words presented 2.4
                  degrees above and below fixation are mostly
                  unavailable to awareness.  However, no negative
                  semantic priming was observed in a lexical decision
                  on a probe letter string appearing about 1 s later,
                  which does not replicate Fox's finding. These
                  results are compatible with the hypothesis
                  underlying the present study, according to which
                  positive semantic priming, if any, rather than
                  negative semantic priming is expected in Fox's
                  situation. The reason is that unavailability to
                  awareness of the parafoveal words is not achieved by
                  means of an act of selective inhibition combined
                  with attentional diversion through masking but is
                  achieved simply by means of perceptual degradation.},
  keywords = {Affect; Attention; Fixation, Ocular; Humans;
                  Perceptual Masking; Random Allocation; Semantics;
                  Unconscious (Psychology); Visual Perception;
                  Vocabulary},
}
@article{Duvuru2003,
  author = {Duvuru, Prathyusha},
  year = {2003},
  title = {Effect of stimulus similarity on activation and
                  inhibition of target and distractor features versus
                  their locations},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {63},
  number = {11},
  pages = {5554},
  abstract = {The primary objectives of the current research were
                  to examine the relative roles of excitatory and
                  inhibitory mechanisms (attributable to target and
                  distractor features as well as to their locations)
                  in visual search and how they interact with search
                  difficulty. Koshino (2001) observed evidence for
                  both target feature activation and distractor
                  features inhibition in conjunctive search, but
                  inhibition was evident only when search was
                  difficult (i.e., when there were a large number of
                  distractors in the display). Maljkovic and Nakayama
                  (1994), however, observed inhibition of distractor
                  features in a relatively easy search task using
                  pop-out targets. In the present studies search
                  difficulty was manipulated within each experiment by
                  varying target-distractor similarity and
                  distractor-distractor similarity. In all three
                  experiments, search displays were presented as prime
                  and probe pairs. Between prime and probe trials, the
                  targets and distractors were identical, reversed, or
                  completely unrelated. The target and distractors in
                  the prime trial were either physically similar or
                  dissimilar, and the distractors were either
                  homogenous or heterogeneous. In Experiment 1a, the
                  magnitude of facilitation (positive priming) was
                  greater when targets and distractors were dissimilar
                  (search was easy) than when they were similar
                  (search was difficult). However, the opposite was
                  true for inhibition (negative priming) effects. In
                  Experiment 1b, in addition to manipulating target
                  and distractor features between the prime and probe
                  trials, their locations were also systematically
                  manipulated, such that the target in the probe trial
                  was presented either in the same location as in the
                  prime trial, in one of the locations occupied by a
                  prime distractor, or in one of the previously
                  unoccupied locations. Target search was facilitated
                  when the target appeared in the same location in
                  both prime and probe trials, but there was no cost
                  associated with presenting the probe target in a
                  previous distractor location. Experiment 2 was a
                  replication of Experiment 1b, except that stimulus
                  similarity was held uniform in the probe trials and
                  manipulated only in the prime trials. Unlike in
                  Experiments 1a and 1b, the strength of distractor
                  feature inhibition did not vary statistically across
                  the similarity conditions, although the level of
                  inhibition observed was highest when the target and
                  distractors were similar. The results suggest that
                  the amount of inhibition observed may be a function
                  of search difficulty in both prime and probe
                  trials. Activation between trials was observed for
                  both featural and location information, but
                  inhibition was observed for featural information
                  only.},
  keywords = {stimulus similarity target feature activation
                  distractor features inhibition excitatory mechanisms
                  inhibitory mechanisms visual search Print},
  tags = {psychology;np},
}
