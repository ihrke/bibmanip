@article{Hughes2003,
  author = {Hughes, Robert and Jones, Dylan M.},
  year = {2003},
  title = {A negative order-repetition priming effect:
                  Inhibition of order in unattended auditory
                  sequences?},
  journal = {JEPHPP},
  volume = {29},
  number = {1},
  pages = {199-218},
  abstract = {A novel negative priming (NP) effect is reported in
                  which serial recall for a sequence of visually
                  presented digits was poorer if the same sequence was
                  presented as an irrelevant auditory sequence on the
                  previous trial (Experiments 1 and 2). The effect was
                  enhanced when attention was divided between the
                  to-be-repeated auditory sequence and the concurrent
                  to-be-remembered (TBR) sequence (Experiment 3).
                  When the TBR sequences were also presented
                  auditorily, NP arose only when the repeated TBR
                  sequence was in the same voice as the previous
                  irrelevant sequence; a voice mismatch produced
                  positive priming (Experiment 4). The results suggest
                  that the order of auditory events is registered
                  preattentively and that inhibition may be applied to
                  the acoustic transitions between irrelevant events.},
  keywords = {negative priming effect serial recall response
                  inhibition unattended auditory sequences attention
                  distraction Interference (Learning) Reaction Time
                  Selective Attention Print},
  tags = {psychology;np},
  comment = {{\bf J{\"o}rg: } \\ {\bf Matthias: } \\ {\bf Hecke:
                  }},
}
@article{Hutchison2002,
  author = {Hutchison, Keith Allen},
  year = {2002},
  title = {Effects of association strength and set size on
                  positive and negative semantic priming},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {63},
  number = {1},
  pages = {558},
  abstract = {Attending to a stimulus often leads to either
                  identity or semantic positive priming by
                  facilitating future responses to identical or
                  semantically related stimuli. In addition, whereas
                  attending to stimuli facilitates performance,
                  ignoring an item on a prime trial can actually hurt
                  future performance on either that same item or
                  related items, producing identity or semantic
                  negative priming. However, the evidence for semantic
                  negative priming is scarce and the crucial factors
                  producing this effect are currently unknown. The
                  present experiments extend previous research by
                  examining the influence of association strength and
                  set size on both positive and negative semantic
                  priming. In Experiment 1, participants made lexical
                  decisions to "asymmetrically associated"
                  prime-targets pairs that contain a strong
                  association in one direction (e.g., from "stork" to
                  "baby"), but not in the other direction (e.g., from
                  "baby" to "stork"). These items were presented to
                  participants in either the forward (e.g.,
                  "stork-baby") or backward (e.g., "baby-stork')
                  direction. The critical new finding was that both
                  positive and negative semantic priming effects were
                  obtained only when prime-target pairs were presented
                  in the forward direction. Using a categorization
                  task, Experiment 2 again found positive semantic
                  priming, but failed to obtain negative semantic
                  priming suggesting the effect may depend upon the
                  use of a "binary" response such as a lexical
                  decision. Experiment 3 again used a categorization
                  task, but obtained both positive and negative
                  semantic priming using a small set of frequently
                  repeated strong associates. The pattern of semantic
                  negative priming observed across all three
                  experiments supports either (a) a "reaction
                  inhibition" model in which items associated to an
                  ignored prime distractor are inhibited during prime
                  selection or (b) a version of episodic retrieval
                  theory in which items associated to the ignored
                  prime distractor are tagged "do not respond" and the
                  future presentation of one of those items cues the
                  retrieval of the past "do not respond tag."},
  keywords = {association strength set size semantic priming
                  Print},
  tags = {psychology;np},
}
@article{Hutchison2002a,
  author = {Hutchison, Keith A.},
  year = {2002},
  title = {The effect of asymmetrical association on positive
                  and negative semantic priming},
  journal = {Memory \& Cognition},
  volume = {30},
  number = {8},
  pages = {1263-1276},
  abstract = {One's actively ignoring a stimulus can impair
                  subsequent responding to that stimulus. This
                  negative priming effect has been argued to
                  generalize to semantically related items as well,
                  but the evidence for this is still somewhat
                  weak. This article presents a new experiment in
                  which participants made lexical decisions to
                  asymmetrically associated prime-target pairs
                  presented in either the forward (e.g., stork-baby)
                  or backward (e.g., baby-stork) direction. The
                  critical new finding was that both attended positive
                  and ignored negative semantic priming occurred only
                  for prime-target pairs presented in the forward
                  direction.  The results support either (1) a
                  spreading inhibition model in which items associated
                  with an ignored distractor are inhibited during
                  prime selection or (2) a version of episodic
                  retrieval theory in which the prime distractor and
                  items associated with it are tagged as
                  "to-be-ignored" during prime selection.},
  keywords = {asymmetrical association positive semantic priming
                  negative semantic priming Print Electronic},
  tags = {psychology;np},
}
