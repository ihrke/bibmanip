@article{Fletcher1997,
  author = {P. C. Fletcher and C. D. Frith and M. D. Rugg},
  year = {1997},
  title = {The functional neuroanatomy of episodic memory.},
  journal = {Trends Neurosci},
  volume = {20},
  number = {5},
  pages = {213--218},
  abstract = {Functional neuroimaging studies have revealed that
                  effective encoding in episodic memory is associated
                  with enhanced activity in left prefrontal cortex,
                  whereas retrieval is accompanied by the enhancement
                  of predominantly right-sided prefrontal
                  activity. The extent of the contribution of
                  prefrontal cortex to episodic memory, and the fact
                  that the encoding and retrieval operations it
                  supports are differentially lateralized, were
                  unexpected on the basis of evidence from lesion
                  studies. Such studies have highlighted the crucial
                  role in episodic memory played by the hippocampus
                  and related medial temporal lobe
