@article{Park1994,
  author = {Park, Jooyong and Kanwisher, Nancy},
  year = {1994},
  title = {Negative priming for spatial locations: Identity
                  mismatching, not distractor inhibition},
  journal = {JEPHPP},
  volume = {20},
  number = {3},
  pages = {613-623},
  abstract = {Negative priming (NP) is commonly thought to occur
                  because distractor inhibition is necessary for
                  target selection (the distractor inhibition
                  hypothesis). Contrary to this account, the selection
                  of a target in the preceding trial is shown to be
                  neither necessary (Exp 1) nor sufficient (Exps 2 and
                  3) for NP in a target localization task modeled
                  after S. P. Tipper et al (see record
                  1991-00251-001). Exps 4 and 5 provide further
                  evidence against the distractor inhibition
                  hypothesis and support an alternative mismatching
                  account: NP in the spatial selection task apparently
                  results from a change in the symbol bound to a given
                  location (D. Kahneman et al; see record
                  1992-37866-001), rather than a change in the status
                  of that location from distractor to target (Tipper
                  et al).},
  keywords = {distractor interference, negative priming in target
                  localization, adults},
  tags = {psychology;np},
  comment = {{\bf J{\"o}rg: } \\ {\bf Matthias: } \\ {\bf Hecke:
