@article{Nemenman2004,
  author = {Nemenman, Ilya and Bialek, William and Rob de Ruyter
                  van Steveninck},
  year = {2004},
  title = {Entropy and information in neural spike trains:
                  Progress on the sampling problem},
  journal = {Physical Review E},
  publisher = {American Physical Society},
  volume = {69},
  number = {5},
  pages = {056111+},
  month = {may},
  day = {24},
  doi = {10.1103/PhysRevE.69.056111},
  url = {http://dx.doi.org/10.1103/PhysRevE.69.056111},
  abstract = {The major problem in information theoretic analysis
                  of neural responses and other biological data is the
                  reliable estimation of entropy-like quantities from
                  small samples. We apply a recently introduced
                  Bayesian entropy estimator to synthetic data
                  inspired by experiments, and to real experimental
                  spike trains. The estimator performs admirably even
                  very deep in the undersampled regime, where other
                  techniques fail. This opens new possibilities for
                  the information theoretic analysis of experiments,
                  and may be of general interest as an example of
                  learning from limited data.},
  tags = {information-theory, neuroscience, spikes},
  pdf = {pdf/Nemenman_2004_Entropy_19661375.pdf},
}
@article{Neubauer1999,
  author = {Neubauer, Dana Lynn},
  year = {1999},
  title = {Differential processing efficiency in anxiety and
                  depression: An investigation of the cognitive
                  mechanism underlying working memory capacity
                  restrictions},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {60},
  number = {4},
  pages = {1865},
  abstract = {Although increased anxiety is associated with
                  decreased performance on a wide range of cognitive
                  tasks, it does not adversely affect all cognitive
                  processes to the same degree. Eysenck's cognitive
                  theory of anxiety (e.g., Eysenck \& Calvo, 1992)
                  accounts for these discrepant findings and
                  postulates that anxiety restricts working memory
                  (WM) capacity. According to this theory, the amount
                  of WM capacity required for successful performance
                  on a task determines whether cognitive processes are
                  affected by anxiety. Although recent research
                  supports Eysenck's theory, no investigation to date
                  elucidates the precise cognitive mechanism
                  underlying this restriction in WM. The goal of this
                  study was to extend Eysenck's theory by
                  investigating a possible mechanism underlying WM
                  restriction in anxiety. The mechanism investigated
                  in the present study functions to prevent irrelevant
                  information from entering into WM and to dismiss
                  information from WM when it is no longer considered
                  relevant to the task at hand. Both aspects of
                  functioning are critical to the cognitive
                  functioning of an individual.  Because WM is limited
                  in the amount of information that it can hold at any
                  one time, cognitive functioning will be impaired on
                  tasks that utilize relatively large amount of WM
                  capacity when valuable space in this system is
                  occupied by non-task relevant information.  It was
                  predicted that both aspects of the functioning of
                  this mechanism would be least efficient for
                  threatening material in anxious individuals and most
                  efficient for depressotypic information in depressed
                  individuals.  No differences in efficiency for
                  different word types were expected in normal control
                  individuals. To investigate these issues, this study
                  adapted a variant of a negative priming paradigm as
                  well as a variant of Sternberg's (1966) short-term
                  memory search paradigm for use with emotional words
                  and emotionally distressed participants.  The
                  findings from this study supported the initial
                  predictions: anxiety and depression are clearly
                  characterized by differential efficiency in the
                  mechanism thought to underlie restrictions in WM
                  capacity.  Implications of these findings and future
                  directions for research in this area are discussed.},
  keywords = {processing efficiency, working memory capacity,
                  anxious Ss},
  tags = {psychology;np},
}
@article{Neumann1991,
  author = {Neumann, Ewald and DeSchepper, Brett G.},
  year = {1991},
  title = {Costs and benefits of target activation and
                  distractor inhibition in selective attention},
  journal = {Journal of Experimental Psychology: Learning,
                  Memory, \& Cognition},
  volume = {17},
  number = {6},
  pages = {1136-1145},
  abstract = {Three experiments examined the relative
                  contributions of positive and negative priming in a
                  selective attention task. A typical trial consisted
                  of a briefly presented letter display containing a
                  red target letter and a distractor letter of another
                  color. This prime display was followed by another
                  similar letter display (probe). When the red target
                  letters were identical in both displays,
                  facilitation from identity repetition priming was
                  obtained. When the unattended letter from the prime
                  display became the probe target letter, response
                  times were delayed (i.e., a negative priming effect
                  was found). Other combinations of target and
                  distractor letters were manipulated to examine the
                  relative influences of positive and negative
                  priming.  Results support the hypothesis that
                  selective attention involves a selective inhibition
                  mechanism and suggest that this inhibitory mechanism
                  can be as influential as its excitatory
                  counterpart.},
  keywords = {positive vs negative priming \& distraction,
                  selective attention, college students},
  tags = {psychology;np},
  pdf = {pdf/Neumann_Deschepper_1991.pdf},
}
@article{Neumann1992,
  author = {Neumann, Ewald and Deschepper, Brett G.},
  year = {1992},
  title = {An inhibition-based fan effect: Evidence for an
                  active suppression mechanism in selective attention},
  journal = {Canadian Journal of Psychology},
  volume = {46},
  number = {1},
  pages = {1-40},
  abstract = {Tested an inhibition-based fan effect hypothesis,
                  using a negative priming paradigm in Exps 1 and 2
                  and a short-term memory scanning paradigm in Exp
                  3. In Exp 1 (24 undergraduates) and Exp 2 (48
                  undergraduates), the time to name a letter
                  (surrounded by 1-3 distractor letters) was longer
                  when it had been a distractor on the previous
                  display than in a control condition where the target
                  letter had not been one of the distractors in the
                  previous display. This negative priming effect
                  attenuated as the number of distractors in the
                  previous display increased. The juxtaposition of an
                  irrelevancy with a relevancy heuristic supports the
                  possible existence of a spreading inhibition
                  counterpart of spreading activation. Several
                  predictions based on this framework were confirmed
                  among 3 undergraduates in a modified short-term
                  memory scanning task in Exp 3. (French abstract)},
  keywords = {number of distractors, negative priming \& short
                  term memory scanning \& selective attention in
                  letter naming RT task, college students, test of
                  inhibiton based fan effect Letters (Alphabet) Naming
                  Priming Reaction Time Short Term Memory},
  tags = {psychology;np},
}
@article{Neumann1993,
  author = {Neumann, Ewald and Cherau, Julie F. and Hood,
                  Kristin L. and Steinnagel, Schaunel L.},
  year = {1993},
  title = {Does inhibition spread in a manner analogous to
                  spreading activation?},
  journal = {Memory},
  volume = {1},
  number = {2},
  pages = {81-105},
  abstract = {Explored limited capacity inhibitory selective
                  attention processes in working memory in 2
                  experiments. Exp 1, using 6 college students, used a
                  modified Sternberg-type "short-term memory scanning"
                  task, where both irrelevant and relevant memory-set
                  words were included to see if an inhibitory fan
                  effect operated on lexical associates of the
                  should-be-ignored (irrelevant) words. Exp 2 (with 46
                  college students) used a "negative priming" task,
                  where a target letter to be named was flanked by 1,
                  2, or 3 distractor letters to see if an inhibitory
                  fan effect operated on the should-be-ignored
                  letters.  Results from both studies supported the
                  existence of a limited capacity spreading inhibition
                  counterpart to spreading activation. The findings
                  were discussed in terms of a model recently proposed
                  by Neumann and B. G. DeSchepper (eg., 1991; 1992) in
                  which 2 selective attention subprocesses (1
                  excitatory and 1 inhibitory) in the brain each
                  maximize opposed functions within their respective
                  resource limitations in working memory. A list of
                  the word pairs used is appended.},
  keywords = {limited capacity inhibitory selective attention
                  processes in working memory, college students},
  tags = {psychology;np},
}
@article{Neumann1999Cross,
  author = {Neumann, Ewald and McCloskey, Michael S. and Felio,
                  Amy C.},
  year = {1999},
  title = {Cross-language positive priming disappears, negative
                  priming does not: Evidence for two sources of
                  selective inhibition},
  journal = {MEMCOG},
  volume = {27},
  number = {6},
  pages = {1051-1063},
  abstract = {The authors used a unilingual and bilingual primed
                  lexical decision task to investigate priming effects
                  produced by attended and ignored words. Ss were
                  college students. In the unilingual experiment,
                  accelerated lexical decisions to probe target words
                  resulted when the word matched the preceding target
                  word, whereas slowed lexical decisions to probe
                  target words resulted when the word matched the
                  preceding ignored nontarget word. In the bilingual
                  (English-Spanish) experiment, between-language,
                  rather than within-language, priming manipulations
                  were used. Although the ignored repetition negative
                  priming effect replicated across languages,
                  cross-language attended repetition positive priming
                  did not. This dissociation of priming effects in the
                  inter- versus intralanguage priming conditions
                  contradicts episodic retrieval accounts of negative
                  priming that deny the existence of selective
                  inhibitory processes. On the other hand, these
                  results support an extension of inhibition-based
                  accounts of negative priming, because they indicate
                  that inhibition can operate at two levels of
                  abstraction--local word and global
                  language--simultaneously.},
  tags = {psychology;np},
}
@article{Neumann2003,
