@article{McDowd1991,
  author = {McDowd, Joan M. and Oseas-Kreger, Deborah M.},
  year = {1991},
  title = {Aging, inhibitory processes, and negative priming},
  journal = {Journals of Gerontology},
  volume = {46},
  number = {6},
  pages = {P340-P345},
  abstract = {The hypothesis that older adults experience a
                  decline in the efficiency of inhibitory processes
                  was tested in a negative priming (NP) experiment.
                  NP is interpreted as being due to the previous
                  inhibition of a subsequent target item. An
                  experiment involving a visual selective attention
                  task with an NP condition was conducted with 20
                  college students and 20 older adults (mean age 73.7
                  yrs). A letter-reading task with partially
                  superimposed red and green letters was used to
                  assess age differences in NP. Ss read red letters
                  and ignored green letters.  Results show a
                  significant interaction between age and the effect
                  of distracting information, with older adults being
                  more slowed by the presence of green letters than
                  young adults. However, older adults failed to show
                  NP effects, whereas young adults did show NP.},
  keywords = {inhibitory processes \& negative priming in visual
                  selective attention letter reading task, college
                  students vs elderly Age Differences},
  tags = {psychology;np},
}
@article{McDowd1993,
  author = {McDowd, Joan M. and Filion, Diane L. and Harris,
                  M. Jackuelyn and Braff, David L.},
  year = {1993},
  title = {Sensory gating and inhibitory function in late-life
                  schizophrenia},
  journal = {Schizophrenia Bulletin},
  volume = {19},
  number = {4},
  pages = {733-746},
  abstract = {Reviews studies of prepulse inhibition, habituation,
                  latent inhibition, and negative priming that
                  indicate marked similarities in the patterns of
                  deficits observed in schizophrenia and in aging. New
                  data are presented from preliminary studies of
                  prepulse inhibition and negative priming that
                  compared young normal controls, older adult normal
                  controls, and late-life schizophrenia patients. For
                  both measures, schizophrenia patients and older
                  adults showed an inhibitory deficit relative to
                  young controls. In the case of negative priming,
                  older schizophrenia patients showed evidence of
                  greater inhibitory dysfunction than did normal older
                  adults, suggesting that older schizophrenia patients
                  suffer a deficit resulting from the combined effects
                  of schizophrenia and aging.},
  keywords = {sensory gating assessed by prepulse inhibition \&
                  negative priming, older adults with late life
                  schizophrenia},
  tags = {psychology;np},
}
@article{McDowd1995,
  author = {McDowd, Joan M. and Filion, Diane L.},
  year = {1995},
  title = {Aging and negative priming in a location suppression
                  task: The long and short of it},
  journal = {Psychology \& Aging},
  volume = {10},
  number = {1},
  pages = {34-47},
  abstract = {The role of temporal parameters in aging and
                  inhibitory function was examined using negative
                  priming (NP) and repetition priming (RP) in a task
                  involving a series of visually presented prime-probe
                  sets; responses were made on the basis of the
                  location of a target stimulus.  Preparatory
                  intervals (PI) preceding prime-probe sets were 3,000
                  ms or 1,500 ms. The longer PI resulted in less NP in
                  older adults than did the shorter PI. Further
                  analyses suggest that older adults may be less
                  prepared to inhibit the distractor following the
                  longer PI. The longer PI also produced more RP than
                  did the shorter PI for both age groups, indicating a
                  greater emphasis on facilitatory processes in this
                  condition. In addition, evidence was obtained to
                  suggest that an inverse relationship between NP and
                  RP exists for both young and old adults. These data
                  suggest that individuals or task conditions may
                  emphasize either facilitation or inhibition in
                  selection.},
  keywords = {negative vs repetition priming in visual location
                  task, effect of aging on inhibitory function in
                  suppression, young vs older adults Distraction
                  Visual Stimulation},
  tags = {psychology;np},
}
