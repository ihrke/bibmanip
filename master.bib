  month = {apr},
  url = {http://www.sciencedirect.com/science?\_ob=ArticleURL\&\_udi=B6VNP-3WKY242-5\&\_user=5731894\&\_rdoc=1\&\_fmt=\&\_orig=search\&\_sort=d\&view=c\&\_acct=C000043105\&\_version=1\&\_urlVersion=0\&\_userid=5731894\&md5=6c6a0432692f706efd306fbacaa0ca3c},
  abstract = {OBJECTIVE: To evaluate the relationship between some
