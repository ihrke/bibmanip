@article{Kobari1998,
  author = {Kobari, Hiroyuki},
  year = {1998},
  title = {Priming effect as related to difficulty in selection
                  for overlapping stimuli manipulated by the relative
                  line thickness of attended and unattended figures},
  journal = {Perceptual \& Motor Skills},
  volume = {86},
  number = {3},
  pages = {1219-1230},
  abstract = {Examined how difficulty of ignoring unattended
                  stimulus affects negative priming in 2
                  experiments. Ss were presented overlapping stimuli
                  consisting of 2 figures of different colors and were
                  required to attend to 1 of the figures while
                  ignoring the other. The difficulty in selection was
                  manipulated by line thickness. Priming effect was
