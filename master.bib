@article{Houghton1996,
  author = {Houghton, George and Tipper, Steven P. and Weaver,
                  Bruce and Shore, David I.},
  year = {1996},
  title = {Inhibition and interference in selective attention:
                  Some tests of a neural network model},
  journal = {Visual Cognition},
  volume = {3},
  number = {2},
  pages = {119-164},
  abstract = {Examined the empirical background to a model
                  focusing on the relationship between inhibition and
                  the control of interference (INT). This neural
                  network model of selective attention is described
                  which employs a combined excitatory/inhibitory
                  architecture. A series of predictions regarding the
                  phenomena of INT and negative priming (NP) are then
                  derived from a simplified version of the model,
                  which focuses on the operation of one particular
                  aspect of it. Three experiments tested these
                  predictions. Exp 1, using 20 male Ss, tested the
                  prediction that representations of unattended
                  objects only drop below resting levels after the
                  stimulus is no longer present. There was a trend
                  towards increased INT and decreased NP when the
                  prime distractor remains on until the probe is
                  presented. In Exp 2, using 25 male Ss, it was
                  predicted that attended repetition effects will not
                  interact with the number of distracting
                  stimuli. Results of Exp 2 provide support for the
                  simulation. Both INT and NP are influenced by the
                  number of distractors in the prime display, whereas
                  attended priming effects are not. Using 20 male Ss,
                  Exp 3 examined stimulus input increases in strength
                  INT. An increase in INT was substantially larger
                  than the increase in NP.},
  keywords = {inhibition \& control of interference, neural
                  network model of selective attention, adult males},
  tags = {psychology;np},
  comment = {{\bf J{\"o}rg: } \\ {\bf Matthias: } \\ {\bf Hecke:
                  }},
}
@article{Houghton1996a,
  author = {G. Houghton and S. P. Tipper},
  year = {1996},
  title = {{Inhibitory Mechanisms of Neural and Cognitive
                  Control: Applications to Selective Attention and
                  Sequential Action}},
  journal = {Brain and Cognition},
  publisher = {Elsevier Inc.},
  volume = {30},
  number = {1},
  pages = {20--43},
  doi = {10.1006/brcg.1996.0003},
  abstract = {This paper discusses the role of inhibition in
                  neural and cognitive control, in particular its role
                  in selective processes in perception and action. We
                  first review neuroanatomical and physiological
                  evidence that cortical control is mediated by a
                  variety of local-circuit inhibitory neurons,
                  distributed throughout all layers and areas of the
                  cortex.  We then consider the use of inhibition in
                  control processes in behavioral neural network
                  models, focusing on two areas: selective attention
                  and sequential action. Relations between the
                  architecture and dynamics of these models and
                  relevant neurological findings are discussed.  We
                  conclude that a full understanding of inhibitory
                  control of mental functioning will require the
                  integration of data from both the behavioral and
                  neural levels, and that formal neural network models
                  can play an important role in bridging this
                  epistemological divide.},
  keywords = {Attention; Cerebral Cortex; Cognition; Humans;
                  Inhibition (Psychology)},
}
