@article{Schacter2004,
  author = {Daniel L Schacter and Ian G Dobbins and David M
                  Schnyer},
  year = {2004},
  title = {Specificity of priming: a cognitive neuroscience
                  perspective.},
  journal = {Nat Rev Neurosci},
  volume = {5},
  number = {11},
  pages = {853--862},
  doi = {10.1038/nrn1534},
  abstract = {Priming is a nonconscious form of memory that
                  involves a change in a person's ability to identify,
                  produce or classify an item as a result of a
                  previous encounter with that item or a related item.
                  One important question relates to the specificity of
                  priming - the extent to which priming reflects the
                  influence of abstract representations or the
                  retention of specific features of a previous
                  episode. Cognitive neuroscience analyses provide
                  evidence for three types of specificity: stimulus,
                  associative and response. We consider empirical,
                  methodological and conceptual issues that relate to
                  each type of specificity, and suggest a theoretical
                  perspective to help in guiding future research.},
  keywords = {Association, Cognition, Cues, Humans, Memory,
