@article{Chawarski1993,
  author = {Chawarski, Marek C. and Sternberg, Robert J.},
  year = {1993},
  title = {Negative priming in word recognition: A context
                  effect},
  journal = {Journal of Experimental Psychology: General},
  volume = {122},
  number = {2},
  pages = {195-206},
  abstract = {In 5 experiments using a priming methodology, the
                  role of contextual factors on Ss' performance in a
                  word-recognition task was investigated.  Ss read
                  short stories, and then their recognition of words
                  from the stories was tested. Effects of contextual
                  factors on Ss' performance were examined by
                  manipulating the context of the stories'
                  presentation and by designing the experimental
                  materials to weaken the effects of semantic
                  relations between primes and targets, thereby
                  enhancing Ss' opportunity to use contextual
                  relations between the words. The overall results of
                  all 5 experiments indicate that context influences
                  the priming effect of close semantic relations. They
                  can be interpreted as supporting cue-retrieval
                  models of priming mechanisms.},
  keywords = {contextual factors, performance on primed word
                  recognition task, college students},
  tags = {psychology;np},
}
@article{Chawarski1996,
  author = {Chawarski, Marek Cezary},
  year = {1996},
  title = {Priming and negative priming in active memory},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {56},
  number = {8},
  pages = {4575},
  abstract = {This dissertation presents the results of three
                  laboratory experiments conducted to investigate
                  priming phenomena in active memory. The study
                  employs a modification of the standard direct
                  priming method which made it possible to focus the
                  investigation more on conceptual similarities
                  between primes and targets than on their perceptual
                  similarities. The results indicate that phenomena
                  similar to long-term memory priming can be observed
                  in subjects' performance on a task that does not
                  require long-term memory and that can be
                  successfully accomplished by using only one's
                  working memory. In Experiments 1 and 2 positive
                  priming was observed for targets preceded by
                  conceptually identical primes, whereas in Experiment
                  3 negative priming was observed for such items. In
                  the first two experiments subjects were deciding
                  upon responses on the basis of the abstract meaning
                  of a target item, whereas in the last experiment
                  they were using a combination of the meaning of a
                  target item and the location of the corresponding
                  physical stimulus on the screen when deciding on how
                  to respond. Additionally, the study compares two
                  different views on active memory. One view is
                  represented by the hypothesis that active memory is
                  just the activated portion of long-term memory; the
                  second view, by the hypothesis that active memory is
                  a separate and functionally different system than
                  the LTM. The study also compares theoretical
                  implications of various types of models of priming
                  (e.g., spreading-activation and compound-cue models)
                  with the experimental results. It is argued here
                  that the combined results of the three experiments
                  can be best explained by a new model of priming. The
                  model proposed in this dissertation postulates that
                  active memory is a separate processing platform
                  where temporary representations of items are
                  generated and kept available for use by other
                  cognitive processes as manipulative elements. These
                  transient representations are created by combining
                  long-term memory represen},
  keywords = {priming phenomena in active memory, humans Long Term
                  Memory},
  tags = {psychology;np},
}
@article{Chiappe1995,
  author = {Chiappe, Dan L. and MacLeod, Colin M.},
  year = {1995},
  title = {Negative priming is not task bound: A consistent
                  pattern across naming and categorization tasks},
  journal = {Psychonomic Bulletin \& Review},
  volume = {2},
  number = {3},
  pages = {364-369},
  abstract = {Explored the extent to which negative priming
                  depends on the task that the S performs during the
                  prime and the probe trials. Ss were 72
                  undergraduates. Tasks were manipulated factorially
                  on the prime and probe trials, resulting in 4
                  groups: name-name, name-categorize, categorize-name,
                  and categorize-categorize. Results show equivalent
                  negative priming of about 22 msec both within and
                  between tasks for identical words, but no negative
                  priming for semantically related words from the same
                  category. Findings suggest that negative priming for
                  identical words can readily cross task types and
                  that semantic negative priming does not occur for
                  words, at least when categorical relatedness alone
                  determines the semantic relation.},
  keywords = {name vs categorization tasks during prime vs probe
                  trials, semantic \& identity negative priming,
                  college students, Canada Semantic Priming},
  tags = {psychology;np},
}
@article{Choudhry2004,
  author = {Choudhry, Ravi Kiran},
  year = {2004},
  title = {Script analysis and planning function in Parkinson's
                  disease: Is it a frontal disorder?},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {64},
  number = {10},
  pages = {5269},
  abstract = {Several studies have indicated that the basal
                  ganglia are involved in adaptive control of action
                  at both the cognitive and motor levels.  This study
                  examined 'forward planning' in patients with
                  Parkinson's Disease (PD) using scripts as familiar
                  and novel activities, and compared results with
                  Grafman's (1989) model of schematic representation
                  of knowledge and Shallice's (1982) functional
                  model. Five activities ranging in degree of
                  familiarity were studied. Twenty-two non-demented,
                  medically treated, early stage patients with
                  Parkinson's disease, matched for age, IQ, and
                  education normal controls were assessed on five
                  tasks: script generation; importance of action;
                  script fluency; recognition; and word
                  arrangement. Patients with PD scored lower than
                  normal control participants on verbal reasoning,
                  motor function, semantic fluency, but endorsed
                  higher levels of depression. PD patients were also
                  impaired on the frontal behaviours (Apathy, and
                  Executive Dysfunction) and on two prospective memory
                  subscales (Techniques to Remember and Short-Term
                  Habitual) compared to a group of normal
                  participants. In the Script Generation task,
                  participants were asked to generate actions
                  belonging to five different scripts. An effect of
                  script familiarity was observed in the PD group. PD
                  patients generated fewer actions in a novel script
                  compared to normal controls, but they did not differ
                  in the mean generation time. Also patients with
                  Parkinson's disease were impaired in evaluating how
                  important each script event was within the context
                  of goal-oriented activity. In the Script Fluency
                  task, impairment was observed in the PD patients
                  when they were asked to generate words for each
                  activity. Familiarity of the script did not account
                  for this deficit. These results suggest negative
                  priming effects in script fluency. PD patients were
                  as accurate as the normal controls at discriminating
                  between semantically related, target, and
                  semantically unrelated words. These results
                  confirmed that patients with PD have intact
                  immediate recognition memory for scripts. As
                  predicted, PD patients made more sequencing errors
                  in the novel script than did normal control
                  participants. The implications of these findings are
                  discussed in terms of Shallice's functional and
                  Grafman's structural models. The evidence suggests
                  that the prefrontal cortex and the basal ganglia are
                  differentially involved in planning.  Future
                  directions are suggested.},
  keywords = {planning function Parkinsons disease frontal
                  disorder Print},
  tags = {psychology;np},
}
@article{Christie2001,
  author = {Christie, John and Klein, Raymond M.},
  year = {2001},
  title = {Negative priming for spatial location?},
  journal = {Canadian Journal of Experimental Psychology},
  volume = {55},
  number = {1},
  pages = {24-38},
  abstract = {Examined negative priming for spatial location in 2
                  studies. Study 1 involved combinations of target,
                  distractor, or both, across prime and probe, being
                  presented once to each S in a negative priming for
                  spatial location procedure. Specifically, stimuli
                  were presented using an oscilloscope controlled by a
                  computer system, and the fixation display appeared
                  immediately after a foot pedal was depressed. After
                  500 msec, the prime array was added to the fixation
                  display until the S responded (depressing the key
                  corresponding to the location of the target). In the
                  1st Exp, the procedure was examined across a number
                  of Ss (12 university students; aged 20-30 yrs). In
                  the 2nd Exp, the procedure was tested over repeated
                  sessions with 1 S (university student; aged 23 yrs)
                  on consecutive days. Study 2 verified the results in
                  13 university students. The findings suggest that
                  negative priming in the spatial location procedure
                  may be more closely related to inhibition of return,
                  or to the automatic attraction of attention by new
                  objects, than to the concepts of distractor
                  inhibition, episodic retrieval, and feature mismatch
                  which have traditionally been used to explain
                  negative priming for spatial location.},
  keywords = {negative priming for spatial location, university
                  students Print Electronic},
  tags = {psychology;np},
  pdf = {pdf/Christie_Klein_2001.pdf},
}
@article{Christie2004,
  author = {Christie, John Joseph},
  year = {2004},
  title = {On the heterogeneity of negative priming effects},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {64},
  number = {9},
  pages = {4660},
  abstract = {Negative priming from ignored distractors is a
                  phenomenon that typically occurs when one processes
                  the target and distractor on one trial (prime) and
                  then is presented with the prime distractor as a
                  target on the next trial (probe). The current line
                  of research was initially launched to investigate
                  two hypotheses. One of these is that the various
                  paradigms that are used to investigate negative
                  priming from ignored distractors may not be tapping
                  the same psychological mechanisms.  The second is
                  that it may be possible to isolate a particular
                  paradigm where the negative priming is caused by a
                  selective attention mechanism.  In order to avoid
                  confound and contingency problems all six of the
                  possible prime and probe relationships were run,
                  including the one typically used to measure negative
                  priming. While puzzling over the best use for the
                  data in the five extra conditions a pattern of
                  performance was derived that can be interpreted as a
                  signature of negative priming.  The pattern uses the
                  six related conditions and does not include the
                  Control condition. There is a general consensus
                  about this signature pattern, called the congruency
                  principle here, among negative priming
                  explanations. The congruency principle states that
                  performance should worsen as the relationship
                  becomes more incongruous (e.g., the distractor and
                  target switch). And, it should improve as the
                  relationship between prime and probe becomes more
                  congruous (e,g., target and distractor both
                  repeat). The findings revealed that three of the
                  most common paradigms for investigating negative
                  priming, location, letter identity, and Stroop,
                  probably have different causes. Furthermore, a
                  biased version of the letter identity paradigm
                  appears to have an attentional cause when measured
                  using a subtraction from a Control condition.  It
                  was further found that, in the unbiased letter
                  identification paradigm, negative priming could be
                  measured using the congruency principle even though
                  it was not present when measured using subtraction
                  from the Control condition. Negative priming is not
                  homogeneous and sometimes it is caused by selective
                  attention. (Abstract shortened by UMI.)},
  keywords = {heterogeneity negative priming selective attention
                  distraction Print},
  tags = {psychology;np},
}
