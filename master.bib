@article{Tallon-Baudry1996,
  author = {C. Tallon-Baudry and O. Bertrand and C. Delpuech and
                  J. Pernier},
  year = {1996},
  title = {Stimulus specificity of phase-locked and
                  non-phase-locked 40 Hz visual responses in human.},
  journal = {J Neurosci},
  volume = {16},
  number = {13},
  pages = {4240--4249},
  abstract = {Considerable interest has been raised by
                  non-phase-locked episodes of synchronization in the
                  gamma-band (30-60 Hz). One of their putative roles
                  in the visual modality is feature-binding. We tested
                  the stimulus specificity of high-frequency
                  oscillations in humans using three types of visual
                  stimuli: two coherent stimuli (a Kanizsa and a real
