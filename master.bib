@article{Wong2000,
  author = {Wong, Kin Fai Ellick},
  year = {2000},
  title = {Dissociative prime-probe contextual similarity
                  effects on negative priming and repetition priming:
                  A challenge to episodic retrieval as a unified
                  account of negative priming},
  journal = {Journal of Experimental Psychology: Learning,
                  Memory, \& Cognition},
  volume = {26},
  number = {6},
  pages = {1411-1422},
  abstract = {This study examined the dependence of repetition
                  priming (RP) and negative priming (NP) as a function
                  of prime-probe contextual similarity in a paradigm
                  in which participants were required to respond to a
                  letter flanked by incompatible distractor letters
                  (e.g., ABA). Experiment 1 used prime and probe
                  displays containing a pair of "+" symbols that were
                  presented horizontally or vertically. Experiments 2
                  and 3 manipulated whether the letter triplets
                  contained the "!" symbol.  In all experiments,
                  regardless of whether the RP trials were intermixed
                  with the NP trials (Experiment 2) or not (Experiment
                  3), RP was stronger in the prime-probe similar
                  conditions than in the prime-probe dissimilar
                  conditions, but NP was independent of prime-probe
                  contextual similarity.  These findings suggest that
                  NP is not necessarily stronger in conditions in
                  which episodic retrieval of the prime is more
                  likely.},
  keywords = {dissociative prime-probe contextual similarity
                  effects, negative \& repetition priming \& episodic
                  retrieval during target \& distractor letter tasks,
                  college students Visual Displays},
  tags = {psychology;np},
  comment = {JB; NP;},
}
@article{Wood1998Negative,
  author = {Wood, Timothy J. and Milliken, Bruce},
  year = {1998},
  title = {Negative priming without ignoring},
  journal = {Psychonomic Bulletin \& Review},
  publisher = {Springer New York},
  volume = {5},
  number = {3},
  pages = {470--475},
  month = {sep},
  day = {1},
  doi = {10.3758/BF03208824},
  url = {http://dx.doi.org/10.3758/BF03208824},
  abstract = {Negative priming is conventionally defined by slowed
                  responses to a target item that appeared previously
                  as a distractor. As a result, it is widely assumed
                  that negative priming is caused by an act of
                  ignoring. Three experiments using a total of 101
                  students are reported in which novel abstract shapes
                  were studied with either "shallow" or "deep"
                  encoding instructions. This study phase was followed
                  by a same-different discrimination task similar to
                  that employed by B. DeSchepper and A. Treisman
                  (1996). Same-different discrimination was slower for
                  old than for new target shapes, and this negative
                  priming effect depended on the difficulty of the
                  discrimination task. Results suggest that negative
                  priming may not be caused by the ignoring of a prime
                  stimulus.},
  issn = {1069-9384},
  keywords = {shallow vs deep encoding instructions,
                  same-different discrimination task performance \&
                  negative priming, undergraduates},
  tags = {binding;psychology;np},
  pdf = {pdf/Wood_1998_Negative_19701690.pdf},
}
