  volume = {49},
  number = {1},
  pages = {30-37},
  abstract = {Investigated the relationship between cognitive
                  inhibition (negative priming) and hypnotic
                  responsiveness. 66 undergraduates were assigned to
                  an intentional forgetting group or a control
                  group. The experimental group were presented with a
                  list of 20 words cued as to-be-remembered or
