@article{Hsu1997,
  author = {Hsu, J. Y. and Tzeng, O. J. L. and Hung, D. L.},
  year = {1997},
  title = {Repetition facilitation, but not suppression of
                  ignored stimulus during selective attention:
                  Evidence from negative priming task in old adults},
  journal = {Chinese Journal of Psychology},
  volume = {39},
  number = {2},
  pages = {139-156},
  abstract = {Examined the priming effects of irrelevant
                  distractor identities and their locations in
                  selective attention to studying distractor identity
                  suppression and distractor identity location
                  inhibition and aging.  The reaction time (RT) of 52
                  Chinese undergraduate and graduate students (aged
                  19-29 yrs) and 25 Chinese retired teachers or
                  federal workers (aged 64-81 yrs) were compared in 2
                  Exps using 672 common Chinese characters as targets
                  and distractors. In Exp 1 Ss were asked to read
                  and/or point to the target position of an irrelevant
                  target-distractor pair in 4 conditions. Exp 2 was
                  similar to Exp 1 except 3 visual distances between
                  target and distractor were added. Results of the 2
                  experiments showed the positive priming of
                  distractor identity and the negative priming of
                  distractor location. Even young Ss did not present
                  significantly different negative priming effect from
                  the elderly Ss when a short distance appeared
                  between the target and the distractor. Results were
                  discussed with the hypothesis of negative priming.},
  keywords = {distractor repetition \& target \& distractor
                  locations in negative priming task, selective
                  attention \& inhibition, 19-29 vs 64-81 yr olds},
  tags = {psychology;np},
}
@article{Hsu1999,
  author = {Hsu, J. Y. and Tzeng, O. J. L. and Hung, D. L.},
  year = {1999},
  title = {Different priming effects of ignored stimulus during
                  selective attention: Roles of stimulus
                  representation time and properties},
  journal = {Chinese Journal of Psychology},
  volume = {41},
  number = {1},
  pages = {39-52},
  abstract = {Studied the inhibition of irrelevant distractor
                  objects during elective attention by examining
                  different priming effects of character, color, and
                  location of distractor objects with unrepeated
                  stimuli in different time presentations. 56
                  university students in Taiwan were presented targets
                  and distractors in 9 ways of grouping (repetitions
                  of color; target; position; repetitions of color and
                  target; target and position; color and position;
                  repetition of all; and 2 neutral controls (one
                  nonrepetition and the other with targets only in
                  priming presentation)) in a priming and a detection
                  presentation at 175 msec and 350 msec.  The targets
                  and distractors, selected from 468 Chinese
                  characters at 1,056-11,4733 frequency, were
                  presented in red, green, blue, or yellow and in the
                  same or different positions on the computer screen.
                  Ss read the targets within 500 msec after they
                  appeared in priming presentation and in unlimited
                  time in detection presentation. Ss' RTs and ways of
                  grouping of target and distractors were analyzed
                  with factorial ANOVA. Results showed negative
                  priming in repetitions of color and location at 174
                  msec; positive priming in character; and identical
                  repetition at 350 msec. The implication of the
                  results and the existing theories of negative
                  priming were discussed.},
  keywords = {time presentation \& priming effects of character \&
                  color \& location of irrelevant distractor objects
                  during selective attention, college students},
  tags = {psychology;np},
}
