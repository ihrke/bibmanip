@article{Cirneci2000,
  author = {Cirneci, Dragos and Meloi, Adela},
  year = {2000},
  title = {Computational modeling of selective attention in
                  anxiety},
  journal = {Cognitie Creier Comportament},
  volume = {4},
  number = {4},
  pages = {371-386},
  abstract = {Discusses an adaptation of a neural network model of
                  inhibitory mechanisms in selective attention
                  (G. Houghton \& S. P. Tipper, 1994) that provides a
                  detailed computational account of negative priming
                  and inhibition-of-return.  This model postulates
