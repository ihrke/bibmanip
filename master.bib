@article{Wright2006,
  author = {Christopher I. Wright and Nancy J. Keuthen and Cary
                  R. Savage and Brian Martis and Danielle Williams and
                  Michelle Wedig and Katherine McMullin and Scott
                  L. Rauch},
  year = {2006},
  title = {Brain correlates of negative and positive
                  visuospatial priming in adults},
  journal = {NeuroImage},
  volume = {30},
  number = {3},
  pages = {983-991},
  abstract = {A balance of inhibitory and facilitatory mechanisms
                  is essential for efficient and goal-directed
                  behaviors. These mechanisms may go awry in several
                  neuropsychiatric disorders characterized by
                  uncontrolled, repetitive behaviors. The visuospatial
                  priming paradigm is a well-established probe of
                  inhibition and facilitation that has been used to
                  demonstrate behavioral deficits in patients with
                  Tourette syndrome and obsessive-compulsive
                  disorder. However, the brain correlates of this
                  visuospatial priming paradigm are not yet well
                  established. In the present study, we used a
                  visuospatial priming paradigm and event-related
                  functional MRI, to probe inhibitory and facilitatory
                  brain mechanisms in healthy adult women. When
                  subjects performed the negative priming (i.e.,
                  inhibitory) task, several regions of the prefrontal
                  cortex were selectively activated relative to the
                  neutral condition. Non-overlapping regions of the
                  prefrontal cortex were deactivated in the positive
                  priming condition. These results support the notion
                  that the prefrontal cortex is involved in both
                  inhibitory and facilitatory processing and
                  demonstrate that this visuospatial priming task
                  shares brain correlates with other positive and
                  negative priming tasks. In conjunction with
                  functional MRI, this visuospatial priming task may
                  be useful for studying the pathophysiology of
                  neuropsychiatric disorders in which deficient
                  inhibitory processing or excessive facilitation is a
                  feature.},
  keywords = {Trichotillomania; Obsessive-compulsive disorder;
                  Inhibition; Visuospatial priming; Female},
  tags = {psychology;np},
}
@article{Wylie2002,
  author = {Scott A Wylie and Julie C Stout},
  year = {2002},
  title = {Enhanced negative priming in Parkinson's disease.},
  journal = {Neuropsychology},
  volume = {16},
  number = {2},
  pages = {242--250},
  abstract = {In the ignored repetition paradigm, negative priming
                  (NP) is defined as the increase in response time
                  that occurs when the current target stimulus served
                  as a distractor stimulus in the previous trial. In
                  this study, 25 Parkinson's disease (PD) participants
                  and 17 age-matched healthy controls (HCs) were
                  tested using a touchscreen version of the ignored
                  repetition task that allowed response time to be
                  partitioned into response initiation and response
                  execution segments. In both groups, NP effects were
                  stronger in the response execution than in the
                  response initiation segments. The most striking
                  result was that the PD group showed larger NP
                  effects overall than the HC group.  In PD, clinical
                  ratings of bradykinesia, but not tremor, were
                  related to larger NP effects. Results indicate that
                  in PD, disruption of dopamine neuromodulation
                  diminishes response efficiency when action must be
                  directed toward previously ignored information.},
  keywords = {Cognition, Cues, Dementia, Female, Humans,
                  Hypokinesia, Male, Middle Aged, Parkinson Disease,
                  Psychiatric Status Rating Scales, Psychomotor
                  Performance, Reaction Time, 11949716},
  tags = {psychology;np},
  comment = {{\bf J{\"o}rg: } \\ {\bf Matthias: } \\ {\bf Hecke:
                  }},
}
@article{Wylie2003,
  author = {Wylie, Scott Alan},
  year = {2003},
  title = {Testing an action selection model of basal ganglia
                  function in Parkinson's disease and Huntington's
                  disease},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {63},
  number = {12},
  pages = {6137},
  abstract = {The basal ganglia are a collection of subcortical
                  brain structures that have been suggested to play an
                  important role in action selection.  According to
                  this model, the basal ganglia receive inputs from
                  widespread cortical areas that represent action
                  commands. The role of the basal ganglia is to select
                  a "winning" command to guide action while preventing
                  competing commands from influencing action. A series
                  of studies investigated changes in the efficiency of
                  action selection in individuals with basal ganglia
                  disease (e.g., Parkinson's disease (PD),
                  Huntington's disease (HD)). In study 1, the negative
                  priming effect, a response time slowing that occurs
                  during the selection of an action associated with a
                  previously ignored distractor, was enhanced in PD (n
                  = 25) and reduced in HD (n = 12) relative to healthy
                  control groups. In study 2, the flanker effect, a
                  response time slowing that occurs when an action to
                  a target stimulus is made in the presence of
                  distractors associated with a competing action, was
                  exaggerated in both PD (n = 18) and HD (n = 13)
                  relative to healthy control groups. However, the
                  slowing in PD was changed to larger response time
                  facilitation relative to healthy controls when the
                  correct action was associated with the distractors
                  and opposite to the target stimulus. Interesting
                  relationships between the altered negative priming
                  and flanker effects and clinical symptoms in PD
                  (e.g., bradykinesia) and HD (e.g., chorea) were
                  observed. The findings are considered within an
                  action selection model of basal ganglia function and
                  suggest that alterations in cognitive processes that
                  resolve the conflict between competing actions may
                  be a useful way to characterize some of the
                  cognitive and clinical sequelae associated with
                  human basal ganglia disease.},
  keywords = {basal ganglia Parkinson's disease Huntington's
                  disease target stimulus healthy control subcortical
                  brain structures negative priming effect cognitive
                  processes Brain Cerebral Atrophy Cognitive Processes
                  Priming Print},
  tags = {psychology;np},
}
@article{Xu2002,
  author = {Xu, Yaoda},
  year = {2002},
  title = {Object parts in visual short-term memory and visual
                  search},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {63},
  number = {1},
  pages = {574},
  abstract = {Often in visual perception, the unit of selection
                  and representation is object-based. Examples include
                  selective attention, visual short-term memory
                  (VSTM), and effects like inhibition of return,
                  negative priming, repetition priming, repetition
                  blindness, and visual object tracking.  Disorders in
                  patients with visual neglect or Balint's syndrome
                  are also object-based. A weakness of many studies
                  reporting object-based effects is that the stimulus
                  objects did not contain any parts, unlike the real
                  world objects that our visual system has to analyze
                  constantly.  The goal of the present thesis,
                  therefore, is to characterize object-based
                  representation in VSTM when the stimuli include
                  objects with parts.  Chapter 1 laid the foundation
                  by showing that features are retained in VSTM in a
                  graded manner, and that features from different
                  dimensions are retained independently. Chapter 2
                  studied the encoding of color and shape features and
                  showed that object-based representation in VSTM is
                  modulated by the spatial relation of the features,
                  such that features are best retained if they are
                  from the same part of an object, less well retained
                  if they are from different parts of an object, and
                  least well retained if they are from two spatially
                  separated objects. Chapter 3 studied the encoding of
                  two features on the same dimension (2 colors or 2
                  orientations per object) in VSTM and found that
                  encoding does not benefit from being from the same
                  object: They are not retained any better than the
                  same features on two spatially separate
                  objects. Chapter 4 extended the findings of Chapter
                  2 to immediate perception in visual search
                  tasks. Together, these results showed th at
                  object-based representation in VSTM is modulated by
                  whether features are located on the same or
                  different parts of the object.  Some of the effects
                  found are not limited to VSTM, they are also found
                  in immediate perception. This thesis indicates that
                  many of the object-based effects reported in visual
                  perception will have to be revised when objects with
                  parts are included in the study.  (Copies available
                  exclusively from MIT Libraries, Rm. 14-0551,
                  Cambridge, MA 02139-4307. Ph. 617-253-5668; Fax
                  617-253-1690.)},
  keywords = {object parts visual short term memory visual search
                  Print},
  tags = {psychology;np},
}
@article{Yang1995,
  author = {Yang, Zhiliang and Ye, Gewei},
  year = {1995},
  title = {A study on implicit memory of Chinese characters:
                  II: Task dissociation and response bias},
  journal = {Acta Psychologica Sinica},
  volume = {27},
  number = {1},
  pages = {1-8},
  abstract = {Studied the discrimination ability (A') and response
                  bias (B") of preference discrimination (implicit
                  memory) tests and recognition (explicit memory)
                  tests with task dissociation paradigm. Ss were 60
                  undergraduate students from East Normal University,
                  Shanghai, China. Ss were asked to response to 50
                  pairs of Chinese characters presented at 250 ms, 500
                  ms, 500 ms, or 750 ms in discrimination tests and
                  recognition tests with uncued characters
                  (nontargets) and cued characters (targets) as
                  attention variables. Ss' reaction time (RT)s and
                  rate of accuracy in different test conditions and
                  between discrimination and recognition tests were
                  studied with ANOVA and paired comparisons. It is
                  reported (1) that a cross over dissociation was
                  found to demonstrate implicit memory with a
                  process-pure assumption, although the criterion of
                  the matched comparison method was not met; (2) that
                  a significant positive priming effect was found in
                  cued characters at 500 ms; a significant negative
                  priming effect in uncued characters at 750 ms in
                  preference discrimination tests; and (3) that a
                  stable mental state accompanied both forms of memory
                  when implicit and explicit memory occurred, however,
                  the bias of preference tests was more liberal and
                  that of the recognition tests was more
                  conservative.},
  keywords = {implicit memory cued Chinese characters response
                  bias reaction time recognition tests priming effect
                  learning Print},
  tags = {psychology;np},
}
@article{Yee2000,
  author = {Yee, Penny L. and Santoro, Kristine E. and Grey, Amy
                  L. and Woog, Vanessa},
  year = {2000},
  title = {Deeper processing at target selection increases the
                  magnitude of negative priming},
  journal = {Memory \& Cognition},
  volume = {28},
  number = {8},
  pages = {1429-1436},
  abstract = {Do deeper levels of processing produce equivalent
                  priming effects at all stages of task performance?
                  In Exp 1, the authors varied the level of processing
                  factorially across 2 task stages--target selection
                  and response selection. Each stage required
                  perceptual (e.g., color) or conceptual (e.g.,
                  friendliness) processing of stimulus items (i.e.,
                  animal names). Negative priming was substantially
                  greater when deeper processing was required at the
                  target selection stage, but it was unaffected by the
                  level of processing at the response selection stage.
                  In contrast, positive priming was greater when
                  deeper processing was required at the response
                  selection stage, but it was unaffected by processing
                  at the target selection stage. In Exp 2, the authors
                  generalized this finding using a task in which
                  numeric targets were selected on the basis of their
                  parity. As in Exp 1, the deeper level of processing
                  at the target selection stage produced a larger
                  negative priming effect. These results illuminate
                  the role of target selection demands in modulating
                  the strength of negative priming. Ss in Exp 1 were
                  63 male and 81 female college students and Ss in Exp
                  2 were 22 male and 18 female college students.},
  keywords = {target selection \& response selection \& perceptual
                  or conceptual processing of stimulus items \&
                  magnitude of negative priming Responses Print
                  Electronic},
  tags = {psychology;np},
  pdf = {pdf/Yee_2000.pdf},
}
@article{Yeh2000,
  author = {Yeh, Yei-Yu and Huang, Chin-Lan and Chao, Hsuan-Fu},
  year = {2000},
  title = {Interactive effects of location and identity
                  information in negative priming},
  journal = {Chinese Journal of Psychology},
  volume = {42},
  number = {2},
  pages = {217-231},
  abstract = {Used a negative priming paradigm to examine the
                  interactive effect of location and identity
                  activation on selective attention based on location
                  information. In addition to manipulating all
                  possible combinations in laying target and
                  distractor locations between the prime and probe
                  displays, the stimulus set size was manipulated
                  between two experiments, with novel stimuli used in
                  Experiment 1 and with repetitive presentation of
                  fourteen stimuli in Experiment 2. The results showed
                  interactive effects between location activation,
                  identity information, and information retrieved from
                  past episodes. No theoretical model can account for
                  all of the results, showing the selection mechanism
                  as adaptive and intricate.},
