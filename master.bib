@article{Ortells1996,
  author = {Ortells, Juan J. and Tudela, Pio},
  year = {1996},
  title = {Positive and negative sematic priming of attended
                  and unattended parafoveal words in a lexical
                  decision task},
  journal = {Acta Psychologica},
  volume = {94},
  number = {2},
  pages = {209-226},
  abstract = {Conducted 2 lexical decision experiments to examine
                  semantic priming from attended and unattended
                  parafoveal words. On every trial, a peripheral cue
                  appearing to the left or right of fixation was
                  followed by 2 parafoveal primes, 1 on each visual
                  field, and a subsequent word/nonword target at
                  fixation. In Exp 1, with 30 undergraduates, duration
                  of the cue was manipulated. Parafoveal semantic
                  priming was found but was significant only for the
                  attending primes, showing variation in priming among
                  Ss, with some showing positive priming and others
                  showing negative priming. In Exp 2, with 60
                  undergraduates, different instructions to process
                  the primes were manipulated. Results show that (1)
                  instructions to attend the primes were associated
                  with positive priming; and (2) instructions to
                  ignore the primes were associated with negative
                  priming, even when the ignored primes were
                  cued. Implications of these findings regarding
                  influence of selective attention on semantic
                  processing of parafoveal words are discussed.},
  keywords = {attention allocation, semantic priming of parafoveal
                  words, college students, Spain},
  tags = {psychology;np},
}
@article{Ortells2001,
  author = {Ortells, J.J. and Abad, M.J.F. and Noguera, C. and
                  Lupi{\'a}{\~n}ez, J.},
  year = {2001},
  title = {Influence of prime-probe stimulus onset asynchrony
                  and prime precuing manipulations on semantic priming
                  effects with words in a lexical-decision task},
  journal = {JEPHPP},
  volume = {27},
  number = {1},
  pages = {75-91},
  abstract = {The present research examines semantic priming from
                  attended and unattended parafoveal
                  words. Participants made a lexical decision in
                  response to a single central target. The target was
                  preceded by two parafoveal prime words, with one of
                  them (the attended prime) being precued by a
                  peripheral cue. The main variables manipulated
                  across experiments were cue informativeness (valid
                  vs. neutral cues) and stimulus onset asynchrony
                  (SOA) between prime and probe (200, 300, 600, or
                  1,000 ms). The results showed (a) reliable semantic
                  priming from both attended and ignored prime words
                  and (b) that the ignored priming effects were either
                  negative or positive, depending on both the
                  prime-probe SOA and cue informativeness. The present
                  findings are discussed in relation to inhibitory
                  versus episodic retrieval models of negative
                  priming.},
  keywords = {inhibitory attentional mechanisms or memory
                  retrieval processes, robust effect of semantic
                  priming, 18-30 yr olds Print},
  tags = {psychology;np},
}
@article{Ortells2003,
  author = {Ortells, Juan J. jortells and Fox, Elaine and
                  Noguera, Carmen and Abad, M. J. F.},
  year = {2003},
  title = {Repetition priming effects from attended vs ignored
                  single words in a semantic categorization task},
  journal = {Acta Psychologica},
  volume = {114},
  number = {2},
  pages = {185-210},
  abstract = {The present research examines priming effects from a
                  centrally presented single-prime word to which
                  participants were instructed to either attend or
                  ignore. The prime word was followed by a single
                  central target word to which participants made a
                  semantic categorization (animate vs. inanimate)
                  task. The main variables manipulated across
                  experiments were attentional instructions (attend
                  vs. ignore the prime word), presentation duration of
                  the prime word (20, 50, 80 or 100 ms), prime-target
                  stimulus onset asynchrony (SOA; 300 vs.  800 ms),
                  and temporal presentation of instructions (before
                  vs. after the prime word). The results showed (a) a
                  consistent interaction between attentional
                  instructions and repetition priming and (b) a
                  qualitatively different ignored priming pattern as a
                  function of prime duration: reduced positive priming
                  (relative to the attend instruction) for prime
                  exposures of 80 and 100 ms, and reliable negative
                  priming for the shorter prime exposures of 20 and 50
                  ms. In addition (c), the differential priming
                  pattern for attend and ignore trials was observed at
                  a prime-target SOA of 800 ms (but not at a shorter
                  300-ms SOA) and only when instructions were
                  presented before the prime word.},
  keywords = {negative priming repetition priming selective
                  attention semantic categorization attentional
                  instructions stimulus duration stimulus onset
                  asynchrony temporal presentation Stimulus Duration
                  Stimulus Onset Words (Phonetic Units) Print
                  Electronic},
  tags = {psychology;np},
}
@article{Oseas-Kreger1996,
  author = {Oseas-Kreger, Deborah M.},
  year = {1996},
  title = {Facilitation and inhibition in selective attention:
                  An investigation of their relative influence and
                  time-course for both younger and older adults},
  journal = {Dissertation Abstracts International: Section B: The
                  Sciences \& Engineering},
  volume = {57},
  number = {3},
  pages = {2190},
  abstract = {There is a variety of research evidence suggesting
                  that older adults suffer from a decline in
                  inhibitory functioning, resulting in an imbalance of
                  facilitory and inhibitory processes and a consequent
                  decrement in selective attention abilities for older
                  adults. Two experiments were conducted in order to
                  explore more closely the "balance" of facilitory and
                  inhibitory processes in both younger and older
                  adults and to investigate the relative time courses
                  of each process by using a negative priming
                  methodology. Each experiment utilized both an
                  identity suppression task and a location suppression
                  task in order to expand the generalizability of the
                  findings to various task requirements. Results of
                  Experiment one lend partial support for the
                  inhibitory deficit hypothesis and also suggest that
                  facilitory processes may be of greater influence
                  when performing a location suppression task than
                  when performing an identity suppression task.
                  Results of Experiment two failed to allow direct
                  comparisons to be made between the time courses of
                  negative priming in an identity suppression task for
                  younger and older adults, however, the results of
                  the location suppression task have important
                  implications for future studies of inhibitory
                  processes and the negative priming phenomenon.},
  keywords = {facilitation \& inhibition, selective attention \&
                  relative influence \& time courses of each process,
                  younger \& older adults Inhibition (Personality)},
  tags = {psychology;np},
}
@article{Ossmann2003,
  author = {Ossmann, Janet M. and Mulligan, Neil W.},
  year = {2003},
  title = {Inhibition and attention deficit hyperactivity
                  disorder in adults},
  journal = {American Journal of Psychology},
  volume = {116},
  number = {1},
  pages = {35-50},
  abstract = {The inhibitory account of attention deficit
                  hyperactivity disorder (ADHD) was tested by
                  examining the performance of college-aged adults on
                  a variety of inhibitory tasks, including the stop
                  signal task, the negative priming task, a measure of
                  working memory capacity, and the Test of Variables
                  of Attention. 24 undergraduates with ADHD (mean age
                  19.21 yrs) and 24 undergraduates without ADHD (mean
                  age 19.42 yrs) participated. The poorer performance
                  of adults with ADHD compared with controls on
                  negative priming, stopping, and continuous
                  performance tasks, combined with similar group
                  performances on a test of working memory capacity,
                  indicates a specific inhibitory deficit as opposed
                  to a general limitation in attentional capacity.
                  Overall results provide evidence for extending the
                  inhibitory deficit hypothesis to adult ADHD, not
                  only for mechanisms of response (or motor)
                  inhibition but also for mechanisms of cognitive
                  inhibition.},
  keywords = {inhibition attention deficit hyperactivity disorder
                  stopping negative priming working memory capacity
                  attention response \& motor \& cognitive inhibition
                  Print},
  tags = {psychology;np},
}
