@article{Cameron1996,
  author = {Cameron, Carol-Anne and Hopper, Elizabeth S. and
                  Tiplady, Brian},
  year = {1996},
  title = {Ethanol and negative priming},
  journal = {Human Psychopharmacology: Clinical \& Experimental},
  volume = {11},
  number = {2},
  pages = {131-136},
  abstract = {Explored the effect of ethanol (EtOH) on negative
                  priming (NP) for 18 adults (aged 20-24 yrs) who took
                  part in a 3-period crossover study in which they
                  received placebo and 2 doses of EtOH in random
                  order. Ss performed an NP task in which they
                  detected the larger of 2 circles that appeared in
                  any of 4 positions on a monitor. A prime trial was
                  rapidly followed by a probe trial in which either
                  both circles were in previously unoccupied positions
                  (C), or the nontarget was in a previously unoccupied
                  position and the target in the same location and the
                  same size as the previous non-target (IR). NP (IR-C)
                  was increased by EtOH, the effect being significant
                  for the lower dose. Other performance tasks showed
                  an impairment of divided attention at the higher
                  dose. Subjective drunkenness was significantly
                  increased for both EtOH doses.},
